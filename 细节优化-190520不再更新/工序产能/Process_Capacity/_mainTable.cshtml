                        </div>
                        <el-divider v-if="index<(scope.row.AfterWeld.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
            <el-table-column label="标准总人数" width="85">
                <template slot-scope="scope">
                    <template v-for="(item,index) in scope.row.AfterWeld">
