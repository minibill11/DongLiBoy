
<el-table :data="tableList"
          max-height="600"
          size="small "
          align="center"
          :row-style="{height:'100%'}"
          cell-class-name="cellParent"
          border
          stripe>
    <div slot="empty">暂无数据</div>
    <el-table-column type="index" @*prop="id"*@ label="序号" fixed width="50">
    </el-table-column>
    <el-table-column prop="Type" label="型号" fixed width="100">
        <template slot-scope="scope">
            <div class="equalTd">
                {{scope.row.Type}}
                <br />
                {{scope.row.Platform}}
            </div>
        </template>
    </el-table-column>
    <el-table-column label="SMT">
        <el-table-column prop="PCB" label="产品PCB编号" width="100">
            <template slot-scope="scope">
                <div class="equalTd">
                    {{scope.row.PCB}}
                </div>
            </template>
        </el-table-column>
        <el-table-column label="IC面贴装" class-name="cc">
            <el-table-column label="工序描述" width="100">
                <template slot-scope="scope">
                    <div class="equalTd">
                        <hover-div touchtype="popover" :ispower="true">
                            <popover-upload :rowdata="scope.row"
                                            :itemdata="scope.row.icProductName"
                                            section="SMT"></popover-upload>
                        </hover-div>
                        <jump-link :rowdata="scope.row"
                                   section="SMT"
                                   :img="scope.row.SMTjpg"
                                   :pdf="scope.row.SMTpdf"
                                   :nametext="scope.row.icProductName"
                                   :examinanstatu="scope.row.SMTExaminanMessage"></jump-link>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="线别" width="70">
                <template slot-scope="scope" v-if="scope.row.SMTExaminanMessage=='审核通过'">
                    <div class="equalTd">
                        1~14线
                        <br />
                        最大产能
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        1~14线
                        <br />
                        最小产能
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标配人数" width="70">
                <template slot-scope="scope">
                    <div class="equalTd">
                        {{scope.row.icMaxStandardTotal}}
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        {{scope.row.icMinStandardTotal}}
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标准产能(PCS/H)" width="70">
                <template slot-scope="scope">
                    <div class="equalTd">
                        {{scope.row.icMaxStandardOutput}}
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        {{scope.row.icMinStandardOutput}}
                    </div>
                </template>
            </el-table-column>
        </el-table-column>
        <el-table-column label="灯面贴装">
            <el-table-column prop="LightProductName" label="工序描述" width="100">
                <template slot-scope="scope">
                    <div class="equalTd">
                        <hover-div touchtype="popover" :ispower="true">
                            <popover-upload :rowdata="scope.row"
                                            :itemdata="scope.row.LightProductName"
                                            section="SMT"></popover-upload>
                        </hover-div>
                        <jump-link :rowdata="scope.row"
                                   section="SMT"
                                   :img="scope.row.SMTjpg"
                                   :pdf="scope.row.SMTpdf"
                                   :nametext="scope.row.LightProductName"
                                   :examinanstatu="scope.row.SMTExaminanMessage"></jump-link>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="线别" width="70">
                <template slot-scope="scope" v-if="scope.row.SMTExaminanMessage=='审核通过'">
                    <div class="equalTd">
                        1~14线
                        <br />
                        最大产能
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        1~14线
                        <br />
                        最小产能
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标配人数" width="70">
                <template slot-scope="scope">
                    <div class="equalTd">
                        {{scope.row.LightMaxStandardTotal}}
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        {{scope.row.LightMinStandardTotal}}
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标准产能(PCS/H)" width="70">
                <template slot-scope="scope">
                    <div class="equalTd">
                        {{scope.row.LightMaxStandardOutput}}
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        {{scope.row.LightMinStandardOutput}}
                    </div>
                </template>
            </el-table-column>
        </el-table-column>
        <el-table-column label="插件">
            <el-table-column label="设备名称" width="100">
                <template slot-scope="scope">
                    <template v-if="scope.row.PluginDevice==null">
                        <hover-div touchtype="edit" :ispower="true">
                            <input-edit-btn :rowdata="scope.row"
                                            section="插件"
                                            :isnull="scope.row.PluginDevice"></input-edit-btn>
                        </hover-div>
                    </template>
                    <template v-else v-for="(item,index) in scope.row.PluginDevice">
                        <div class="equalTd">
                            <hover-div touchtype="edit" :ispower="true" :isfix="item.PluginEidt">
                                <input-edit-btn :rowdata="scope.row"
                                                :isedit="item.PluginEidt"
                                                :index="index"
                                                :itemdata="item"
                                                :itemid="item.PluginDeviceID"
                                                section="插件"
                                                :nametext="item.PluginDeviceName"
                                                :isnull="scope.row.PluginDevice"></input-edit-btn>
                            </hover-div>
                            <input-edit :isedit="item.PluginEidt"
                                        v-on:input="item.PluginDeviceName = $event"
                                        :nametext="item.PluginDeviceName"></input-edit>
                        </div>
                        <el-divider v-if="index<(scope.row.PluginDevice.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
            <el-table-column label="机台固定标准单灯工时(秒/颗)" width="110">
                <template slot-scope="scope">
                    <template v-for="(item,index) in scope.row.PluginDevice">
                        <div class="equalTd">
                            <div v-if="item.PluginDeviceName=='/'" class="red">/</div>
                            <div v-else>{{item.SingleLampWorkingHours}}</div>
                        </div>
                        <el-divider v-if="index<(scope.row.PluginDevice.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
            <el-table-column label="PCBA插件单灯数" width="70">
                <template slot-scope="scope">
                    <template v-for="(item,index) in scope.row.PluginDevice">
                        <div class="equalTd">
                            <div v-if="item.PluginDeviceName=='/'" class="red">/</div>
                            <div v-else>
                                <input-edit :isedit="item.PluginEidt"
                                            v-on:input="item.PCBASingleLampNumber = $event"
                                            :nametext="item.PCBASingleLampNumber"></input-edit>
                            </div>
                        </div>
                        <el-divider v-if="index<(scope.row.PluginDevice.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
            <el-table-column label="标配人数" width="70">
                <template slot-scope="scope">
                    <template v-for="(item,index) in scope.row.PluginDevice">
                        <div class="equalTd">
                            <div v-if="item.PluginDeviceName=='/'" class="red">/</div>
                            <div v-else>
                                <input-edit :isedit="item.PluginEidt"
                                            v-on:input="item.PluginStandardNumber = $event"
                                            :nametext="item.PluginStandardNumber"></input-edit>
                            </div>
                        </div>
                        <el-divider v-if="index<(scope.row.PluginDevice.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
            <el-table-column prop="PluginStandardCapacity" label="标准产能(PCS/H)" width="70">
                <template slot-scope="scope">
                    <template v-for="(item,index) in scope.row.PluginDevice">
                        <div class="equalTd">
                            <div v-if="item.PluginDeviceName=='/'" class="red">/</div>
                            <div v-else>{{item.PluginStandardCapacity}}</div>
                            @*{{3600/(item.PCBASingleLampNumber*item.SingleLampWorkingHours)}}*@
                        </div>
                        <el-divider v-if="index<(scope.row.PluginDevice.length-1)"></el-divider>
                    </template>
                </template>
            </el-table-column>
        </el-table-column>
        <el-table-column label="后焊">
            <el-table-column label="工序描述" width="100">
                <template slot-scope="scope">
                    <hover-div touchtype="popover" :ispower="true">
                        <popover-upload :rowdata="scope.row"
