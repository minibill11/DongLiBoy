<!--- 二级按钮导航 --->
<template>
  <div class="sa-header">
    <div class="sa-title">{{active}}</div>
    <div class="sa-header-btn">
      <el-button
        v-for="(item,index) in header_list"
        :key="index"
        type="primary"
        :plain="active==item.title?false:true"
        @click="skip(item)"
        style="border-bottom: none;font-size:14px;"
      >{{item.title}}</el-button>
    </div>
    <hr class="style-one" />
  </div>
</template>

<script>
export default {
  name: "secondNav",
  props: ["active", "header_list"],
  data() {
    return {};
  },
  components: {},
  computed: {},
  watch: {},
  methods: {
    skip(val) {
      this.header_list.forEach((item) => {
        if (val.title == item.title) {
          this.$router.push({
            name: val.name,
          });
        }
      });
    },
  },
  created() {},
  mounted() {},
  beforeCreate() {},
  beforeMount() {},
  beforeUpdate() {},
  updated() {},
  beforeDestroy() {},
  destroyed() {},
  activated() {},
};
</script>

