        float: left;
    }

    .Dthree input {
        width: 33.3%;
        float: left;
    }

    label {
        font-weight: normal;
    }

    #P0, #R0, #AM0 {
        padding: 0;
    }

    #RoHS_REACH, #SamplingPlan, #NGD, #NGHandle, #Creator, #CreatedDate, #AssessedDate ,#ApprovePass,#AssessorPass ,#Assessor{
        display: none;
    }

    textarea,textarea[class=form-control]{
        border:none;
        width:100%;
        max-width:initial;
    }
    .saveShow{
        display:none
    }
