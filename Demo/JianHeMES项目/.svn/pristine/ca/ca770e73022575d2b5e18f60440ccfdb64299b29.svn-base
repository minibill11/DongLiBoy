
@{
    /**/

    ViewBag.Title = "b_a_list";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Bootstraps/Element-ui.js"></script>
<script src="~/Scripts/axios.min.js"></script>
<link href="~/Scripts/Bootstraps/Element-ui.css" rel="stylesheet" />

<style>
    .top-container {
        display: flex;
        justify-content: space-between;
        margin-top: 25px;
    }

    .app-container {
        margin-top: 15px;
    }

    .filter-tree {
        margin-top: 10px;
        width: 100%;
    }

    .bottom-container {
        height: 650px;
        overflow-y: auto;
        width: 240px;
    }

    #hello {
        overflow-y: auto;
        position: relative;
        display: block;
        width: 900px;
        height: 650px;
        text-align: left;
    }

        #hello > img {
            display: block;
            position: relative;
            left: 0;
        }

    .gottable {
        width: 74%;
        /*border:1px solid green;*/
    }

        .gottable > table {
            text-align: center;
        }

    .button-container {
        width: 50px;
        display: flex;
        justify-content: flex-start;
    }

        .button-container > button {
            padding: 6px;
            margin: 0;
        }

    .formedia {
        display: flex;
        justify-content: space-between;
    }

    ul, li {
        width: 200px;
    }

    @@media (max-width:450px) {
        .formedia {
            display: block;
        }

        .bottom-container {
            width: 100%;
            height: auto;
        }

        .gottable {
            width: 837px;
        }
    }
</style>
