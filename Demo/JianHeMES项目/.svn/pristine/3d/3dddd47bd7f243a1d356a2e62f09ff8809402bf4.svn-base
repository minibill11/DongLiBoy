let TeamEvaluation = {
    data: function () {
        return {
            loading: false,//loading状态开关
            Ordernum: {
                value: '',
                list: [],
            },
            Department: {
                value: '',
                list: [],
            },
            Group: {
                value: '',
                list: [],
            },
            ExecuteTime: {
                value: '',
                list: [],
            },
        }
    },
    created: function () {
        for (let i = 1; i <= 5; i++) {
            this.Department.list.push({
                label: `部门${i}`,
                value: `部门${i}`
            });
            this.Group.list.push({
                label: `班组${i}`,
                value: `班组${i}`
            });
            this.Ordernum.list.push({
                label: `订单${i}`,
                value: `订单${i}`
            });
            this.ExecuteTime.list.push({
                label: `执行时间${i}`,
                value: `执行时间${i}`
            });
        };
        //this.GetOrdernum();
        //this.GetDeprment();
        //this.GetGroup();
        //this.GetExTime();
    },
    methods: {
        GetExTime() {
            axios.post('/KPI/GetExTime').then(res => {
                this.ExecuteTime.list = res.data;
            }).catch(err => {
                console.warn(err);
            });
        },
        GetDeprment() {
            axios.post('/KPI/GetDeprment').then(res => {
                this.Department.list = res.data;
            }).catch(err => {
                console.warn(err);
            });
        },
        GetGroup() {
            axios.post('/KPI/GetGroup').then(res => {
                this.Group.list = res.data;
            }).catch(err => {
                console.warn(err);
            });
        },
        GetOrdernum() {
            axios.post('/Packagings/GetOrderList').then(res => {
                console.table(res.data)
                this.Ordernum.list = res.data;
            }).catch(err => {
                console.warn(err);
            });
        },
    },
};