/*全局样式*/
*, body {
    margin: 0;
    padding: 0;
}

#app {
    padding: 50px 0 0;
}

.el-input {
    max-width: 280px;
}

[v-cloak] {
    display: none;
}

.textCenter {
    text-align: center;
}

/*内箱条码样式*/
#innerBarcode, .innerBarDiv {
    width: 100%;
}

.mozhu {
    font-size: 8em;
    line-height: 1em;
}

#innerCarton {
    margin-top: 1em;
}




/*外箱条码样式*/
#masterCarton {
    margin: 1em 0 0.5em;
    padding: 1em;
}

#masterBarcode {
    width: 80%;
}

.mcFrame {
    margin: 3%;
    width: 94%;
    height: 94%;
    border: 1px solid #000;
}

.mcFrame_top, .mcFrame_bottom {
    height: 50%;
}

    .mcFrame_top > div {
        height: 33.3%;
    }


.mcFrame_top_1 {
    position: relative;
}

.imgicon {
    position: absolute;
    width: 30%;
    top: 5px;
    left: 5px;
}

.mcFrame_top_1_h {
    margin: 0;
    padding-top: 1em;
    text-align: center;
    font-size: 3em;
}

.mcFrame_top_1_h_noimg {
    margin: 0;
    padding-top: 0.6em;
    text-align: center;
    font-size: 3.2em;
}

.mcFrame_top_3_1_1, .mcFrame_top_3_1_2 {
    display: inline-block;
    line-height: 2.5em;
    text-align: center;
}

.mcFrame_top_3_1_1 {
    width: 24%;
    font-size: 1.5em;
}

    .mcFrame_top_3_1_1 > span {
        font-weight: 600;
    }

.mcFrame_top_3_1_2 {
    width: 74%;
    font-size: 2em;
    line-height: 1.5em
}

.mcFrame_top_3_1_1:not(:last-child) {
    border-right: 1px solid #000;
}

.borderTop {
    border-top: 1px solid #000;
}

.mcFrame_bottom_1 {
    margin: 3% 0;
    text-align: left;
}

.mcFrame_top_2 {
    position: relative;
}

.masterBarDiv {
    width: 100%;
    position: absolute;
    top: 1em;
}
.boxspan {
    display: inline-block;
    padding: 0.1em 0.3em;
}

.font20md {
    font-size: 2.2em;
}

.font20xs {
    font-size: 2.8em;
    padding: 0em 0.3em;
}