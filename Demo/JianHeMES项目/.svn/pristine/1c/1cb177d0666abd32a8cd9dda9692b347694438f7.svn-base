@{
    ViewBag.Title = "MES操作手册";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<style>
    * {
        margin: 0;
        padding: 0;
    }

    [v-cloak] {
        display: none;
    }

    .el-header {
        height: auto !important;
    }

    .el-main {
        min-height: 600px;
        padding: 5px 0 0;
    }

    .el-tabs__item {
        height: 30px;
        line-height: 30px;
    }

    .el-tabs__nav-next, .el-tabs__nav-prev {
        line-height: 30px;
    }

    @@media screen and (max-width:768px) {
        .PC {
            display: none;
        }

        .el-tabs__item {
            height: 20px;
            line-height: 20px;
            padding: 0 5px;
            font-size: 11px;
        }

        .el-tabs--top .el-tabs--left .el-tabs__item:last-child, .el-tabs--top.el-tabs--border-card .el-tabs__item:last-child {
            padding-right: 5px;
        }

        .container, .body-content {
            padding: 0;
        }

        h3 {
            margin: 10px 0 5px;
        }

        .el-tabs__nav-next, .el-tabs__nav-prev {
            line-height: 20px;
        }
    }

    @@media screen and (min-width:768px) {
        .PDA {
            display: none;
        }
    }

    .el-tabs--left, .el-tabs--right, .el-tabs__header is-left, .el-tabs__content {
        overflow: auto;
    }

    .el-tabs__content {
        height: 100%;
        overflow: auto;
    }

    .el-tabs--border-card > .el-tabs__content {
        padding: 0px;
    }
    /*   侧边栏高度，每增加一个内容＋30px   */
    .innertabs {
        max-height: 720px;
    }

    .el-tab-pane {
        text-align: center;
        overflow:auto;
    }

    .el-tabs__header {
        margin: 0 0 10px;
    }
    /*图片占满整个框*/
    /*img{
        width:100%;
    }*/
</style>

