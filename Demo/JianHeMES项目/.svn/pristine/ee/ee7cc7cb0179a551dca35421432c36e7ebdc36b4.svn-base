@{
    ViewBag.Title = "生产管控历史信息";
}
<link href="~/Scripts/Bootstraps/bootstrap-select.css" rel="stylesheet" />
<script src="~/Scripts/Bootstraps/bootstrap-select.js"></script>
<script src="~/Scripts/imgJS/jquery.artZoom.js"></script>
<link href="~/Content/styleFile/jquery.artZoom.css" rel="stylesheet" />
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<style>
    * {
        margin: 0;
        padding: 0;
    }

    .body-content, .container {
        padding-left: 0px;
        padding-right: 0px;
    }

    .table > thead > tr > th, .table > tbody > tr > td {
        text-align: center;
        vertical-align: middle;
        /*word-break:break-all;*/
        padding: 2px 1px;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(-n+5), .table > thead:first-child > tr:first-child > th:nth-child(-n+5) {
        border-top: 1px solid red;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(n+6), .table > thead:first-child > tr:first-child > th:nth-child(n+6) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(-n+5), .rowTitle2 > th:nth-child(-n+3), .table > tbody > tr > td:nth-child(-n+6) {
        border: 1px solid red;
    }

    .rowTitle1 > th:nth-child(n+6), .rowTitle2 > th:nth-child(n+3), .table > tbody > tr > td:nth-child(n+7) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(5), .rowTitle2 > th:nth-child(2), .table > tbody > tr > td:nth-child(6) {
        border-right: 1px solid blue;
    }

    .table > tbody > tr > td:nth-child(n+9), .table > tbody > tr > td:nth-child(2) {
        cursor: pointer;
    }

    .table > tbody > tr > td:nth-child(n+27),
    .table > tbody > tr > td:nth-child(11),
    .table > tbody > tr > td:nth-child(12) {
        cursor: auto;
    }

    .table > tbody > tr:nth-child(n+1):hover {
        background-color: rgba(77, 72, 72, 0.15);
    }



    .scroll_table_content {
        width: 100%;
        height: 600px;
        overflow: auto;
    }

    #app {
        font-size: 12px;
    }

    .bgcolor100 {
        background-color: rgba(62, 215, 0, 0.4);
    }

    .bgcolor90 {
        background-color: rgba(233, 230, 0, 0.6);
    }

    .bgcolor80 {
        background-color: rgba(233, 32, 32, 0.8);
        color: white;
    }

    .modal-body, .modal-header {
        padding: 8px 10px 2px;
    }

    .showBtn {
        position: relative;
        cursor: pointer;
        padding: 0 !important;
        font-size: 11px;
    }

    .showDiv {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0px;
        right: 0px;
        visibility: hidden;
        opacity: 0;
        transition: opacity 1s;
        transition: opacity 0s;
    }

    .btnSty {
        position: absolute;
        height: 100%;
        padding: 0;
        z-index: 10;
        font-size: 12px;
    }

    .showBtn:hover .showDiv {
        visibility: visible;
        opacity: 1;
    }

    .imgModalc {
        margin-bottom: 5px;
    }

    @@media screen and (min-width: 1200px) {
        .container {
            width: 1855px;
        }

        .titleul {
            width: 1155px;
        }
    }

    #content {
        width: 1872px;
    }

    .table {
        width: 1852px;
    }

        .table > tbody > tr:first-child > td {
            border-top: none;
        }

    .selectpicker {
        border: 1px solid #ccc;
    }

    [v-cloak] {
        display: none;
    }

    .smtdiv {
        width: 100%;
        height: 24px;
        position: relative;
        font-size: 11px;
        line-height: 1.1;
    }

        .smtdiv > span {
            position: absolute;
            font-size: 10px
        }

            .smtdiv > span:first-child {
                left: 5px;
                top: 7px;
            }

            .smtdiv > span:last-child {
                right: 5px;
                top: 1px;
            }
</style>
<div id="content">
    <h3 class="text-center">生产管控历史记录查询</h3>
    <br />
    <form action="/ProductionControl/ProductionControlHistory" class="text-center" method="post">
        @*<span>平台：<input name="PlatformType" type="text"></span>*@
        <span>订单号：</span>@Html.DropDownList("orderNum", (List<SelectListItem>)ViewBag.OrderNumList, new { @class = "selectpicker", title = "全部订单", data_live_search = "true", data_style = "form-control", data_selected_text_format = "count", data_actions_box = "true", multiple = true })
        @*<span>订单号内容：</span>
            <input type="text" class="form-control" style="display:inline;width:150px;" name="orderNumContent" />*@
        <span>  平台：@Html.DropDownList("PlatformType", (List<SelectListItem>)ViewBag.PlatformType, new { @class = "selectpicker", title = "全部平台", data_live_search = "true", data_style = "form-control", data_selected_text_format = "count", data_actions_box = "true", multiple = true })</span>
        <input id="queryBtn" class="btn btn-default" value="查找" type="submit">
    </form>
    <br />
