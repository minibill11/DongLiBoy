                            {{item.AssembleAbnormal_Description}}
                            <div v-if="(item.AssembleAbnormal_Description)&&(item.AssembleAbnormal_DescriptionJpg=='true')&&(item.AssembleAbnormal_DescriptionPdf=='true')" class="showDiv">
                                <button v-on:click.stop="imgModal_AssembleAbnormal(item.OrderNum)" class="btn btn-info btnSty" style="width: 50%;right: 50%;">IMG</button>
                                <button v-on:click.stop="pdfModal_AssembleAbnormal(item.OrderNum)" class="btn btn-warning btnSty" style="width: 50%;left: 50%;">PDF</button>
                            </div>
                            <div v-else-if="(item.AssembleAbnormal_Description)&&(item.AssembleAbnormal_DescriptionJpg=='true')&&(item.AssembleAbnormal_DescriptionPdf=='false')" class="showDiv">
                                <button v-on:click.stop="imgModal_AssembleAbnormal(item.OrderNum)" class="btn btn-info btnSty" style="width: 100%;right: 0;">IMG</button>
                            </div>
                            <div v-else-if="(item.AssembleAbnormal_Description)&&(item.AssembleAbnormal_DescriptionJpg=='false')&&(item.AssembleAbnormal_DescriptionPdf=='true')" class="showDiv">
                                <button v-on:click.stop="pdfModal_AssembleAbnormal(item.OrderNum)" class="btn btn-warning btnSty" style="width: 100%;left: 0;">PDF</button>
