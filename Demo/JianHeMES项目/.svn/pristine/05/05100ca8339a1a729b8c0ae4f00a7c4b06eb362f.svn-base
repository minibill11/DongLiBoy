@{
    ViewBag.Title = "MES操作手册";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Bootstraps/Element-ui.js"></script>
<link href="~/Scripts/Bootstraps/Element-ui.css" rel="stylesheet" />
<style>
    * {
        margin: 0;
        padding: 0;
    }

    [v-cloak] {
        display: none;
    }

    .el-header {
        height: auto !important;
    }

    .el-main {
        min-height: 600px;
        padding: 5px 0 0;
    }

    .el-tabs__item {
        height: 30px;
        line-height: 30px;
    }

    @@media screen and (max-width:728px){
        .el-tabs__item {
            height: 20px;
            line-height: 20px;
            padding: 0 5px;
            font-size:11px;
        }
        .el-tabs--top .el-tabs--left .el-tabs__item:last-child, .el-tabs--top.el-tabs--border-card .el-tabs__item:last-child {
            padding-right: 5px;
        }
        .container, .body-content {
            padding:0;
        }
        h3{
            margin:10px 0 5px;
        }
    }
    .el-tabs--left, .el-tabs--right, .el-tabs__header is-left, .el-tabs__content {
        overflow: auto;
    }

    .el-tabs__content {
        height: 100%;
        overflow:auto;
    }
    .el-tabs--border-card > .el-tabs__content {
        padding: 0px;
    }
    /*   侧边栏高度，每增加一个内容＋30px   */
    .innertabs {
        height: 660px;
    }

    /*图片占满整个框*/
    /*img{
        width:100%;
    }*/
</style>

<div id="app" v-cloak>
    <el-container>
        <el-header class="text-center">
            <h3>@ViewBag.Title</h3>
        </el-header>
        <el-main v-loading="loading">
            <el-tabs type="border-card">
                @*PC部*@
                <el-tab-pane label="PC部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="创建订单">
                            <img class="createOrdenum" src="~/Images/menu2/PC/createOrder.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="查看订单">
                            <img class="selectOrdenum" src="~/Images/menu2/PC/selectOrder.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改订单信息">
                            <img class="updateOrder" src="~/Images/menu2/PC/updateOrder.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="上传首样订单">
                            <img class="shoujian" src="~/Images/menu2/PC/shoujian.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="转特采或异常订单">
                            <img class="specialORabnormal" src="~/Images/menu2/PC/tecai.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="创建条码">
                            <img class="creteBarcode" src="~/Images/menu2/PC/creteBarcode.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="查看条码">
                            <img class="selectBarcode" src="~/Images/menu2/PC/selectBarcode.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="查看挪用条码">
                            <img class="selectNuoBarcode" src="~/Images/menu2/PC/selectNuoBarcode.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT计划创建">
                            <img class="createSMTplan" src="~/Images/menu2/PC/createSMTplan.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="查看SMT计划">
                            <img class="selectSMTplan" src="~/Images/menu2/PC/selectSMTplan.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*MC部*@
                <el-tab-pane label="MC部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="外箱入库">
                            <img class="createOrdenum" src="~/Images/menu2/MC/packangCheck.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改库位号">
                            <img class="selectOrdenum" src="~/Images/menu2/MC/updateWarehouseNum.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外箱出库">
                            <img class="updateOrder" src="~/Images/menu2/MC/warehouseOutCheck.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*SMT部*@
                <el-tab-pane label="SMT部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="SMT产线录入">
                            <img class="createSMTLine" src="~/Images/menu2/SMT/createSMTLine.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改SMT产线信息">
                            <img class="updateSMTlin" src="~/Images/menu2/SMT/updateSMTlin.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT产线历史信息">
                            <img class="smtLineHistory" src="~/Images/menu2/SMT/smtLineHistory.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT看板管理">
                            <img class="smtBoard" src="~/Images/menu2/SMT/smtBoard.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT看板历史记录">
                            <img class="smtBoardHistory" src="~/Images/menu2/SMT/smtBoardHistory.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT生产信息">
                            <img class="smtProduction" src="~/Images/menu2/SMT/smtProduction.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT生产历史记录">
                            <img class="smtProducitonHistory" src="~/Images/menu2/SMT/smtProducitonHistory.jpg" />
                        </el-tab-pane>

                    </el-tabs>
                </el-tab-pane>
                @*装配一/二部*@
                <el-tab-pane label="装配一/二部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="开始PQC">
                            <img class="createAssbem" src="~/Images/menu2/Assemb/createAssemb.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="查看PQC">
                            <img class="selectAssemb" src="~/Images/menu2/Assemb/selectAssemb.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="老化拼屏">
                            <img class="mosaicScreen" src="~/Images/menu2/Assemb/mosaicScreen.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="校正检查">
                            <img class="calibration" src="~/Images/menu2/Assemb/calibration.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="球场屏扫码">
                            <img class="courtScreen" src="~/Images/menu2/Assemb/courtScreen.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="装箱信息录入">
                            <img class="createPackingInfo" src="~/Images/menu2/Assemb/createPackingInfo.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="内箱确认">
                            <img class="intoPackingCheck" src="~/Images/menu2/Assemb/intoPackingCheck.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*品质部*@
                <el-tab-pane label="品质部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="FQC检查">
                            <img class="FQCcheck" src="~/Images/menu2/Quality/FQC.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="老化检查">
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC1.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC2.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC3.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC4.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC5.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC6.jpg" />
                            <img class="OQCcheck" src="~/Images/menu2/Quality/OQC7.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外观包装">
                            <img class="Appearncheck" src="~/Images/menu2/Quality/Appearm.jpg" />
                            <img class="Appearncheck" src="~/Images/menu2/Quality/appearm2.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="IQC">
                            <img class="IQC" src="~/Images/menu2/Quality/iqc.jpg" />
                            <img class="IQC" src="~/Images/menu2/Quality/iqc2.jpg" />
                            <img class="IQC" src="~/Images/menu2/Quality/iqc3.jpg" />
                            <img class="IQC" src="~/Images/menu2/Quality/iqc4.jpg" />
                            <img class="IQC" src="~/Images/menu2/Quality/iqc5.jpg" />
                            <img class="IQC" src="~/Images/menu2/Quality/iqc6.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外箱标签打印">
                            <img class="FQCcheck" src="~/Images/menu2/Quality/packing.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外箱重新打印">
                            <img class="FQCcheck" src="~/Images/menu2/Quality/replayPacking.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外箱标签删除">
                            <img class="FQCcheck" src="~/Images/menu2/Quality/packingDelete.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*人力资源部*@
                <el-tab-pane label="人力资源部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="添加人事日报">
                            <img class="addDaliy" src="~/Images/menu2/person/addDaliy.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改人事日报">
                            <img class="updateDaliy" src="~/Images/menu2/person/updateDaliy.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="下载人事日报">
                            <img class="downloadDaliy" src="~/Images/menu2/person/downloadDaliy.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="增加人工成本周对比">
                            <img class="addContrast" src="~/Images/menu2/person/addContrast.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="删除人工成本周对比">
                            <img class="deleteContrast" src="~/Images/menu2/person/deleteContrast.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="月流失率">
                            <img class="turnoverrate" src="~/Images/menu2/person/Turnoverrate.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="添加招聘周报">
                            <img class="addRecruitment" src="~/Images/menu2/person/addRecruitment.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改招聘周报">
                            <img class="updateRecruitment" src="~/Images/menu2/person/updateRecruitment.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="离职员工工龄分布">
                            <img class="leavUser" src="~/Images/menu2/person/reasonForLeaving.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="组织架构">
                            <img class="freamwork" src="~/Images/menu2/person/Framework.jpg" />
                            <img class="freamwork2" src="~/Images/menu2/person/Framework2.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="离职原因调查">
                            <img class="leavereson" src="~/Images/menu2/person/resultForLeaving.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="请假记录">
                            <img class="leaveInfo" src="~/Images/menu2/person/leaving.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
            </el-tabs>
        </el-main>
    </el-container>
</div>
<script>
    var app = new Vue({
        el: "#app",
        data: {
            loading: false,
        },
        mounted: function () {

        }
    });
</script>