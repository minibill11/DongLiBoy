/*全局样式*/
*, body {
    margin: 0;
    padding: 0;
}

#app {
    padding: 50px 0 0;
}

.body-content, .container {
    padding-left: 0px;
    padding-right: 0px;
}

.el-header {
    height: auto !important;
    padding: 0;
}

.el-main {
    padding: 10px 0;
}

[v-cloak] {
    display: none;
}

@media screen and (max-width: 768px) {
    .el-message, .el-notification, .el-message-box {
        min-width: 90%;
        max-width: 90%;
    }
}

.el-input {
    width: 220px;
}

.inputtext {
    font-weight: 600;
    font-size: 15px;
}

.inputframe {
    width: 320px;
    text-align: right;
    margin: 1px auto;
}

.el-date-picker__editor-wrap > .el-input {
    width: auto;
}

.divframe {
    width: 300px;
    margin: 1px auto;
    text-align: center;
    display: inline-block;
    border: 1px solid #ccc;
}

.el-table th {
    padding: 5px 0;
    background-color: #f4df42;
    color: #000;
    text-align: center;
}

.el-table td {
    padding: 2px 0;
    text-align: center;
}

.el-table .cell {
    padding: 0
}

.jiaobanDiv {
    margin: 5px 0;
}


.el-table .warning-row {
    background: #fcd2d2;
}

.el-table .success-row {
    background: #f0f9eb;
}

.time {
    margin: 5px 20px;
    font-size: 16px;
    height: 32px;
    line-height: 32px;
    color: #999;
}
