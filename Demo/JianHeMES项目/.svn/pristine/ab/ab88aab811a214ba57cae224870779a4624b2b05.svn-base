@model IEnumerable<JianHeMES.Models.Packing_BasicInfo>
@{
    ViewBag.Title = "包装箱界面设计";
}
<script src="~/Scripts/Bootstraps/Element-ui.js"></script>
<link href="~/Scripts/Bootstraps/Element-ui.css" rel="stylesheet" />
<script src="~/Scripts/axios.min.js"></script>
<link href="~/Content/styleFile/packingStyle.css" rel="stylesheet" />

<div id="app" v-cloak>
    <h3 class="text-center">包装箱操作PDA界面设计</h3>
    <el-container>
        <el-main v-loading="loading">
            <el-tabs @*type="border-card"*@ @@tab-click="handleClick" stretch="true" tab-position="left">
                @* 录入包装基本信息 *@
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-edit"></i> 录入包装基本信息</span>
                    @ViewBag.TypeList
                    <el-card class="cardbody" shadow="always">
                        <div slot="header" class="enterMes_header text-center">
                            <span>录入包装箱基本信息</span>
                            @*<el-button style="float: right; padding: 3px 0" type="text">操作按钮</el-button>*@
                        </div>
                        <div style="margin:0 0 15px">
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;订单号：<el-select size="mini" clearable placeholder="请选择订单号">
                                <el-option v-for="item in selectoptions"
                                           :key="item.value"
                                           :label="item.label"
                                           :value="item.value">
                                </el-option>
                            </el-select>
                        </div>

                        <div v-for="o in 4" :key="o">
                            <enter-row></enter-row>
                        </div>

                        <el-button size="mini">主要按钮</el-button>
                        <el-button size="mini">默认按钮</el-button>
                    </el-card>
                </el-tab-pane>

                <el-tab-pane>
                    <span slot="label"><i class="el-icon-printer"></i> 内箱标签打印</span>
                    @ViewBag.OrderList
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-check"></i> 内箱装箱确认</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-printer"></i> 外箱(重新)装箱和标签打印</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-check"></i> 外箱装箱确认</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-check"></i> 外箱入库确认</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-edit-outline"></i> 外箱库位号修改</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-view"></i> 产值看板</span>
                </el-tab-pane>
                <el-tab-pane>
                    <span slot="label"><i class="el-icon-check"></i> 外箱出库确认</span>
                </el-tab-pane>
            </el-tabs>
        </el-main>
    </el-container>
</div>


<script>
    Vue.component('enter-row', {
        data: function () {
            return {
                selectoptions: [{
                    value: '选项1',
                    label: '黄金糕'
                }, {
                    value: '选项2',
                    label: '双皮奶'
                }, {
                    value: '选项3',
                    label: '蚵仔煎'
                }, {
                    value: '选项4',
                    label: '龙须面'
                }, {
                    value: '选项5',
                    label: '北京烤鸭'
                }],
                num1: 0,
                num2: 0,
                typeSelectVal: ""
            }

        },
        template: `<div class="mesDiv">
        包装箱类型：<el-select v-model="typeSelectVal" clearable size="mini" placeholder="请选择类型">
                                    <el-option v-for="item in selectoptions"
                                               :key="item.value"
                                               :label="item.label"
                                               :value="item.value">
                                    </el-option>
                                </el-select>
        <br />&nbsp;&nbsp;&nbsp;每箱件数：<el-input-number size="mini" :min="0" v-model="num1"></el-input-number>
        <br />包装箱数量：<el-input-number size="mini" :min="0" v-model="num2"></el-input-number>
        </div >`
    });
    var app = new Vue({
        el: "#app",
        data: {
            loading: false,
            dialogVisible: false,
            currentDate: new Date(),
            selectoptions: [{
                value: '选项1',
            }, {
                value: '选项2',
            }, {
                value: '选项3',
                label: '蚵仔煎'
            }, {
                value: '选项4',
                label: '龙须面'
            }, {
                value: '选项5',
                label: '北京烤鸭'
            }],
        },
        methods: {
            handleClick(tab, event) {
                console.log(tab, event);
            },
            handleClose(done) {
                this.$confirm('确认关闭？')
                    .then(_ => {
                        done();
                    })
                    .catch(_ => { });
            }
        },
        created: function () {
            console.log("@ViewBag.OrderList")
            console.log("@ViewBag.TypeList")
        }
    });
</script>