@{
    ViewBag.Title = "MES操作手册";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Bootstraps/Element-ui.js"></script>
<link href="~/Scripts/Bootstraps/Element-ui.css" rel="stylesheet" />
<style>
    * {
        margin: 0;
        padding: 0;
    }

    [v-cloak] {
        display: none;
    }

    .el-header {
        height: auto !important;
    }

    .el-main {
        min-height: 600px;
        padding: 5px 0 0;
    }

    .el-tabs__item {
        height: 30px;
        line-height: 30px;
    }

    .el-tabs--left, .el-tabs--right, .el-tabs__header is-left, .el-tabs__content {
        overflow: auto;
    }

    .el-tabs__content {
        height: 100%
    }
    /*   侧边栏高度，每增加一个内容＋30px   */
    .innertabs {
        height: 660px;
    }

    /*图片占满整个框*/
    /*img{
        width:100%;
    }*/
</style>

<div id="app" v-cloak>
    <el-container>
        <el-header class="text-center">
            <h3>@ViewBag.Title</h3>
        </el-header>
        <el-main v-loading="loading">
            <el-tabs type="border-card">
                @*PC部*@
                <el-tab-pane label="PC部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="创建订单">
                            <img class="createOrdenum" src="/Images/manuals/createOrder1.jpg" />
                            <img class="createOrdenum" src="/Images/manuals/createOrder2.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改订单小样状态">
                            <img class="updateOrdenumStatu" src="/Images/manuals/updateOrdenumStatu.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="上传小样或首样订单">
                            <img class="samllORfirst" src="/Images/manuals/samllORfirst1.jpg" />
                            <img class="samllORfirst" src="/Images/manuals/samllORfirst2.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="转特采或异常订单">
                            <img class="specialORabnormal" src="/Images/manuals/specialORabnormal.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="创建条码">
                            <img class="creteBarcode" src="/Images/manuals/creteBarcode.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT计划创建">
                            <img class="createSMTplan" src="/Images/manuals/createSMTplan1.jpg" />
                            <img class="createSMTplan" src="/Images/manuals/createSMTplan2.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*SMT部*@
                <el-tab-pane label="SMT部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="SMT计划信息操作">
                            <img class="updateSMTplan" src="/Images/manuals/updateSMTplan.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="SMT产线录入">
                            <img class="createSMTlin" src="/Images/manuals/createSMTlin.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="修改SMT产线录入信息">
                            <img class="updateSMTlin" src="/Images/manuals/updateSMTlin.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*装配一/二部*@
                <el-tab-pane label="装配一/二部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="PQC检查">
                            <img class="PQCcheck" src="/Images/manuals/PQCcheck.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="老化拼屏">
                            <img class="mosaicScreen" src="/Images/manuals/mosaicScreen.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="校正检查">
                            <img class="calibration" src="/Images/manuals/calibration.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="球场屏扫码">
                            <img class="courtScreen" src="/Images/manuals/courtScreen.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*品质部*@
                <el-tab-pane label="品质部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="FQC检查">
                            <img class="FQCcheck" src="/Images/manuals/FQCcheck.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="老化检查">
                            <img class="OQCcheck" src="/Images/manuals/OQCcheck1.jpg" />
                            <img class="OQCcheck" src="/Images/manuals/OQCcheck2.jpg" />
                            <img class="OQCcheck" src="/Images/manuals/OQCcheck3.jpg" />
                            <img class="OQCcheck" src="/Images/manuals/OQCcheck4.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="外观包装">
                            <img class="Appearncheck" src="/Images/manuals/Appearncheck1.jpg" />
                            <img class="Appearncheck" src="/Images/manuals/Appearncheck2.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
                @*人力资源部*@
                <el-tab-pane label="人力资源部">
                    <el-tabs tab-position="left" class="innertabs">
                        <el-tab-pane label="人事日报">
                            <img class="daily" src="/Images/manuals/daily.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="人工成本周对比">
                            <img class="contrast" src="/Images/manuals/contrast.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="月流失率">
                            <img class="turnoverrate" src="/Images/manuals/turnoverrate.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="招聘周报">
                            <img class="recruit" src="/Images/manuals/recruit.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="离职员工分布">
                            <img class="leavUser" src="/Images/manuals/leavUser.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="花名册">
                            <img class="restore" src="/Images/manuals/restore.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="组织架构">
                            <img class="freamwork" src="/Images/manuals/freamwork.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="离职原因调查">
                            <img class="leavereson" src="/Images/manuals/leavereson.jpg" />
                        </el-tab-pane>
                        <el-tab-pane label="请假记录">
                            <img class="leaveInfo" src="/Images/manuals/leaveInfo.jpg" />
                        </el-tab-pane>
                    </el-tabs>
                </el-tab-pane>
            </el-tabs>
        </el-main>
    </el-container>
</div>
<script>
    var app = new Vue({
        el: "#app",
        data: {
            loading: false,
        },
        mounted: function () {

        }
    });
</script>