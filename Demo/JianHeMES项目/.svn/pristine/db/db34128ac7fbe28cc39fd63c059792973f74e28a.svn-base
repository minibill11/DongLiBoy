using System;
using System.Collections.Generic;
using System.ComponentModel.DataAnnotations;
using System.Linq;
using System.Web;

namespace JianHeMES.Models
{
    //小样报告
    public class Small_Sample
    {
        [Key]
        public int Id { get; set; }

        [Required]
        [Display(Name = "客户名称")]
        public string Customer { get; set; }

        [Display(Name = "生产单号")]
        public string OrderNumber { get; set; }

        [Required]
        [Display(Name = "模块数量（单位:PCS）")]
        public int Module_Number { get; set; }

        [Display(Name = "规格型号")]
        public string Mo1 { get; set; }

        [Display(Name = "模块型号")]
        public string Mo2 { get; set; }

        [Display(Name = "电源型号")]
        public string Mo3 { get; set; }

        [Display(Name = "测试目的")]
        public string Te1 { get; set; }

        [Display(Name = "测试工具")]
        public string Te2 { get; set; }

        [Display(Name = "测试条件")]
        public string Te3 { get; set; }

        [Display(Name = "固定值（温度）")]
        public string fixed_value { get; set; }

        [Required]
        [Display(Name = "实际点间距")]
        public float Actual_point { get; set; }

        [Required]
        [Display(Name = "订单接收卡")]
        public string ReceivingCard { get; set; }

        [Required]
        [Display(Name = "点数")]
        public int Points { get; set; }

        [Required]
        [Display(Name = "扫描数")]
        public int Scanningway { get; set; }

        [Required]
        [Display(Name = "调式系统")]
        public string system { get; set; }

        [Required]
        [Display(Name = "驱动板电压")]
        public float Driveplate { get; set; }

        //样品参数实测值
        [Display(Name = "角度颜色")]
        public string An1 { get; set; }

        [Display(Name = "角度亮度")]
        public Decimal An2 { get; set; }

        [Display(Name = "角度坐标")]
        public float An3 { get; set; }

        [Display(Name = "客户要求亮度")]
        public int Cu1 { get; set; }

        [Display(Name = "客户要求坐标")]
        public float Cu2 { get; set; }

        [Display(Name = "分压电阻阻值（欧）")]
        public int Re1 { get; set; }

        [Display(Name = "可调电阻阻值（欧）")]
        public int Re2 { get; set; }

        [Display(Name = "单IC引脚电流（mA）")]
        public Decimal IC1 { get; set; }

        [Display(Name = "IC VDS")]
        public Decimal IC2 { get; set; }

        [Display(Name = "模块电流")]
        public Decimal Module_current { get; set; }

        [Display(Name = "IC型号")]
        public string IC3 { get; set; }

        [Display(Name = "刷新频率")]
        public int R1 { get; set; }

        [Display(Name = "接收卡Data包")]
        public string R2 { get; set; }

        [Display(Name = "接收卡带载点数")]
        public int R3 { get; set; }

        [Display(Name = "LCT版本")]
        public string LCT { get; set; }

        [Display(Name = "亮度有效率")]
        public float De1 { get; set; }

        [Display(Name = "灰度等级")]
        public int De2 { get; set; }

        [Display(Name = "DCLK")]
        public Decimal DCLK { get; set; }

        [Display(Name = "电源带载率")]
        public float Load { get; set; }

        [Display(Name = "每平方最大功耗")]
        public int Per { get; set; }

        [Display(Name = "GCLK")]
        public Decimal GCLK { get; set; }

        [Display(Name = "MOS管型号")]
        public string MOS1 { get; set; }

        [Display(Name = "MOS压降(V)")]
        public Decimal MOS2 { get; set; }

        [Display(Name = "电流增益")]
        public float Current { get; set; }

        //小样灯管及参数（供应商测试值/联建测试值)
        [Display(Name = "LED厂家及型号")]
        public string LED { get; set; }

        [Display(Name = "实际")]
        public float Actual { get; set; }

        [Display(Name = "范围")]
        public float Scopeof { get; set; }

        [Display(Name = "Min")]
        public float Min { get; set; }

        [Display(Name = "Max")]
        public float Max { get; set; }

        [Display(Name = "IV（mcd）")]
        public float IVd { get; set; }

        [Display(Name = "WD(nm)")]
        public float WDm { get; set; }

        [Display(Name = "正向电压（vf）")]
        public float Forward { get; set; }

        [Display(Name = "IV@IF(mA)")]
        public int IVm { get; set; }

        [Display(Name = "报告创建人")]
        public string Creator { get; set; }

        [Display(Name = "报告创建时间")]
        [DataType(DataType.DateTime)]
        [DisplayFormat(DataFormatString = "{0:yyyy/MM/dd}", ApplyFormatInEditMode = true)]
        public DateTime? CreatedDate { get; set; }

        [Display(Name = "审核人")]
        public string Assessor { get; set; }

        [Display(Name = "报告审核时间")]
        [DataType(DataType.DateTime)]
        [DisplayFormat(DataFormatString = "{0:yyyy/MM/dd}", ApplyFormatInEditMode = true)]
        public DateTime? AssessedDate { get; set; }

        [Display(Name = "核准人")]
        public string Approved { get; set; }

        [Display(Name = "核准时间")]
        [DataType(DataType.DateTime)]
        [DisplayFormat(DataFormatString = "{0:yyyy/MM/dd}", ApplyFormatInEditMode = true)]
        public DateTime? ApprovedDate { get; set; }

        [Display(Name = "备注")]
        public string ReportRemark { get; set; }

    }
}