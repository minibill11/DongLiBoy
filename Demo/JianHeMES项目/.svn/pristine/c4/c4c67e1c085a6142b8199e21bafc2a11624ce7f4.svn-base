@model JianHeMES.Models.Warehouse_Material_BaseInfo

@{
    ViewBag.Title = "修改库位号清单";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Scripts/Bootstraps/Element-ui.css" rel="stylesheet" />
<script src="~/Scripts/axios.min.js"></script>
<script src="~/Scripts/Bootstraps/Element-ui.js"></script>
<script src="~/Content/styleFile/solder/solderJavascript.js"></script>
<style>
    h2 {
        text-align: center;
        font-size: 1.8rem;
    }
</style>
<h2>修改库位号清单</h2>

<div id="app">
    <el-button @@click="getInfos" type="primary">主要按钮</el-button>
    <el-table v-bind:data="tableData"
              style="width: 100%">
        <el-table-column prop="MaterialBacrcode"
                         label="日期"
                         width="180">
        </el-table-column>
        <el-table-column prop="MaterialNumber"
                         label="姓名"
                         width="180">
        </el-table-column>
        <el-table-column prop="OldWarehouseNum"
                         label="姓名"
                         width="180">
        </el-table-column>
        <el-table-column prop="NewWarehouseNum"
                         label="姓名"
                         width="180">
        </el-table-column>
        <el-table-column label="操作">
        </el-table-column>
    </el-table>
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

<script>
    const app = new Vue({
        el: "#app",
        data: {
            tableData: [{
                date: '2016-05-02',
                name: '王小虎',
                address: '上海市普陀区金沙江路 1518 弄'
            }, {
                date: '2016-05-04',
                name: '王小虎',
                address: '上海市普陀区金沙江路 1517 弄'
            }, {
                date: '2016-05-01',
                name: '王小虎',
                address: '上海市普陀区金沙江路 1519 弄'
            }, {
                date: '2016-05-03',
                name: '王小虎',
                address: '上海市普陀区金沙江路 1516 弄'
            }]
        },
        methods: {
            // 获取列表
            getInfos() {
                axios.post("/Warehouse_Material/LocationQuery").then(res=> {
                    console.log(res.data)
                    this.tableData = res.data;
                })
            }
        }
    })
</script>
