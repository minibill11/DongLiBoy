                                               placeholder="请输入面积"
                                               class="el-input__inner">
                                    </div>
                                    <el-button type="success" :loading="props.row.btnloading" @@click="areaCompute(props.row)">计算</el-button>
                                    <div class="areaResult" v-show="props.row.infoResult">结果：{{props.row.infoResult}}</div>
                                </div>
                                <table class="table table-bordered">
                                    <thead>
                                        <tr class="theadtd">
                                            <td>详细表</td>
                                            <td v-for="i in props.row.info.detail" v-if="i.person!=0||i.capacity!=0||i.capacityPerHour!=0">
                                                <span>{{i.name}}</span>
                                            </td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>标配人数</td>
