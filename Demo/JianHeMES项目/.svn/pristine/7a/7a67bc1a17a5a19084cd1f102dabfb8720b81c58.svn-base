                                         min-width="150">
                            <template slot-scope="scope">
                                <div v-if="scope.row.mzOption.length>1">
                                    <el-select v-model="scope.row.mzID" @@change="mzselect(scope.row)" placeholder="请选择">
                                        <el-option v-for="item in scope.row.mzOption"
