
.container, .body-content {
    padding-left: 0px;
    padding-right: 0px;
}

.el-header {
    height: auto !important;
}

.el-header, .el-main {
    padding: 10px 0 5px;
}

.el-main {
    padding-top: 0;
}

[v-cloak] {
    display: none;
}

.cell {
    text-align: center;
}

.el-table__body .cell {
    line-height: 20px;
}

h2 {
    margin: 0;
}

.el-link {
    font-size: 12px;
}


/*表格*/
.el-table .cell /*, .el-table th, .el-table td*/, .el-table th div {
    padding: 0;
}

.el-divider--horizontal {
    margin: 0;
}

.el-table tbody tr td {
    padding: 0;
}

.cellParent .cell {
    height: auto;
}

.statu-table-style > .cell {
    padding: 3px 0;
}

.cellParent {
}

.el-table .cell, .el-table--border td:first-child .cell, .el-table th > .cell, .el-table--border th:first-child .cell {
    padding: 0;
}

.el-table__body .cell {
    line-height: 20px;
}

.el-table__empty-block {
    min-height: 20px;
}

.el-table__empty-text {
    line-height: 20px;
}

.el-form-item--small.el-form-item {
    margin-bottom: 2px;
}

.el-popover {
    padding: 0;
}

.el-table--mini th {
    padding: 3px 0;
}

.el-table__fixed-body-wrapper {
    top: 37px;
}

.gongduan .cell {
    line-height: 15px;
}

.el-table_topfixed {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 3;
}

.total_div {
    display: grid;
    grid-template-columns: 50% 50%;
    justify-items: end;
}

.total_rightdiv {
    justify-self: start;
}
