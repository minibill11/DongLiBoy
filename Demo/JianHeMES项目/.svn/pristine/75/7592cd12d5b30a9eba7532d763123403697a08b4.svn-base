.sa-header {
    width: 100%;
    text-align: center;
}

.sa-title {
    font-weight: 400;
    font-size: 24px;
}

.sa-header-btn {
    padding: 15px 0 0 0;
}

hr.style-one {
    width: 100%;
    margin: 0 auto;
    border: 0;
    height: 0;
    border-top: 1px solid rgb(179, 216, 255);
    border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

/*导出按钮样式*/
.el-button--success.is-plain {
    color: #2bcfaa;
    background: #e6f8ea;
    border-color: #8cdfcb;
}
.el-button--success.is-plain:hover {
    color: #ffffff;
    background-color: #2bcfaa;
    border-color: #2bcfaa;
}

.el-button--success {
    color: #FFF;
    background-color: #2bcfaa;
    border-color: #2bcfaa;
}
.el-button--success:hover {
    color: #FFF;
    background-color: #2bcfaa;
    border-color: #2bcfaa;
}