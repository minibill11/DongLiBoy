@section scripts {
    <script src="~/Scripts/jquery.signalR-2.3.0.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script>
        $(function () {
            var mypush1 = $.connection.Hub1;
            var mypush3 = $.connection.Hub3;
            var mypush4 = $.connection.Hub4;
            var mypush5 = $.connection.Hub5;
            var mypush6 = $.connection.Hub6;
                    //以下为数据显示方法，目前是用id定位节点赋值，改为vue方式用data显示数据即可，
                    //css控制可以写在html上，用vue的方式判断显示颜色和描述，
                    //判断逻辑为：
                    //温度：16℃～28℃ 是正常、绿色，非范围内报警
                    //湿度：45%～70% 是正常、绿色，非范围内报警
            mypush1.client.sendTH1 = function (TH1_json) {
                var TH_40004493_1 = JSON.parse(TH1_json.TH_40004493_1);
                var TH_40004518_1 = JSON.parse(TH1_json.TH_40004518_1);
                var TH_40004518_2 = JSON.parse(TH1_json.TH_40004518_2);
                var TH_40004518_3 = JSON.parse(TH1_json.TH_40004518_3);
                var TH_40004518_6 = JSON.parse(TH1_json.TH_40004518_6);
                var TH_40004518_4 = JSON.parse(TH1_json.TH_40004518_4);
                var TH_40004518_5 = JSON.parse(TH1_json.TH_40004518_5);

                var TH_40004557_1 = JSON.parse(TH1_json.TH_40004557_1);
                var TH_40004557_2 = JSON.parse(TH1_json.TH_40004557_2);
                var TH_40004557_3 = JSON.parse(TH1_json.TH_40004557_3);
                var TH_40004557_4 = JSON.parse(TH1_json.TH_40004557_4);
                var TH_40004557_5 = JSON.parse(TH1_json.TH_40004557_5);
                //噪声
                var Noise = JSON.parse(TH1_json.Noise);

                //1F_点1
                $("#1F_temprature1").html(TH_40004518_3.Tem);
                $("#1F_humidity1").html(TH_40004518_3.Hum);
                $("#1F_time1").html(TH_40004518_3.RecordTime.split('T').join(" "));
                if (TH_40004518_3.Tem > 16 && TH_40004518_3.Tem < 28) {
                    if (TH_40004518_3.Hum > 45 && TH_40004518_3.Hum < 70) {
                        $("#1F_state1").html("正常").css("color", "green");
                    } else { $("#1F_state1").html("湿度警报").css("color", "red"); }
                } else {
                    if (TH_40004518_3.Hum > 45 && TH_40004518_3.Hum < 70) {
                        $("#1F_state1").html("温度警报").css("color", "red");
                    } else { $("#1F_state1").html("温湿度警报").css("color", "red"); }
                }

                //1F_点2 暂改2楼编带房
                $("#1F_temprature2").html(TH_40004518_2.Tem);
                $("#1F_humidity2").html(TH_40004518_2.Hum);
