
@{
    ViewBag.Title = "温湿度实时数据显示";
}
<style>
    h5 {
        margin: 5px 0;
        font-weight: 500;
        color: #4859da;
    }

    table {
        font-size: 12px;
    }

    hr {
        border-color: #9D9D9D;
        margin: 10px 0;
    }

    .table-bordered > tbody > tr > th, .table-bordered > tbody > tr > td {
        border-color: #9D9D9D;
        line-height: 10px;
        padding: 5px;
    }

    .row div {
        width: 165px;
        height: 140px;
        float: left;
        margin: 0px 10px 15px 10px;
    }

    #GJB1, #GJB2 {
        color: red;
    }

    a, a:hover, a:link, a:active, a:visited {
        text-decoration: none;
        color: #111;
    }

    a {
        padding-left: 0 !important;
    }
</style>
<h3 style="text-align:center;margin:15px 0 0px">@ViewBag.Title</h3>


