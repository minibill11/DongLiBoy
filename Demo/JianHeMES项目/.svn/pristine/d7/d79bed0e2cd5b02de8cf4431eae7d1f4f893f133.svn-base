/*全局样式*/
*, body {
    margin: 0;
    padding: 0;
}

#app {
    padding: 50px 0 0;
}

.body-content, .container {
    padding-left: 0px;
    padding-right: 0px;
}

.el-header {
    height: auto !important;
    padding: 0;
}

.el-main {
    min-height: 600px;
    padding: 20px 0 0;
}

[v-cloak] {
    display: none;
}

a:hover {
    text-decoration: none;
    color: #000;
}

.inputframe {
    width: 280px;
    text-align: right;
    margin: 2px auto;
}

.el-input {
    width: 220px;
}

.divframe {
    width: 280px;
    margin: 1px auto;
    text-align: center;
    border: 1px solid #ccc;
    min-height: 280px;
}

a:hover {
    text-decoration: none;
    color: #000;
}

.el-button--small {
    padding: 8px 12px;
}

.el-table th {
    padding: 5px 0;
    background-color: #f4df42;
    color: #000;
    text-align: center;
}

.el-table td {
    padding: 2px 0;
    text-align: center;
}

.el-table .warning-row {
    background: #fcd2d2;
}

.el-table .success-row {
    background: #f0f9eb;
}

.green {
    color: #67c23a;
}

.red {
    color: #f56c6c;
}

.lineblock {
    display: block;
    float: left;
    margin: 2px 2px 20px;
}

p {
    margin: 1px 0 0;
}

.boxspan {
    display: inline-block;
    padding: 2px 6px;
}

.outsidebar {
    background-color: #f4df42;
    font-size: 1.4em;
    border-radius: 4px;
    margin: 0 10px;
}

.outsidebox {
    padding: 8px 4px;
    font-size: 1.0em;
    text-align: left;
    display: block;
}

.el-button--medium {
    padding: 10px;
}

.numcount {
    margin: 0px;
    font-size: 15px;
    border: 1px solid #ccc;
    position: relative;
    height: 23px;
}

    .numcount > span {
        display: inline-block;
        position: absolute;
    }

.listbarcode {
    display: inline-block;
    font-size: 13px;
    height: 16px;
    line-height: 16px;
    background: #ffe2b3;
    border-radius: 6px;
    padding:1px 1px 0 4px;
}
.adiv {
    display: inline-block;
}