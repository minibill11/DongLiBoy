.baseline {
    position: absolute;
    border-radius: 2px;
    opacity: 0.6;
    margin-top: -7px;
    height: 12px;
    background: #ffd180;
    border-radius: 8px;
    border: 1px solid rgb(255, 153, 0);
}

/* move task lines upper */
.gantt_task_line,
.gantt_line_wrapper {
    margin-top: -9px;
}

.gantt_side_content {
    margin-bottom: 7px;
}

.gantt_task_link .gantt_link_arrow {
    margin-top: -12px
}

.gantt_side_content.gantt_right {
    bottom: 0;
}

/*显示进度*/
.gantt_task_progress {
    text-align: left;
    padding-left: 10px;
    box-sizing: border-box;
    color: white;
    font-weight: bold;
}
/*周末设置颜色*/
.weekend {
    background-color: rgba(231, 246, 231, 0.5);
}

.gantt_selected .weekend {
    background: #f7eb91;
}
/*提示工具*/
.gantt_tooltip {
    font-size: 14px;
    line-height: 25px;
}
