        <el-table-column label="IC面贴装" @*class-name="cc"*@>
            <el-table-column label="工序描述" width="110">
                <template slot-scope="scope">
                    <div class="equalTd">
                        <hover-div touchtype="popover" :ispower="limitsRole[2]">
                            <popover-upload :rowdata="scope.row"
                                            :isnull="scope.row.icProductName"
                                            :nametext="scope.row.icProductName"
                                            section="SMT"></popover-upload>
                        </hover-div>
                        <jump-link :rowdata="scope.row"
                                   section="SMT"
                                   :ispower="limitsRole[3]"
                                   :img="scope.row.SMTjpg"
                                   :pdf="scope.row.SMTpdf"
                                   :nametext="scope.row.icProductName"
                                   :examinanstatu="scope.row.SMTExaminanMessage"></jump-link>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="线别" width="60">
                <template slot-scope="scope" v-if="scope.row.SMTExaminanMessage=='审核通过'">
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>1~14线<br />最大产能</div>
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>1~14线<br />最小产能</div>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标配人数" width="60">
                <template slot-scope="scope">
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>{{scope.row.icMaxStandardTotal}}</div>
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>{{scope.row.icMinStandardTotal}}</div>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="标准产能(PCS/H)" width="60">
                <template slot-scope="scope">
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>{{scope.row.icMaxStandardOutput}}</div>
                    </div>
                    <el-divider></el-divider>
                    <div class="equalTd">
                        <div v-if="scope.row.icProductName=='/'" class="red">/</div>
                        <div v-else>{{scope.row.icMinStandardOutput}}</div>
                    </div>
                </template>
            </el-table-column>
        </el-table-column>
        <el-table-column label="灯面贴装">
            <el-table-column prop="LightProductName" label="工序描述" width="110">
                <template slot-scope="scope">
                    <div v-if="scope.row.icProductName!=null
                         && scope.row.icProductName!='/'
                         && scope.row.LightMaxStandardTotal==0
                         && scope.row.LightMinStandardTotal==0">/</div>
                    <div v-else class="equalTd">
                        <hover-div touchtype="popover" :ispower="limitsRole[2]">
                            <popover-upload :rowdata="scope.row"
                                            :isnull="scope.row.LightProductName"
                                            :nametext="scope.row.LightProductName"
                                            section="SMT"></popover-upload>
                        </hover-div>
                        <jump-link :rowdata="scope.row"
                                   section="SMT"
                                   :ispower="limitsRole[3]"
                                   :img="scope.row.SMTjpg"
                                   :pdf="scope.row.SMTpdf"
                                   :nametext="scope.row.LightProductName"
                                   :examinanstatu="scope.row.SMTExaminanMessage"></jump-link>
                    </div>
                </template>
            </el-table-column>
            <el-table-column label="线别" width="60">
                <template slot-scope="scope" v-if="scope.row.SMTExaminanMessage=='审核通过'">
                    <div v-if="scope.row.icProductName!=null
