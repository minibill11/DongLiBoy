                         && scope.row.LightMaxStandardTotal==0
                         && scope.row.LightMinStandardTotal==0">/</div>
                    <template v-else>
                        <div class="equalTd">
                            <div v-if="scope.row.LightProductName=='/'" class="red">/</div>
                            <div v-else>1~14线<br />最大产能</div>
                        </div>
                        <el-divider></el-divider>
                        <div class="equalTd">
                            <div v-if="scope.row.LightProductName=='/'" class="red">/</div>
