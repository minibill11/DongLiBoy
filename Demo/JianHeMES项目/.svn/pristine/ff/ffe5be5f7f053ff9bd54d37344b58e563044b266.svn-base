
<el-table :data="tableList"
          max-height="600"
          size="small "
          align="center"
          :row-style="rowStyle"
          cell-class-name="cellParent"
          @*highlight-current-row*@
          border>
    <div slot="empty">暂无数据</div>
    <el-table-column type="index" @*prop="id"*@ label="序号" fixed width="40">
    </el-table-column>
    <el-table-column @*prop="Type" label="型号"*@ fixed width="70">
        <template slot="header" scope="scope">
            平台
            <br />型号
        </template>
        <template slot-scope="scope">
            <div class="equalTd">
                <input-edit :isedit="scope.row.TotalEdit"
                            v-on:input="scope.row.Type = $event"
                            :nametext="scope.row.Type"></input-edit>
                <br />
                <input-edit :isedit="scope.row.TotalEdit"
                            v-on:input="scope.row.Platform = $event"
                            :nametext="scope.row.Platform"></input-edit>
            </div>
        </template>
    </el-table-column>
    <el-table-column label="平台模块" fixed width="80">
        <template slot="header" scope="scope">
            平台
            <br />模块
        </template>
        <template slot-scope="scope">
            <div class="equalTd">
                <input-edit :isedit="scope.row.TotalEdit"
                            v-on:input="scope.row.PlatformModul = $event"
                            :nametext="scope.row.PlatformModul"
                            class="pcbInput"></input-edit>
            </div>
        </template>
    </el-table-column>
    <el-table-column label="SMT">
        <el-table-column prop="PCB" label="产品PCB编号" width="100">
            <template slot-scope="scope">
                <div class="equalTd">
                    <input-edit :isedit="scope.row.TotalEdit"
                                v-on:input="scope.row.PCB = $event"
                                :nametext="scope.row.PCB"
                                class="pcbInput"></input-edit>
                    @*<div v-if="scope.row.PCB.split('(').length==2">
                            {{scope.row.PCB.split('(')[0]}}<br />
                            ({{scope.row.PCB.split('(')[1]}}
                        </div>
                        <div v-else>
                            {{scope.row.PCB}}
                        </div>*@
                </div>
            </template>
        </el-table-column>

        <el-table-column label="PCB烘烤" width="60">
            <template slot-scope="scope">
                <div class="equalTd">
                    <input-edit :isedit="scope.row.TotalEdit"
                                v-on:input="scope.row.PCBDrying = $event"
                                :nametext="scope.row.PCBDrying"
                                class="pcbInput"></input-edit>
                </div>
            </template>
        </el-table-column>
        @*<el-table-column label="IC面-灯面">
                <el-table-column label="模组需求数量" width="90">
                    <template slot-scope="scope">
                        <div class="equalTd">
                            <div v-if="scope.row.LightProductName=='/'" class="red">/</div>
