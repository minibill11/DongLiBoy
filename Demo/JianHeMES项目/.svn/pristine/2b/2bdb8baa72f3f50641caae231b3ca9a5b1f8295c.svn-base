@{
    ViewBag.Title = "指标日报表";
}
@* css放置处 *@
@section cssStyle {
    <link rel="stylesheet/less" type="text/css" href="~/Content/KPI/KPI_TeamEvaluation.less" />
}

<el-container id="app" v-cloak>
    <el-header>
        <h1 class="title">{{show?'计划信息':'计划信息录入'}}指标日报表</h1>
    </el-header>
    <el-main v-loading="loading">
        <kpi-plan-component v-show="show"
                            v-model="show"
                            :ordernumlist="Ordernum.list"
                            :departmentlist="Department.list"
                            :grouplist="Group.list"
                            :sectionlist="Section.list"
                            :processlist="Process.list"
                            :executelist="ExecuteTime.list">
        </kpi-plan-component>
        <kpi-plan-input-component v-show="!show"
                                  v-model="show"
                                  :ordernumlist="Ordernum.list"
                                  :departmentlist="Department.list"
                                  :grouplist="Group.list"
                                  :sectionlist="Section.list"
                                  :processlist="Process.list"></kpi-plan-input-component>
    </el-main>
</el-container>
@* 分部页放置处 *@
@section renderPage {
    @RenderPage("~/Views/Warehouse_Material/_wareEdit.cshtml")
    @RenderPage("_KPI_Plan.cshtml")
    @RenderPage("_KPI_Plan_Input.cshtml")
}
@* js放置处 *@
@section jsScript {
    <script src="~/Scripts/KPI/KPI_TeamEvaluation.js"></script>
    <script>
        const app = {
            mixins: [TeamEvaluation],
            data: function () {
                return {
                    show: true,
                }
            },
            mounted() {
                //this.show = false;
            }
        };
    </script>
}

