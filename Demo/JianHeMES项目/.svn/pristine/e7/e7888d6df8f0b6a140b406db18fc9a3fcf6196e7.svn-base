<div class="mcFrame" v-if="isOutside">
    <div v-show="tagVersion==='old'">
        <div class="mcFrame_top">
            <div class="mcFrame_top_1">
                <img v-show="imgShow" class="imgicon" src="~/Images/LOGO.jpg" />
                <h1 v-bind:class="[selectVal2!=''?'changecolor':'',imgShow?'mcFrame_top_1_h':'mcFrame_top_1_h_noimg']"
                    v-text="selectVal2!=''?selectVal2:selectVal"></h1>
            </div>
            <div class="borderTop mcFrame_top_2">
                <div class="masterBarDiv  text-center">
                    <svg id="masterBarcode"></svg>
                </div>
            </div>
            <div class="borderTop mcFrame_top_3" style="height:120px;">
                <div class="mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.mz[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{maozhongliang}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.jz[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{jingzhong}}</span></div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.wlms[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{wuliao}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.px[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{screenNum}}</span></div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.jh[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{sntn}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.sl[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{customize?'':mozutable.length}} PCS</span></div>
                </div>
            </div>
        </div>
        <div class="mcFrame_bottom borderTop">
            <div class="mcFrame_bottom_1">
                <template v-for="item in mozutable">
                    <span class="boxspan" v-text="item.barcode"></span>
                </template>
            </div>
        </div>
    </div>
    <div v-show="tagVersion==='new'" class="new-tag-frame">
        <div class="mcFrame_top" style="height:auto">
            <div class="borderTop mcFrame_top_3" style="height:86px;border-top:none;">
                <div class="mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.dd_new[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="width:202px;">
                        <span v-bind:class="[selectVal2!=''?'changecolor':'']"
                              v-text="selectVal2!=''?selectVal2:selectVal"></span>
                    </div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.ggxh[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{ITEMNO}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.ys[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{COLOURS}}</span></div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.sl_new[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span v-if="typeVal.split('装').length>1">{{typeVal.split('装')[1]}}/{{CTNS_PSC}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.jh_new[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span v-if="sntn.split('/').length>1">{{sntn.split('/')[0]}} OF {{sntn.split('/')[1]}}</span></div>
                </div>
            </div>
            <div class="borderTop mcFrame_top_3" style="height:86px;">
                <div class="mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1" style="width:320px;">
                        <div :style="{'height':Remark?'28px':'25px'}">{{Remark}}</div>
                    </div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.wlms[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{wuliao}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.px[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{screenNum}}</span></div>
                </div>
                <div class="borderTop mcFrame_top_3_1">
                    <div class="mcFrame_top_3_1_1">
                        <span>{{lengTags.mz[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1" style="border-right:none;"><span>{{maozhongliang}}</span></div>
                    <div class="mcFrame_top_3_1_1" style="border-left:1px solid #000;">
                        <span>{{lengTags.jz[leng]}}</span>
                    </div>
                    <div class="mcFrame_top_3_1_1"><span>{{jingzhong}}</span></div>
                </div>
            </div>
            <div class="borderTop mcFrame_top_2" style="height:80px;">
                <div class="masterBarDiv  text-center">
                    <svg id="masterBarcode"></svg>
                </div>
            </div>
        </div>
        <div class="mcFrame_bottom borderTop">
            <div class="mcFrame_bottom_1">
                <template v-for="item in mozutable">
                    <span class="boxspan" v-text="item.barcode"></span>
                </template>
            </div>
        </div>
    </div>
</div>
<div class="inFrame" v-else>
    <div v-show="tagVersion==='old'">
        <table class="table table-bordered">
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.dd[leng]}}</span>
                </td>
                <td class="inFram_top"><span>{{selectVal}}</span></td>
            </tr>
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.jh[leng]}}</span>
                </td>
                <td class="inFram_top"><span>{{sntn}}</span></td>
            </tr>
            <tr>
                <td colspan="2" class="inFram_bar">
                    <div class="masterBarDiv  text-center">
                        <svg id="masterBarcode"></svg>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="2" class="inFram_bottom">
                    <template v-for="item in mozutable">
                        <span class="boxspan" v-text="item.barcode.substr(-6)"></span>
                    </template>
                </td>
            </tr>
        </table>
    </div>
    <div v-show="tagVersion==='new'">
        <table class="table table-bordered new-tag-table">
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.dd_new[leng]}}</span>
                </td>
                <td class="inFram_top"><span>{{selectVal}}</span></td>
            </tr>
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.ggxh[leng]}}</span>
                </td>
                <td class="inFram_top"><span>{{ITEMNO}}</span></td>
            </tr>
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.ys[leng]}}</span>
                </td>
                <td class="inFram_top"><span>{{COLOURS}}</span></td>
            </tr>
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.sl_new[leng]}}</span>
                </td>
                <td class="inFram_top"><span v-if="typeVal.split('装').length>1">{{typeVal.split('装')[1]}}/{{CTNS_PSC}}</span></td>
            </tr>
            <tr>
                <td class="inFram_top">
                    <span>{{lengTags.jh_new[leng]}}</span>
                </td>
                <td class="inFram_top"><span v-if="sntn.split('/').length>1">{{sntn.split('/')[0]}} OF {{sntn.split('/')[1]}}</span></td>
            </tr>
            <tr>
                <td colspan="2">
                    <div class="tag-remark">{{Remark}}</div>
                </td>
            </tr>
            <tr>
                <td colspan="2" class="inFram_bar">
                    <div class="masterBarDiv  text-center">
                        <svg id="masterBarcode"></svg>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="2" class="inFram_bottom">
                    <template v-for="item in mozutable">
                        <span class="boxspan" v-text="item.barcode.substr(-6)"></span>
                    </template>
                </td>
            </tr>
        </table>
    </div>
</div>