@{
    ViewBag.Title = "MES使用说明书";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
}

<style>
    body, button, dd, div, dl, dt, form, h1, h2, h3, input, li, ol, p, pre, td, textarea, th, ul {
        margin: 0;
        padding: 0;
    }

    body {
        line-height: 24px;
        font: 14px Arial,sans-serif;
        color: #293038;
    }

    div {
        display: block;
    }

    .myheader {
        height: 50px;
        background-color: #293038;
        position: relative;
    }

    .header-nav {
        position: absolute;
        right: 0;
        top: 0;
        margin-right: 20px;
    }

        .header-nav a {
            color: #a7b3c8;
            text-decoration: none;
            line-height: 50px;
            padding: 0 20px;
            cursor: pointer;
        }

        .header-nav li {
            list-style: none;
            margin: 0 10px;
            display: inline-block;
            position: relative;
        }

    .nav-more-down {
        content: '';
        width: 0;
        height: 0;
        border-style: solid dashed dashed;
        border-color: #a7b3c8 transparent transparent;
        overflow: hidden;
        cursor: pointer;
        transition: all .2s;
        -webkit-transition: all .2s;
        position: absolute;
        top: 50%;
        right: 3px;
        margin-top: -2px;
        border-width: 4px;
    }

    .nav-more-up {
        content: '';
        width: 0;
        height: 0;
        border-style: dashed dashed solid;
        border-color: transparent transparent #a7b3c8;
        overflow: hidden;
        cursor: pointer;
        transition: all .2s;
        -webkit-transition: all .2s;
        position: absolute;
        top: 50%;
        right: 3px;
        margin-top: -6px;
        border-width: 4px;
    }

    .nav-more-right {
        content: '';
        width: 0;
        height: 0;
        border-style: dashed solid dashed;
        border-color: transparent transparent transparent #fff;
        overflow: hidden;
        cursor: pointer;
        transition: all .2s;
        -webkit-transition: all .2s;
        position: absolute;
        top: 50%;
        right: 6px;
        margin-top: -4px;
        border-width: 4px;
    }

    .myleftside {
        width: 150px;
        position: absolute;
        top: 50px;
        left: 0;
        bottom: 0;
        background-color: #2f4056;
        overflow-x: hidden;
        z-index: 1000;
    }

    .myleftside-scroll {
        position: relative;
        width: 170px;
        height: 100%;
        overflow-x: hidden;
    }

    .left-nav {
        margin-top: 10px;
        width: 150px;
    }

    .left-nav-item {
        width: 100%;
        display：block;
    }

        .left-nav-item a {
            position: relative;
            height: 50px;
            line-height: 50px;
            text-overflow: ellipsis;
            overflow: hidden;
            white-space: nowrap;
            display: block;
            padding: 0 10px;
            color: #fff;
            text-decoration: none;
            cursor: pointer;
            transition: background-color .3s;
            -moz-transition: background-color .3s; /* Firefox 4 */
            -webkit-transition: background-color .3s; /* Safari and Chrome */
            -o-transition: background-color .3s; /* Opera */
        }

            .left-nav-item a:hover {
                background-color: #26364b;
            }

    .left-nav-item-selected {
        background-color: #1890ff;
        color: #fff;
    }

    .mylocation {
        position: absolute;
        left: 150px;
        right: 0;
        top: 50px;
        box-shadow: 0px 7px 7px 0px #eff2f7;
        -moz-box-shadow: 0px 7px 7px 0px #eff2f7;
        -webkit-box-shadow: 0px 7px 7px 0px #eff2f7;
        z-index: 999;
    }

        .mylocation .alink {
            padding: 0 20px;
            height: 40px;
            line-height: 40px;
            color: #8f9fb9;
        }

            .mylocation .alink a {
                color: #8f9fb9;
                text-decoration: none;
            }

    .into-left-menu {
        width: 220px;
        position: absolute;
        left: 170px;
        top: 160px;
        bottom: 40px;
        overflow-x: hidden;
    }

    .into-left-menu-scroll {
        position: relative;
        width: 240px;
        height: 100%;
        overflow-x: hidden;
    }

    .search-part {
        height: 40px;
        width: 220px;
        position: absolute;
        top: 110px;
        left: 170px;
    }

        .search-part form {
            width: 100%;
            position: relative;
        }

        .search-part input {
            width: 100%;
            height: 40px;
            line-height: 40px;
            border-radius: 20px;
            background-color: #fff;
            border: 0;
            padding-left: 10px;
            box-sizing: border-box;
            color: #8f9fb9;
            border: 1px solid #ccd3de;
        }

        .search-part button {
            width: 40px;
            height: 40px;
            position: absolute;
            right: 0;
            top: 0;
            border: 0;
            cursor: pointer;
            background-color: transparent;
        }

    .icon-search {
        width: 12px;
        height: 12px;
        border-radius: 100%;
        border: 1px solid #8f9fb9;
        position: relative;
        margin: auto;
        display: block;
    }

        .icon-search:after {
            content: "";
            -webkit-transform: rotate(45deg);
            -moz-transform: rotate(45deg);
            transform: rotate(45deg);
            width: 6px;
            height: 1px;
            position: absolute;
            top: 12px;
            left: 9px;
            background-color: #8f9fb9;
        }

    .left-menu-all {
        width: 220px;
    }

        .left-menu-all li {
            line-height: 40px;
            display: block;
            border: 1px solid #ccd3de;
            border-radius: 4px;
            margin: 5px 0;
        }

            .left-menu-all li a {
                position: relative;
                height: 40px;
                line-height: 40px;
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
                display: block;
                padding: 0 15px;
                color: #293038;
                text-decoration: none;
                cursor: pointer;
                background-color: #eff2f7;
                font-weight: bold;
            }

                .left-menu-all li a .nav-more-down {
                    right: 10px;
                    margin-top: -3px;
                    border-width: 6px;
                }

                .left-menu-all li a .nav-more-up {
                    right: 9px;
                    margin-top: -9px;
                    border-width: 6px;
                }

            .left-menu-all li dl a {
                background-color: #fff;
                height: 36px;
                line-height: 36px;
                font-weight: normal;
                font-size: 13px;
                transition: color .5s;
                -moz-transition: color .5s; /* Firefox 4 */
                -webkit-transition: color .5s; /* Safari and Chrome */
                -o-transition: color .5s; /* Opera */
            }

                .left-menu-all li dl a:hover {
                    color: #1890ff;
                }

            .left-menu-all li dl dd .selected {
                color: #1890ff;
            }

    .viewtittle {
        position: absolute;
        left: 410px;
        right: 0;
        top: 110px;
        height: 40px;
        line-height: 40px;
        background-color: #eff2f7;
        text-align: center;
        font-size: 24px;
        margin-bottom: 20px;
        z-index: 800;
    }

    .viewpage {
        position: absolute;
        left: 410px;
        right: 0;
        top: 155px;
        bottom: 40px;
        width: auto;
        overflow-y: hidden;
        box-sizing: border-box;
        background-color: #fff;
        overflow-x:hidden;
    }

        .viewpage .mycontent {
            box-shadow: 0 0 15px #8f9fb9;
            -moz-box-shadow: 0 0 15px #8f9fb9;
            -webkit-box-shadow: 0 0 15px #8f9fb9;
            margin: 10px auto;
        }

            .viewpage .mycontent .pdfcontent {
                width: 100%;
                height: 100%;
            }

    .myfooter {
        position: fixed;
        left: 150px;
        right: 0;
        bottom: 0;
        height: 40px;
        line-height: 40px;
        background-color: #eff2f7;
        text-align: center;
        font-size: 12px;
        color: #8f9fb9;
    }
</style>



