@{
    ViewBag.Title = "生产管控信息";
}
<link href="~/Scripts/Bootstraps/bootstrap-select.css" rel="stylesheet" />
<script src="~/Scripts/Bootstraps/bootstrap-select.js"></script>
<script src="~/Scripts/imgJS/jquery.artZoom.js"></script>
<link href="~/Content/styleFile/jquery.artZoom.css" rel="stylesheet" />
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<base target="_blank" />
<style>
    * {
        margin: 0;
        padding: 0;
    }

    .body-content, .container {
        padding-left: 0px;
        padding-right: 0px;
    }

    .table > thead > tr > th, .table > tbody > tr > td {
        text-align: center;
        vertical-align: middle;
        /*word-break:break-all;*/
        padding: 2px 1px;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(-n+5), .table > thead:first-child > tr:first-child > th:nth-child(-n+5) {
        border-top: 1px solid red;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(n+6), .table > thead:first-child > tr:first-child > th:nth-child(n+6) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(-n+5), .rowTitle2 > th:nth-child(-n+3), .table > tbody > tr > td:nth-child(-n+6) {
        border: 1px solid red;
    }

    .rowTitle1 > th:nth-child(n+6), .rowTitle2 > th:nth-child(n+3), .table > tbody > tr > td:nth-child(n+7) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(5), .rowTitle2 > th:nth-child(2), .table > tbody > tr > td:nth-child(6) {
        border-right: 1px solid blue;
    }

    .table > tbody > tr > td:nth-child(n+9), .table > tbody > tr > td:nth-child(2) {
        cursor: pointer;
    }

    .table > tbody > tr > td:nth-child(n+25),
    .table > tbody > tr > td:nth-child(9),
    .table > tbody > tr > td:nth-child(10),
    .table > tbody > tr > td:nth-child(11),
    .table > tbody > tr > td:nth-child(14),
    .table > tbody > tr > td:nth-child(15),
    .table > tbody > tr > td:nth-child(18),
    .table > tbody > tr > td:nth-child(21),
    .table > tbody > tr > td:nth-child(24) {
        cursor: auto;
    }

    .table > tbody > tr:nth-child(n+1):hover {
        background-color: rgba(77, 72, 72, 0.15);
    }



    .scroll_table_content {
        width: 100%;
        height: 600px;
        overflow: auto;
    }

    #app {
        font-size: 12px;
    }

    .bgcolor100 {
        background-color: rgba(62, 215, 0, 0.4);
    }

    .bgcolor90 {
        background-color: rgba(233, 230, 0, 0.6);
    }

    .bgcolor80 {
        background-color: rgba(233, 32, 32, 0.8);
        color: white;
    }

    .modal-body, .modal-header {
        padding: 8px 10px 2px;
    }

    .showBtn {
        position: relative;
        cursor: pointer;
        padding: 0 !important;
        font-size: 11px;
    }

    .showDiv {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0px;
        right: 0px;
        visibility: hidden;
        opacity: 0;
        transition: opacity 0s;
    }

    .btnSty {
        position: absolute;
        height: 100%;
        padding: 0;
        z-index: 10;
        font-size: 12px;
    }

    .showBtn:hover .showDiv {
        visibility: visible;
        opacity: 1;
    }

    .imgModalc {
        margin-bottom: 5px;
    }

    @@media screen and (min-width: 1200px) {
        .container {
            width: 1815px;
        }

        .titleul {
            width: 1115px;
        }
    }

    #content {
        width: 1832px;
    }

    .table {
        width: 1812px;
    }

        .table > tbody > tr:first-child > td {
            border-top: none;
        }

    .selectpicker {
        border: 1px solid #ccc;
    }

    [v-cloak] {
        display: none;
    }

    .smtdiv {
        width: 100%;
        height: 24px;
        position: relative;
        font-size: 11px;
        line-height: 1.1;
    }

        .smtdiv > span {
            position: absolute;
            font-size: 10px
        }

            .smtdiv > span:first-child {
                left: 5px;
                top: 7px;
            }

            .smtdiv > span:last-child {
                right: 5px;
                top: 1px;
            }
</style>
<div id="ieShow" class="row" style="display:none;padding:200px 0">
    <h2 class="text-center">当前浏览器版本过低，建议升级浏览器</h2>
    <h4 class="text-center">建议使用Firefox浏览器，下载地址：<a href="http://www.firefox.com.cn/" target="_blank">Firefox官网</a></h4>
</div>
<div id="content" style="display:none">
    <div id="vmTitle" class="row text-center" style="position:relative;padding:15px 20px;" v-cloak>
        <span class="text-center" style="font-size:24px;font-family: inherit;font-weight: 500;">生产管控信息</span>
        <div>
            @*<span>订单号：</span>@Html.DropDownList("orderNum", (List<SelectListItem>)ViewBag.OrderNumList, "全部", new { @class = "selectpicker", data_live_search = "true", data_style = "form-control" })
                <span>  平台：@Html.DropDownList("PlatformType", (List<SelectListItem>)ViewBag.PlatformType, "全部", new { @class = "selectpicker", data_style = "form-control" })</span>*@
            <select id="ordernumVal" class="selectpicker" title="全部订单" data-live-search="true" data-selected-text-format="count" data-style="form-control" data-actions-box="true" multiple>
                @*<option value="">全部订单</option>*@
                <template v-for="item in ordernumList">
                    <option>{{item}}</option>
                </template>
            </select>
            <select id="platformVal" class="selectpicker" title="全部平台" data-live-search="true" data-selected-text-format="count" data-style="form-control" data-actions-box="true" multiple>
                @*<option value="">全部平台</option>*@
                <template v-for="item in platformList">
                    <option>{{item}}</option>
                </template>
            </select>
        </div>
        <div class="row form-group" style="position:absolute;width:150px;right:37px;top:40px">
            <span class="btn btn-default">
                @Html.ActionLink("历史记录", "ProductionControlHistory", new { id = "" }, new { target = "_blank" })
            </span><br />
            <span style="width:140px">{{nowtime}}</span>
        </div>
    </div>

