    //根据实际开始时间排序
    function sortByActualProductionTime(array, key) {
        return array.sort(function (a, b) {
            let x = a[key];
            let y = b[key];
            if (x == '未开始' && y == '未开始') {
                return 0;
            } else if (x == '未开始' && y != '未开始') {
                return 1;
            } else if (x != '未开始' && y == '未开始') {
                return -1;
            } else {
                let xt = new Date(x);
                let yt = new Date(y);
                return ((xt < yt) ? -1 : ((xt > yt) ? 1 : 0))
            };
        });
    };
    //根据出入库信息排序
    function sortByJoinAndOutDepot(array, key) {
        return array.sort(function (a, b) {
            //方案1
            let x = Number(a[key].split("/")[0]), y = Number(b[key].split("/")[0]);
            let num1 = isNaN(x) ? 0 : x, num2 = isNaN(y) ? 0 : y;
            return ((num1 > num2) ? -1 : (num1 < num2) ? 1 : 0);
            //方案2
            //let x = a[key];
            //let y = b[key];
            //return ((x > y) ? -1 : ((x < y) ? 1 : 0));
        })
    };
    //根据完成时间进行二次排序
    function sortByActualFinishTime(array, key) {
        return array.sort(function (a, b) {
            let x = a[key];
            let y = b[key];
            if (x == '' && y == '') {
                return 0;
            } else if (x == '' && y != '') {
                return 1;
            } else if (x != '' && y == '') {
                return -1;
            } else {
                let xt = new Date(x);
                let yt = new Date(y);
                return ((xt > yt) ? -1 : ((xt < yt) ? 1 : 0));
            };
        });
    };
    //根据生产排期起始日期，进行三次排序未开始部分订单
    function sortByPlanInputTime(array, key) {
        return array.sort(function (a, b) {
            if (a["ActualProductionTime"] == "未开始" && b["ActualProductionTime"] == "未开始") {
                let x = new Date(a[key]);
                let y = new Date(b[key]);
                return ((x < y) ? -1 : ((x > y) ? 1 : 0));
            };
        });
    };
    //根据SMT信息，进行四次排序未开始部分订单
    function sortBydisplayFinishRate(array, key) {
        return array.sort(function (a, b) {
            if (a["ActualProductionTime"] == "未开始" && b["ActualProductionTime"] == "未开始") {
                let x = a[key];
                let y = b[key];
                return ((x[0] && !y[0]) ? -1 : 1);
            };
        });
    };

    Date.prototype.Format = function (fmt) {
        var o = {
            "M+": this.getMonth() + 1,
            "d+": this.getDate(),
            "h+": this.getHours(),
            "m+": this.getMinutes(),
            "s+": this.getSeconds(),
            "q+": Math.floor((this.getMonth() + 3) / 3),
            "S": this.getMilliseconds()
        };
        if (/(y+)/.test(fmt))
            fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
            if (new RegExp("(" + k + ")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    }
