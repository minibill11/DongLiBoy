                                         label="合计总分"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="否定项"
                                     align="center"
                                     min-width="110">
                        <el-table-column prop="CustomerName"
                                         label="总分不得低于90分"
                                         align="center"
                                         min-width="180">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="平均人数(不得少于5人)"
                                         align="center"
                                         min-width="180">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="当月有无工伤事故"
                                         align="center"
                                         min-width="180">
                        </el-table-column>
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="出勤率"
                                     align="center"
                                     min-width="110">
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="行政违纪情况"
                                     align="center"
                                     min-width="110">
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="排名"
                                     align="center"
                                     min-width="110">
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="积分"
                                     align="center"
                                     min-width="110">
                    </el-table-column>
                    <el-table-column label="操作" align="center" width="90">
                        <template slot-scope="scope" class="btnCell">
                            <el-button @@click="editClick(scope.row)" v-show="!scope.row.edit" class="etitor" type="text">编辑</el-button>
                            <el-button @@click="cancelClick(scope.row)" v-show="scope.row.edit" size="mini" class="miniBtn">取消</el-button>
                            <el-button @@click="saveClick(scope.row)" v-show="scope.row.edit" size="mini" class="miniBtn" type="primary" plain>保存</el-button>
                        </template>
                    </el-table-column>
                </el-table>
            </el-row>
        </el-main>
    </el-container>
    @*底部*@
    <div class="bottomInfo">
        <el-form class="bt_flex">
            <el-form-item label="确认:">
                <el-select v-model="value" placeholder="请选择">
                    <el-option v-for="item in options"
                               :key="item.value"
                               :label="item.label"
                               :value="item.value">
                    </el-option>
                </el-select>
                <el-button type="primary" size="medium">确认</el-button>
            </el-form-item>
        </el-form>
        <el-form class="bt_flex1">
            <el-form-item label="审核:">
                <el-select v-model="value" placeholder="请选择">
                    <el-option v-for="item in options"
                               :key="item.value"
                               :label="item.label"
                               :value="item.value">
                    </el-option>
                </el-select>
                <el-button type="primary" size="medium">确认</el-button>
            </el-form-item>
        </el-form>
        <el-form class="bt_flex1">
            <el-form-item label="核准:">
                <el-select v-model="value" placeholder="请选择">
                    <el-option v-for="item in options"
                               :key="item.value"
                               :label="item.label"
                               :value="item.value">
                    </el-option>
                </el-select>
                <el-button type="primary" size="medium">确认</el-button>
            </el-form-item>
        </el-form>
    </div>
</div>
