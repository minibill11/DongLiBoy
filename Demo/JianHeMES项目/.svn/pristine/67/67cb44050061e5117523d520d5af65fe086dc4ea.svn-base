@{
    ViewBag.Title = "KPI班组评优汇总表";
}
@* css放置处 *@
@section cssStyle {
    <style>
        .el-progress-bar__inner::after, .el-row::after, .el-row::before, .el-slider::after, .el-slider::before, .el-slider__button-wrapper::after, .el-upload-cover::after {
            content: none;
        }
        /*统一表格字体*/
        .el-table thead {
            font-family: '微软雅黑 Bold', '微软雅黑';
            font-weight: 700;
            font-style: normal;
            color: #5E5E5E;
            height: 34px;
            background-color: rgba(228, 236, 248, 1);
        }

        .el-table th > .cell {
            font-size: 14px;
        }

        .top_title {
            font-size: 24px;
            color: #293038;
            text-align: left;
        }

        .rightMain .rightMainContent {
            padding-top: 30px;
        }

        .table_top {
            display: flex;
            justify-content: space-between;
            margin-bottom: 15px;
            height: 40px;
        }

        .m_left {
            width: 200px !important;
            height: 100%;
        }

        .tips_text {
            font-family: '微软雅黑';
            font-weight: 400;
            font-style: normal;
            font-size: 14px;
            color: #293038;
            text-align: left;
            margin-right: 10px;
        }

        .el-input--medium .el-input__inner {
            height: 40px;
            line-height: 40px;
        }

        .el-button--medium {
            padding: 12px 10px;
        }

        .top_right {
            height: 40px;
            position: fixed;
            right: 60px;
            z-index: 1000;
        }

        .summary {
            width: 110px;
            margin-right: 5px;
        }

        .summary, .derivedForm {
            height: 40px;
        }

            .derivedForm .el-button--primary.is-plain {
                width: 123px;
                border-color: rgba(140, 223, 203, 1);
                background-color: rgba(230, 248, 234, 1);
                font-size: 14px;
                color: #2BCFAA;
            }

            .derivedForm .el-button--medium:hover {
                background-color: rgba(43, 207, 170, 1);
                color: #fff;
            }

        .bottomInfo {
            padding: 20px 30px;
            display: flex;
            justify-content: space-around;
            align-items: center;
        }

            .bottomInfo .el-form-item__content {
                display: flex;
                align-items: center;
            }

            .bottomInfo .el-input__inner {
                width: 160px;
                height: 32px;
            }

            .bottomInfo .el-form-item__label {
                font-family: 微软雅黑;
                font-weight: 400;
                font-style: normal;
                font-size: 16px;
            }

            .bottomInfo .el-button--primary {
                width: 60px;
                height: 32px;
                line-height: 0;
                background-color: #22C49E;
                border-color: #22C49E;
                margin-left: 10px;
            }
    </style>
}

<div id="app" v-cloak>
    <el-container>
        <el-header>
            <span class="top_title">KPI班组评优汇总表</span>
        </el-header>
        <el-main>
            @*日期选择框 *@
            <el-row class="table_top">
                <div class="top_left">
                    <el-date-picker class="m_left"
                                    type="month"
                                    size="medium"
                                    style="width:150px;"
                                    placeholder="请选择日期">
                    </el-date-picker>
                    <span class="tips_text">月度汇总</span>
                    <el-button class="btnIfo" type="primary" size="medium">查询</el-button>
                </div>
                <div class="top_right">
                    <a href="###" class="summary"><el-button type="primary" size="medium" plain>效率指标汇总表</el-button></a>
                    <a href="###" class="summary"><el-button type="primary" size="medium" plain>品质指标汇总表</el-button></a>
                    <a href="DeparturesNum_Index" class="summary"><el-button type="primary" size="medium" plain>班组人员流失汇总表</el-button></a>
                    <a href="KPI_7S_Summarizing" class="summary"><el-button type="primary" size="medium" plain>7S评比得分汇总表</el-button></a>
                    <a href="###" class="derivedForm"><el-button type="primary" size="medium" plain>导出EXCEL表</el-button></a>
                </div>
            </el-row>
            <el-row class="text-center">
                <el-table min-height="234"
                          max-height="560"
                          size="small"
                          align="center"
                          cell-class-name="cellParent"
                          stripe
                          border>
                    <el-table-column type="index"
                                     label="序号"
                                     align="center">
                        <template slot-scope="scope">
                            <span>{{scope.$index+1}}</span>
                        </template>
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="部门"
                                     align="center"
                                     min-width="78">
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="班组 "
                                     align="center"
                                     min-width="78">
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="效率指标"
                                     align="center"
                                     min-width="110">
                        <el-table-column prop="CustomerName"
                                         label="考核指标名称"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="权重"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="实际得分"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="得分小计"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="单项得分"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="品质指标"
                                     align="center"
                                     min-width="110">
                        <el-table-column prop="CustomerName"
                                         label="考核指标名称"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="权重"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="实际得分"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="得分小计"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="单项得分"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                    </el-table-column>
                    <el-table-column prop="CustomerName"
                                     label="7S评比指标"
                                     align="center"
                                     min-width="110">
                        <el-table-column prop="CustomerName"
                                         label="考核指标名称"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="权重"
                                         align="center"
                                         min-width="110">
                        </el-table-column>
                        <el-table-column prop="CustomerName"
                                         label="实际得分"
