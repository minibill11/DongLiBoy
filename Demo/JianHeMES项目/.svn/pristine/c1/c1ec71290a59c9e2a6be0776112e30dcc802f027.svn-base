<div class="el-form-item el-form-item--medium" style="display:none;">
    <label class="el-form-item__label" style="width: 25%;">是否异常</label>
    <div class="el-form-item__content" style="margin-left: 25%;">
        <el-switch v-model="IsAbnormal"
                   size="medium"
                   active-text="异常"
                   inactive-text="正常">
        </el-switch>
    </div>
</div>
<div class="el-form-item el-form-item--medium" v-show="IsAbnormal">
    <label class="el-form-item__label" style="width: 25%;">异常列表</label>
    <div class="el-form-item__content" style="margin-left: 25%;">
        <ul class="abnormalUlLi">
            <li v-for="i in AbnormalResult">{{i}}</li>
        </ul>
    </div>
</div>
<div class="el-form-item el-form-item--medium">
    <label class="el-form-item__label" style="width: 25%;">备注</label>
    <div class="el-form-item__content" style="margin-left: 25%;">
        <el-input type="textarea"
                  :rows="2"
                  size="medium"
                  placeholder="请输入备注内容"
                  v-model="Remark">
        </el-input>
    </div>
</div>
<input type="password" name="password" autocomplete="new-password" style="display:none;" />