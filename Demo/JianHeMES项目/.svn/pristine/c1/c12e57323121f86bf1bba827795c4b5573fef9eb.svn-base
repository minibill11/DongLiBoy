@{
    ViewBag.Title = "版本管理";
}
@* css放置处 *@
@section cssStyle {
    <link rel="stylesheet/less" type="text/css" href="~/Content/VersionManagement/VersionManagement.less" />
}

<el-container id="app" v-cloak>
    <el-header>
        <h1 class="title">{{show?'MES版本管理':'MES版本录入'}}</h1>
    </el-header>
    <el-main>
        <version-management-component v-show="show" v-model="show" :editlimit="editlimit"></version-management-component>
        <version-management-input-component v-show="!show" v-model="show" :editlimit="editlimit"></version-management-input-component>
    </el-main>
</el-container>
@* 分部页放置处 *@
@section renderPage {
    @RenderPage("~/Views/Warehouse_Material/_wareEdit.cshtml")
    @RenderPage("_Index.cshtml")
    @RenderPage("_Index_Input.cshtml")
}
@* js放置处 *@
@section jsScript {
    <script>
        const app = {
            data: function () {
                return {
                    show: true,
                    editlimit: false,//编辑版本权限
                }
            },
            created() {
                this.editlimit = this.islimit('MES版本修改');
            }
        };
    </script>
}

