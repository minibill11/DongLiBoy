@{
    ViewBag.Title = "工序产能首页";
}

@*  <summary>
    1.工序产能首页
    </summary>*@

<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />@*2.13版本 elementui css文件*@
<script src="~/Content/styleFile/packaging/index.js"></script>@*2.13版本 elementui js文件*@
<link href="~/Content/styleFile/processCapacity/indexStyle.css" rel="stylesheet" />@*工序产能公共样式文件*@
<style>
    /* 宽度设为屏幕宽度的96% */
    .container, .body-content {
        width: 96vw;
    }

    .el-table tbody tr td {
        padding: 0;
    }

    .fontsizelie {
        font-size: 16px;
    }

    .ulli {
        margin: 0;
        line-height: 16px;
        text-align: left;
        padding-left: 15px;
    }

        .ulli li {
            margin-bottom: 5px;
        }

    .el-table__expanded-cell[class*="cell"] {
        padding: 10px 20px;
        background-color: #fff4f4;
    }

    .el-table__body tr:hover > td[class="el-table__expanded-cell"] {
        background-color: #fff4f4 !important;
    }

    .el-table__body tr:hover > td {
        background-color: #30BCF242 !important;
    }

    .areaDiv {
        margin: 5px auto;
        width: 1000px;
    }

        .areaDiv .el-input, .areaDiv el-button, .areaDiv > div {
            display: inline-block;
            max-width: 180px;
        }

    .shebeiDiv {
        height: 28px;
        line-height: 28px;
    }

    .shebeiDivNew {
        display: inline-block;
    }

    .el-divider--vertical {
        margin: 0;
        width: 2px;
        height: 28px;
        background-color: #d5d7db;
        vertical-align: initial;
    }

    .danwei {
        font-size: 12px;
        height: 12px;
        line-height: 12px;
    }

    .jieguo {
        font-size: 19px;
        color: #1539f7;
        max-width: 500px;
        padding-left: 10px;
    }

    .el-table .smtbgcolor {
        background: #f9f9eb;
    }

    .el-table .mzbgcolor {
        background: #fff4f4;
    }

    .el-table .mkbgcolor {
        background: #f0f9eb;
    }

    .detailTable {
        border: 1px solid #d5d7db;
        border-collapse: unset;
    }

        .detailTable > thead > tr > td,
        .detailTable > tbody > tr > td,
        .detailTable > tfoot > tr > td {
            text-align: center;
            vertical-align: middle;
            border: 1px solid #d5d7db;
        }

            .detailTable > thead > tr > td > .cell,
            .detailTable > tbody > tr > td > .cell,
            .detailTable > tfoot > tr > td > .cell {
                height: 28px;
                line-height: 28px;
                text-align: center;
            }

    .shebeiDivNew > .cell {
        width: 60px;
        height: 28px;
        line-height: 28px;
        text-align: center;
    }

    .detailTable > thead > tr > td,
    .detailTable > tfoot > tr > td {
        font-weight: bold;
        height: 35px;
    }

    .trParent > tr > td {
        min-width: 70px;
    }

    .trParent tr td:first-child {
        width: 120px;
    }

    .bpstyle {
        background-color: #ededed !important;
    }

    .sbstyle {
        background-color: #ebf4f9 !important;
    }

    .phbsstyle {
        background-color: #fcfba3 !important;
    }

    .phstyle {
        background-color: #ffefda !important;
    }

    .trBottom {
        border-bottom: 2px solid #d5d7db;
    }

    .phstyle, .phbsstyle {
        font-weight: bold;
    }


    .detailTable > thead > tr > td:nth-child(1),
    .detailTable > thead > tr > td:nth-child(3),
    .detailTable > thead > tr > td:nth-last-child(5),
    .detailTable > tbody > tr > td:nth-child(1),
    .detailTable > tbody > tr > td:nth-child(3),
    .detailTable > tbody > tr > td:nth-last-child(5) {
        border-right: 1px solid black;
    }

    .finalTable > thead > tr > td:not(:last-child),
    .finalTable > tfoot > tr > td:not(:last-child) {
        border-right: 1px solid black;
    }

    .flexDiv {
        display: flex;
        justify-content: center;
    }
</style>
<div id="app" v-cloak>
    <el-container>
        <el-header class="text-center">
            @*标题*@
            <h2>@ViewBag.Title</h2>
        </el-header>
        <el-main v-loading="loading" style="min-height:600px">
            <el-row class="text-center">
                <el-form :inline="true" :model="queryTable" size="small">
                    <el-form-item label="筛选平台">
                        <select-input v-model.trim="queryTable.proplatform" :disabled="false" @@watchval="watchPlatform($event)" :appendbody="false" :options="options.proplatform" :isfocus="true" @*:ismultiple="true"*@ size="medium" :allowcreate="true"></select-input>
                    </el-form-item>
                    <el-form-item label="筛选型号">
                        <select-input v-model.trim="queryTable.protype" :disabled="false" @@watchval="watchType($event)" :appendbody="false" :options="options.protype" :isfocus="true" @*:ismultiple="true"*@ size="medium" :allowcreate="true"></select-input>
                    </el-form-item>
                    <el-button type="primary" @@click="onQuerySubmit" size="medium">查找</el-button>
                </el-form>
            </el-row>
            <el-row class="text-center">
                <el-table :data="filterResult"
                          max-height="700"
                          size="medium"
                          align="center"
                          cell-class-name="cellParent"
                          stripe
                          ref="thistable"
                          border>
                    <el-table-column type="expand" width="1">
                        <template slot-scope="props">
                            <div v-if="props.row.info!=''">
                                @*<div>PCB编号：<b>{{ props.row.PCB }}</b></div>*@
                                <div class="areaDiv">
                                    <el-switch v-model="detailshow"
                                               active-text="最终"
                                               inactive-text="详细">
                                    </el-switch>
                                    <div>
                                        <span class="danwei">模块瓶颈参考值</span><br />
                                        <el-select v-model="Reference.value" placeholder="请选择" @@change="selectCapHouse(props.row,$event)">
                                            <el-option v-for="(item,index) in Reference.options"
                                                       :key="index"
                                                       :label="item.name"
                                                       :value="index">
                                            </el-option>
                                        </el-select>
                                    </div>
                                    <div>
                                        <span class="danwei">单位：㎡(平方米)</span><br />
                                        <el-input v-model="props.row.area"
                                                  ref="computeInput"
                                                  v-on:keyup.enter.native="areaCompute(props.row,props.row.area)"
                                                  placeholder="请输入面积"></el-input>
                                    </div>
                                    <div>
                                        <span></span><br />
                                        <el-button type="success" :loading="props.row.btnloading" @@click="areaCompute(props.row,props.row.area)">计 算</el-button>
                                    </div>
                                    <span class="jieguo">{{props.row.info.resultMes}}</span>
                                </div>
                                <template v-if="detailList.data">
                                    <table v-if="detailshow" class="el-table el-table--border detailTable finalTable">
                                        <thead class="el-table__header-wrapper trParent">
                                            <tr class="">
                                                <td>工段</td>
                                                <td v-bind:colspan="finalDetail.smtlength">SMT段工序产能(设备:线)</td>
                                                <td v-bind:colspan="finalDetail.mklength">模块段工序产能</td>
                                                <td v-bind:colspan="finalDetail.mzlength">模组段工序产能(设备:线)</td>
                                            </tr>
                                        </thead>
                                        <tbody class="el-table__body-wrapper trParent">
                                            <tr class="">
                                                <td>工位</td>
                                                <td v-for="(item,index) in detailList.name">
                                                    <template v-if="detailList.equipmentName[index].length>1">
                                                        <div class="cell">{{item}}</div>
                                                        <el-divider></el-divider>
                                                        <div class="flexDiv">
                                                            <template v-for="(i,idx) in detailList.equipmentName[index]">
                                                                <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                                <el-divider v-if="idx<(detailList.equipmentName[index].length-1)" direction="vertical"></el-divider>
                                                            </template>
                                                        </div>
                                                    </template>
                                                    <template v-else>
                                                        <div class="cell">{{item}}</div>
                                                    </template>
                                                </td>
                                            </tr>
                                            <tr class="">
                                                <td>人力</td>
                                                <td v-for="(item,index) in detailList.balancePerson">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="">
                                                <td>设备(线)</td>
                                                <td v-for="(item,index) in detailList.balanceEquipmentNum">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">{{detailList.balanceMultiple[index][0] || 1}}</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                        </tbody>
                                        <tfoot class="el-table__header-wrapper trParent">
                                            <tr class="">
                                                <td>产能</td>
                                                <td v-bind:colspan="finalDetail.smtlength">
                                                    <u>{{finalDetail.smtmincapacity}} PCS</u> / {{finalDetail.smtpersoncount}}人 / 小时
                                                </td>
                                                <td v-bind:colspan="finalDetail.mklength">
                                                    <u>{{finalDetail.mkmincapacity}} PCS</u> / {{finalDetail.mkpersoncount}}人 / 小时
                                                </td>
                                                @* 模组合并产能后的数据，累加 *@
                                                @*<td v-bind:colspan="finalDetail.mzlength">
                                                        <u>{{finalDetail.mzcapacitycount.toFixed(2)}} PCS</u> / {{finalDetail.mzpersoncount}}人 / 小时
                                                    </td>*@
                                                <td v-for="(item,index) in finalDetail.mzlength" style="border-right:1px solid #d5d7db;">
                                                    <u>{{finalDetail.mzarr[index].capacity.toFixed(2)}} PCS</u> <br />/ {{finalDetail.mzarr[index].person}}人 / 小时
                                                </td>
                                            </tr>
                                        </tfoot>
                                    </table>

                                    <table v-else @*v-if="Object.keys(detailList).length!=0"*@ class="el-table el-table--border detailTable">
                                        <thead class="el-table__header-wrapper trParent">
                                            <tr>
                                                <td>详细表</td>
                                                <td v-for="(item,index) in detailList.name">
                                                    <div class="cell">{{item}}</div>
                                                </td>
                                            </tr>
                                        </thead>
                                        <tbody class="el-table__body-wrapper trParent">
                                            @*<tr>
                                                    <td>每小时产能</td>
                                                    <td v-for="(item,index) in detailList.capacityPerHour"><div class="cell">{{item}}</div></td>
                                                </tr>
                                                <tr>
                                                    <td>模组需求数</td>
                                                    <td v-for="(item,index) in detailList.moduleNeed"><div class="cell">{{item}}</div></td>
                                                </tr>
                                                <tr class="bpstyle">
                                                    <td>标配人数</td>
                                                    <td v-for="(item,index) in detailList.person"><div class="cell">{{item}}</div></td>
                                                </tr>
                                                <tr class="bpstyle">
                                                    <td>标准产能</td>
                                                    <td v-for="(item,index) in detailList.capacity"><div class="cell">{{item}}</div></td>
                                                </tr>*@
                                            <tr>
                                                <td>模组需求数</td>
                                                <td v-for="(item,index) in detailList.moduleNeed"><div class="cell">{{item}}</div></td>
                                            </tr>
                                            <tr class="sbstyle">
                                                <td>设备名字</td>
                                                <td v-for="(item,index) in detailList.equipmentName">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="sbstyle">
                                                <td>设备数量</td>
                                                <td v-for="(item,index) in detailList.equipmentNum">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="sbstyle">
                                                <td>产能(PCS/H)</td>
                                                <td v-for="(item,index) in detailList.equipmentCapacity">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="sbstyle">
                                                <td><span style="font-size:12px;">一人能控制的机台数</span></td>
                                                <td v-for="(item,index) in detailList.controllerNum">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="sbstyle">
                                                <td>人数</td>
                                                <td v-for="(item,index) in detailList.person">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="phbsstyle">
                                                <td>平衡后倍数</td>
                                                <td v-for="(item,index) in detailList.balanceMultiple">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="phstyle">
                                                <td>平衡后产能</td>
                                                <td v-for="(item,index) in detailList.balanceCapacity">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="phstyle">
                                                <td>平衡后人数</td>
                                                <td v-for="(item,index) in detailList.balancePerson">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="phstyle">
                                                <td><span style="font-size:13px;">平衡后设备需求数</span></td>
                                                <td v-for="(item,index) in detailList.balanceEquipmentNum">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="phstyle trBottom">
                                                <td>平衡率</td>
                                                <td v-for="(item,index) in detailList.balanceRate">
                                                    <template v-if="item==''">
                                                        <div class="shebeiDivNew"><div class="cell">/</div></div>
                                                    </template>
                                                    <div v-else class="flexDiv">
                                                        <template v-for="(i,idx) in item">
                                                            <div class="shebeiDivNew"><div class="cell">{{i}}</div></div>
                                                            <el-divider v-if="idx<(item.length-1)" direction="vertical"></el-divider>
                                                        </template>
                                                    </div>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </template>
                                <template v-else>
                                    <table class="el-table el-table--border detailTable">
                                        <thead class="el-table__header-wrapper trParent">
                                            <tr>
                                                <td>暂无数据</td>
                                            </tr>
                                        </thead>
                                    </table>
                                </template>
                            </div>
                            <div v-else class="text-center">暂无数据</div>
                        </template>
                    </el-table-column>
                    <el-table-column type="index" label="序号" width="40">
                        <template slot-scope="scope">
                            <span style="font-size:16px;">{{scope.$index+1}}</span>
                        </template>
                    </el-table-column>
                    <el-table-column prop="platfrom"
                                     min-width="80"
                                     @*fixed*@
                                     sortable
                                     label="平台">
                        <template slot-scope="scope">
                            @*<el-link :href="`/Process_Capacity/index2?Type=${scope.row.type}&ProductPCBnumber=${scope.row.PCB}&Platform=${scope.row.platfrom}`"
                                target="_blank"
                                class="fontsizelie"
                                style="color:#409EFF"
                                :underline="false">{{scope.row.platfrom}}</el-link>*@
                            <span class="fontsizelie">{{scope.row.platfrom}}</span>
                        </template>
                    </el-table-column>
                    <el-table-column prop="type"
                                     sortable
                                     min-width="80"
                                     label="型号">
                        <template slot-scope="scope">
                            <span class="fontsizelie">{{scope.row.type}}</span>
                        </template>
                    </el-table-column>
                    <el-table-column prop="PlatformModul"
                                     label="平台模块"
                                     min-width="100">
                        <template slot-scope="scope">
                            <div v-if="scope.row.platformModule.length>0">
                                <div v-if="scope.row.platformModule.length>1">
                                    <el-select v-model="scope.row.ptmkVal" @@change="ptmkselect($event,scope.row)" placeholder="请选择">
                                        <el-option v-for="item in scope.row.platformModule"
                                                   :key="item.PlatformModul"
                                                   :label="item.PlatformModul"
                                                   :value="item.PlatformModul">
                                        </el-option>
                                    </el-select>
                                </div>
                                <div v-else class="fontsizelie">{{scope.row.ptmkVal}}</div>
                            </div>
                        </template>
                    </el-table-column>
                    <el-table-column prop="Maintenance"
                                     label="维护方式"
                                     min-width="100">
                        <template slot-scope="scope">
                            <div v-if="scope.row.platformModule.length>0">
                                <div v-if="scope.row.platformModule[scope.row.ptmkIndex].Maintenance.length>1">
                                    <el-select v-model="scope.row.whVal" @@change="whselect(scope.row)" placeholder="请选择">
                                        <el-option v-for="item in scope.row.platformModule[scope.row.ptmkIndex].Maintenance"
                                                   :key="item"
                                                   :label="item"
                                                   :value="item">
                                        </el-option>
                                    </el-select>
                                </div>
                                <div v-else class="fontsizelie">{{scope.row.whVal}}</div>
                            </div>
                        </template>
                    </el-table-column>
                    <el-table-column label="模组单位"
                                     min-width="65"
                                     prop="ModuleUnits">
                        <template slot-scope="scope">
                            <div v-if="scope.row.platformModule.length>0">
                                {{scope.row.platformModule[scope.row.ptmkIndex].ModuleUnits}}
                            </div>
                        </template>
                    </el-table-column>
                    <el-table-column label="模块"
                                     min-width="140">
                        <template slot-scope="scope">
                            <div v-if="scope.row.sgOption.length>1">
                                <el-select v-model="scope.row.sgID" @@change="sgselect(scope.row)" placeholder="请选择">
                                    <el-option v-for="item in scope.row.sgOption"
                                               :key="item.threeid+item.guleid"
                                               :label="sglabel(item.threename,item.gulename)"
                                               :value="item.threeid+'+'+item.guleid">
                                    </el-option>
                                </el-select>
                            </div>
                            <div v-else>
                                <div v-if="sglabel(scope.row.sgVal.split('+')[0],scope.row.sgVal.split('+')[1]).split('+').length>1">
                                    <ul class="ulli">
                                        <li v-for="i in sglabel(scope.row.sgVal.split('+')[0],scope.row.sgVal.split('+')[1]).split('+')">
                                            {{i}}
                                        </li>
                                    </ul>
                                </div>
