        const app = {
            data: function () {
                return {
                    dateRange: null,
                    tableList: [{}]
                }
            },
            created() {
            },
            mounted() {
            },
            methods: {
                DisplayVersion() {
                    axios.post('/VersionManagement/DisplayVersion').then(res => {
                        console.log(res.data);
                    }).catch(err => {
                    });
                },
            }
        };
    </script>
}

