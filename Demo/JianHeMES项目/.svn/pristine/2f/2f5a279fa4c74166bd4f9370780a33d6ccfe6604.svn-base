@{
    ViewBag.Title = "MES版本录入";
}
@* css放置处 *@
@section cssStyle {
    <link rel="stylesheet/less" type="text/css" href="~/Content/VersionManagement/VersionManagement.less" />
}
<el-container id="app" v-cloak>
    <el-header>
        <el-row class="header-title"><h1>@ViewBag.Title</h1></el-row>
        <el-row class="header-input">
            <a href="Version_Management_Index"><el-button type="primary" size="small" plain>版本管理</el-button></a>
        </el-row>
    </el-header>
    <el-main>
        <el-table :data="tableList"
                  @*row-class-name="zero-padding"*@
                  max-height="600"
                  size="small"
                  stripe
                  border>
            <el-table-column type="index" label="序号"></el-table-column>
            <el-table-column prop="" label="版本号">
                <el-input></el-input>
            </el-table-column>
            <el-table-column prop="" label="模块">
                <el-select @*v-model=""*@ placeholder="请选择" clearable>
                    <el-option label="工序产能" value="工序产能"></el-option>
                    <el-option label="工序产能" value="工序产能"></el-option>
                    <el-option label="工序产能" value="工序产能"></el-option>
                    <el-option label="工序产能" value="工序产能"></el-option>
                    <el-option label="工序产能" value="工序产能"></el-option>
                </el-select>
            </el-table-column>
            <el-table-column prop="" label="更新内容">
                <el-input type="textarea"
                          :autosize="{ minRows: 2, maxRows: 4}"
                          placeholder="请输入内容">
                </el-input>
            </el-table-column>
            <el-table-column prop="" label="更新日期">
                <template slot-scope="scope">
                    <el-date-picker type="date"
                                    placeholder="选择日期">
                </template>
            </el-table-column>
            <el-table-column prop="" label="SVN版本号">
                <template slot-scope="scope">
                    <el-input></el-input>
                </template>
            </el-table-column>
        </el-table>
    </el-main>
    <el-footer>
        <el-button type="primary" size="small">录入</el-button>
        <el-button size="small">取消</el-button>
    </el-footer>
</el-container>
@* 分部页放置处 *@
@section renderPage {
}
