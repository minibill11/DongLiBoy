
<el-table :data="tableList"
          max-height="600"
          size="mini "
          align="center"
          ref="filterTable"
          cell-class-name="cellParent"
          stripe
          border>
    <el-table-column type="index" label="序号" width="40">
    </el-table-column>
    <el-table-column prop="Platform"
                     width="82"
                     @*fixed*@
                     :filters="filterObj.Platform"
                     :filter-method="filterHandler"
                     sortable
                     label="平台">
        <template slot-scope="scope">
            <el-link :href="`/Process_Capacity/Detail?Type=${scope.row.Type}&ProductPCBnumber=${scope.row.ProductPCBnumber}&Platform=${scope.row.Platform}`"
                     target="_blank"
                     style="color:#409EFF"
                     :underline="false">{{scope.row.Platform}}</el-link>
        </template>
    </el-table-column>
    <el-table-column prop="Type"
                     width="82"
                     :filters="filterObj.Type"
                     :filter-method="filterHandler"
                     sortable
                     label="型号">
    </el-table-column>
    <el-table-column prop="ProductPCBnumber"
                     width="85"
                     :filters="filterObj.ProductPCBnumber"
                     :filter-method="filterHandler"
                     sortable
                     label="PCB">
    </el-table-column>
    <el-table-column prop="SMT"
                     width="334"
                     label="SMT">
        <template slot-scope="scope">
            <div class="el-table__fixed-header-wrapper">
                <table class="el-table__header" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <thead class="has-gutter">
                        <tr class="">
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标配人数</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标准产能</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">平衡系数</div></th>
                        </tr>
                    </thead>
                </table>
            </div>
            <div class="el-table__fixed-body-wrapper is-scrolling-none">
                <table class="el-table__body" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <tbody>
                        <tr class="el-table__row" v-for="item in scope.row.SMT">
                            <td rowspan="1" colspan="1"><div class="cell">{{item.name}}</div></td>
                            <td rowspan="1" colspan="4">
                                <div class="cell">
                                    <el-table :data="item.content"
                                              size="mini"
                                              width="280"
                                              :show-header="false"
                                              stripe
                                              border>
                                        <el-table-column prop="seaction"
                                                         width="70"
                                                         label="工段">
                                            <template slot-scope="scope">
                                                {{scope.row.seaction}}
                                            </template>
                                        </el-table-column>
                                        <el-table-column prop="StandardTotal"
                                                         width="70"
                                                         label="标配人数">
                                        </el-table-column>
                                        <el-table-column prop="StandardOutput"
                                                         width="70"
                                                         label="标准产能">
                                        </el-table-column>
                                        <el-table-column prop="BalanceRate"
                                                         width="70"
                                                         label="平衡系数">
                                        </el-table-column>
                                    </el-table>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
    </el-table-column>
    <el-table-column prop="module"
                     width="334"
                     label="模块">
        <template slot-scope="scope">
            <div class="el-table__header-wrapper">
                <table class="el-table__header" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <thead class="has-gutter">
                        <tr class="">
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标配人数</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标准产能</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">平衡系数</div></th>
                        </tr>
                    </thead>
