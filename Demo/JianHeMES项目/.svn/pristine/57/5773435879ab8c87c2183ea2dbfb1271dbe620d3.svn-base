            </div>
            <div class="el-table__body-wrapper is-scrolling-none">
                <table class="el-table__body" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <tbody>
                        <tr class="el-table__row" v-for="item in scope.row.module">
                            <td rowspan="1" colspan="1"><div class="cell">{{item.name}}</div></td>
                            <td rowspan="1" colspan="4">
                                <div class="cell">
                                    <el-table :data="item.content"
                                              size="mini"
                                              width="280"
                                              :show-header="false"
                                              stripe
                                              border>
                                        <el-table-column prop="seaction"
                                                         width="70"
                                                         label="工段">
                                            <template slot-scope="scope">
                                                {{scope.row.seaction}}
                                            </template>
                                        </el-table-column>
                                        <el-table-column prop="StandardTotal"
                                                         width="70"
                                                         label="标配人数">
                                        </el-table-column>
                                        <el-table-column prop="StandardOutput"
                                                         width="70"
                                                         label="标准产能">
                                        </el-table-column>
                                        <el-table-column prop="BalanceRate"
                                                         width="70"
                                                         label="平衡系数">
                                        </el-table-column>
                                    </el-table>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
    </el-table-column>
    <el-table-column prop="packing"
                     width="334"
                     label="组装">
        <template slot-scope="scope">
            <div class="el-table__fixed-header-wrapper">
                <table class="el-table__header" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <thead class="has-gutter">
                        <tr class="">
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">工段</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标配人数</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">标准产能</div></th>
                            <th colspan="1" rowspan="1" class="is-leaf"><div class="cell">平衡系数</div></th>
                        </tr>
                    </thead>
                </table>
            </div>
            <div class="el-table__fixed-body-wrapper is-scrolling-none">
                <table class="el-table__body" style="width: 332px;" cellspacing="0" cellpadding="0" border="0">
                    <colgroup>
                        <col width="50">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                        <col width="70">
                    </colgroup>
                    <tbody>
                        <tr class="el-table__row" v-for="item in scope.row.packing">
                            <td rowspan="1" colspan="1"><div class="cell">{{item.name}}</div></td>
                            <td rowspan="1" colspan="4">
                                <div class="cell">
                                    <el-table :data="item.content"
                                              size="mini"
                                              width="280"
                                              :show-header="false"
                                              stripe
                                              border>
                                        <el-table-column prop="seaction"
                                                         width="70"
                                                         label="工段">
                                            <template slot-scope="scope">
                                                {{scope.row.seaction}}
                                            </template>
                                        </el-table-column>
                                        <el-table-column prop="StandardTotal"
                                                         width="70"
                                                         label="标配人数">
                                        </el-table-column>
                                        <el-table-column prop="StandardOutput"
                                                         width="70"
                                                         label="标准产能">
                                        </el-table-column>
                                        <el-table-column prop="BalanceRate"
                                                         width="70"
                                                         label="平衡系数">
                                        </el-table-column>
                                    </el-table>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
    </el-table-column>
    <el-table-column label="平衡系数图表" width="50">
        <template slot-scope="scope">
            <chart-div :urldata="{Type:scope.row.Type,ProductPCBnumber:scope.row.ProductPCBnumber,Platform:scope.row.Platform}" :ispopover="true" :id="scope.$index"></chart-div>
        </template>
    </el-table-column>
    <el-table-column label="汇总">
        <el-table-column prop="modeleNeedHoure"
                         label="单人生产每模组需求总工时(H)">
        </el-table-column>
        <el-table-column prop="totalperson"
                         label="总人数(人)">
        </el-table-column>
        <el-table-column prop="capacityperhour"
                         label="每小时产能(模组/H)">
        </el-table-column>
    </el-table-column>
</el-table>
