<script>
    var app = new Vue({
        el: "#app",
        data: {
            loading: false,
            tableList: [],
        },
        created: function () {

        },
        mounted: function () {
            this.loading = true;
            for (let i = 1; i <= 50; i++) {
                this.tableList.push({
                    testData: "测试数据",
                    testNum: (Math.random() * 1000).toFixed(0)
                });
            };
            this.loading = false;
        },
        methods: {

        }
    });
</script>