                    flag = true
                }
            }
        }
        return flag
    }
    //IC单引脚计算公式
    function f1() {
        let V = app.Vvalue,
            R = app.Rvalue,
            N = app.Nvalue,
            G = app.Gvalue,
            L = app.Lvalue ,
            result = app.result,
            Z = app.Zvalue,
            F = app.Fvalue;
        for (let i in V) {
            if (V[i] != "" && R[i] != "" && N[i] != "" && G[i] != "" && L[i] != "") {
                var m = ((V[i] / R[i]) * N[i] * G[i] * L[i] * 1000).toFixed(2);
                Vue.set(app.result, i, isNaN(m) ? "" : m);
                if (result[i] != "") {
                    Z[i] = (result[i] * 1.1).toFixed(2);
                    F[i] = (result[i] * 0.9).toFixed(2);
                }
            } else {
                Vue.set(app.result, i, "");
                    Z[i] = "";
                    F[i] = "";
            }
        }
    }
