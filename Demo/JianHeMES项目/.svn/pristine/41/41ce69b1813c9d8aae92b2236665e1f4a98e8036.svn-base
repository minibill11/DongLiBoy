
@{
    ViewBag.Title = "小样报告参数";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<script src="~/Scripts/axios.min.js"></script>
<style>
    * {
        margin: 0;
        padding: 0;
    }

    .body-content, .container {
        padding-left: 0px;
        padding-right: 0px;
    }
    .container {
        width: 1300px;
    }
    .table-container {
        margin-top: 5px;
    }

    .table-container0 {
        margin-top: 5px;
    }

    .table-container1 {
        margin-top: 5px;
    }

    .tb {
        display: inline-flex;
        margin-bottom: 5px;
        width:100%
    }

    .table .cell, .el-table th, .el-table td {
        text-align: center;
        padding: 4px;
    }

    .el-form-item {
        margin-bottom: 10px;
    }

    .corparationBtn {
        margin: 0;
        padding: 5px;
        float: left;
        height: 100%;
    }

    .el-dialog__headerbtn .el-dialog__close {
        display: none;
    }

    .el-form-item__content {
        margin-left: 0px !important;
    }

    .aa {
        width: 65px;
        display: inline-block;
        font-size: 14px;
    }

    .f1Div {
        display: inline-block;
    }

        .f1Div p {
            display: inline-block;
            font-size: 13px;
        }

    .dianl {
        margin-left: 15px !important;
    }

    .wuca {
        width: 75px;
        display: inline-block;
        font-size: 14px;
    }

    .pyuansu {
        font-weight: normal !important;
    }

    .one {
        margin-left: 200px;
    }

    .btt {
        display: inline-flex;
    }

    .el-button + .el-button {
        margin-left: 2px;        
    }
</style>
<div id="app" >
    <h3 style="text-align:center;margin-top:10px">小样参数</h3><br />
    <div style="display:inline-flex;margin-bottom:12px" class="one">
        <el-select v-model="icNum" size="small" filterable clearable placeholder="请选择IC型号" v-on:change="" style="margin-right:5px">
            <el-option v-for="item in icOption"
                       v-bind:key="item.value"
                       v-bind:label="item.label"
                       v-bind:value="item.value">
            </el-option>
        </el-select>
        <el-select v-model="mxNum" size="small" filterable clearable placeholder="请选择模块型号" v-on:change="" style="margin-right:5px">
            <el-option v-for="item in mxOption"
                       v-bind:key="item.value"
                       v-bind:label="item.label"
                       v-bind:value="item.value">
            </el-option>
        </el-select>
        <el-select v-model="orderNum" size="small" allow-create filterable clearable placeholder="请选择订单">
            <el-option v-for="item in option"
                       v-bind:key="item.value"
                       v-bind:label="item.label"
                       v-bind:value="item.value">
            </el-option>
        </el-select>
        <el-container>
            <a href="/Small_Sample/Index" style="margin-left:5px;"><el-button size="small" type="primary" plain>返回首页</el-button></a>
            <a href="/Small_Sample/Create" style="margin-left:5px;"><el-button size="small" class="cret" type="primary" plain>新建小样报告</el-button></a>
        </el-container>
    </div>
    <el-dialog title="提示" v-bind:visible.sync="icVisible" width="360px">
        <span slot="title">
            <h3 style="text-align:center;">添加IC型号</h3>
            <el-form label-position="labelPosition" label-width="80px">
                <el-form-item label="IC型号">
                    <el-input v-model="IC_Model" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="IC_ModelMessage">IC型号值只能输入字母和数字</span>
                </el-form-item>
                <el-form-item label="V值">
                    <el-input v-model="V_REXT" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="V_REXTMessage">V值只能输入整数或小数(小数限制4位)</span>
                </el-form-item>
                <el-form-item label="N值">
                    <el-input v-model="N_Value" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="N_ValueMessage">N值只能输入整数或小数(小数限制4位)</span>
                </el-form-item>
                <el-form-item label="G值">
                    <el-input v-model="Gain" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="GainMessage">G值只能输入整数或小数(小数限制4位)</span>
                </el-form-item>
            </el-form>
        </span>
        <span slot="footer" class="dialog-footer">
            <el-button v-on:click="quxiaoIC" size="small">取 消</el-button>
            <el-button type="primary" v-on:click="addIC" size="small" v-bind:disabled="bind">添 加</el-button>
        </span>
    </el-dialog>
    <el-dialog title="提示" v-bind:visible.sync="jjVisible" width="370px">
        <span slot="title">
            <h3 style="text-align:center;">添加PH间距</h3>
            <el-form label-position="labelPosition" label-width="80px">
                <el-form-item label="间距">
                    <el-input v-model="Spacing" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="SpacingMessage">间距值只能输入字母、数字和"-"</span>
                </el-form-item>
                <el-form-item label="最低值">
                    <el-input v-model="Least" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="LeastMessage">最低值只能输入整数或小数(小数限制2位)</span>
                </el-form-item>
                <el-form-item label="最高值">
                    <el-input v-model="Maximum" size="small" style="width:150px;"></el-input>
                    <br /><span style="color:red" v-show="MaximumMessage">最高值只能输入整数或小数(小数限制2位)</span>
                </el-form-item>
            </el-form>
        </span>
        <span slot="footer" class="dialog-footer">
            <el-button v-on:click="quxiaoJj" size="small">取 消</el-button>
            <el-button type="primary" v-on:click="addJj" size="small" v-bind:disabled="bind1">添 加</el-button>
        </span>
    </el-dialog>
    <el-dialog title="提示" v-bind:visible.sync="mxVisible" width="370px">
        <span slot="title">
            <h3 style="text-align:center;">添加模块型号</h3>
            <el-form label-position="labelPosition" label-width="70px">
                <el-form-item label="模块型号" style="display:flex">
                    <el-input v-model="Mo2" size="small" style="width:100px;"></el-input>
                    <span>-</span>
                    <el-input v-model="Mo1" size="small" style="width:100px"></el-input>
                    <br /><span style="color:red" v-show="Mo2Message">模块型号值只能输入字母和数字</span>
                    <br /><span style="color:red" v-show="Mo1Message">第二个框的值只能输入数字</span>
                </el-form-item>
                <el-form-item label="间距">
                    <el-select v-model="Spacing1" size="small" placeholder="请选择">
                        <el-option v-for="item in options"
                                   v-bind:key="item.value"
                                   v-bind:label="item.label"
                                   v-bind:value="item.value">
                        </el-option>
                    </el-select>
                </el-form-item>
        </span>
        <span slot="footer" class="dialog-footer">
            <el-button v-on:click="quxiaoMx" size="small">取 消</el-button>
            <el-button type="primary" v-on:click="addMx" size="small" v-bind:disabled="bind2">添 加</el-button>
        </span>
    </el-dialog>
    <el-dialog title="提示" v-bind:visible.sync="toVisible" width="370px">
        <span slot="title">
            <h3 style="text-align:center;">添加发送人信息</h3>
            <el-form label-position="labelPosition" label-width="80px">
                <el-form-item label="发送人">
                    <el-input v-model="sender" size="small" style="width:150px;"></el-input>
                </el-form-item>
                <el-form-item label="发送地址">
                    <el-input v-model="sendAddress" size="small" style="width:150px;"></el-input>
                    <span>@@lcjh.local</span>
                </el-form-item>
            </el-form>
        </span>
        <span slot="footer" class="dialog-footer">
            <el-button v-on:click="quxiaoTo" size="small">取 消</el-button>
            <el-button type="primary" v-on:click="addTo" size="small">添 加</el-button>
        </span>
    </el-dialog>
    <el-dialog title="提示" v-bind:visible.sync="copyVisible" width="370px">
        <span slot="title">
            <h3 style="text-align:center;">添加抄送人信息</h3>
            <el-form label-position="labelPosition" label-width="80px">
                <el-form-item label="抄送人">
                    <el-input v-model="copyer" size="small" style="width:150px;"></el-input>
                </el-form-item>
                <el-form-item label="抄送地址">
                    <el-input v-model="copyAddress" size="small" style="width:150px;"></el-input>
                    <span>@@lcjh.local</span>
                </el-form-item>
            </el-form>
        </span>
        <span slot="footer" class="dialog-footer">
            <el-button v-on:click="quxiaoCopy" size="small">取 消</el-button>
            <el-button type="primary" v-on:click="addCopy" size="small">添 加</el-button>
        </span>
    </el-dialog>
    <div>
        <span style="margin-left:200px;font-size:15px">IC型号计算参数</span>
        <span style="margin-left:480px;font-size:15px">间距对应参数范围</span>
    </div>
    <div class="tb">
        <el-container>
            <el-aside width="620px">
                <el-table v-bind:data="tableData"
                          border max-height="265"
                          v-bind:row-style="trStyle"
                          v-bind:header-cell-style="{background:'#F0F8FF'}"
                          class="table-container" key='oneTable'>
                    <el-table-column prop="IC_Model"
                                     label="IC型号"
                                     width="117">
                    </el-table-column>
                    <el-table-column prop="V_REXT"
                                     label="V值"
                                     width="85">
                    </el-table-column>
                    <el-table-column prop="N_Value"
                                     label="N值"
                                     width="77">
                    </el-table-column>
                    <el-table-column prop="Gain"
                                     label="G值"
                                     width="75">
                    </el-table-column>
                    <el-table-column prop="Creator"
                                     label="添加者"
                                     width="72">
                    </el-table-column>
                    <el-table-column prop="Modifier"
                                     label="修改者"
                                     width="72">
                    </el-table-column>
                    <el-table-column prop="operation"
                                     label="操作"
                                     width="120" style="text-align:center">
                        <template slot-scope="scope">
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnone" type="primary" v-on:click="edit(scope.$index, scope.row)">编辑</el-button>
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnfour" type="danger" v-on:click="remove(scope.$index, scope.row)">删除</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="primary" v-on:click="postData(scope.$index, scope.row)">保存</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="danger" v-on:click="cancelSave(scope.$index, scope.row)">取消</el-button>
                        </template>
                    </el-table-column>
                </el-table>
                <el-button class="tianjia1" type="primary" plain size="mini" v-on:click="icVisible = true" style="margin:3px;display:none">添加IC</el-button>
            </el-aside>
        </el-container>
        <el-container style="margin-left:6px">
            <el-aside width="524">
                <el-table v-bind:data="tableData1"
                          border max-height="132"
                          v-bind:row-style="trStyle1"
                          v-bind:header-cell-style="{background:'#F0F8FF'}"
                          class="table-container0" key='twoTable'>
                    <el-table-column prop="Spacing"
                                     label="间距"
                                     width="148">
                    </el-table-column>
                    <el-table-column prop="Least"
                                     label="最低值"
                                     width="92">
                    </el-table-column>
                    <el-table-column prop="Maximum"
                                     label="最高值"
                                     width="92">
                    </el-table-column>
                    <el-table-column prop="Creator"
                                     label="添加者"
                                     width="94">
                    </el-table-column>
                    <el-table-column prop="Modifier"
                                     label="修改者"
                                     width="94">
                    </el-table-column>
                    <el-table-column prop="operation1"
                                     label="操作"
                                     width="143">
                        <template slot-scope="scope" style="text-align:center justify">
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnone" type="primary" v-on:click="edit1(scope.$index, scope.row)">编辑</el-button>
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnfour" type="danger" v-on:click="remove1(scope.$index, scope.row)">删除</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="primary" v-on:click="postData1(scope.$index, scope.row)">保存</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="danger" v-on:click="cancelSaveph(scope.$index, scope.row)">取消</el-button>
                        </template>
                    </el-table-column>
                </el-table>
                <el-button class="tianjia1" type="primary" plain size="mini" v-on:click="jjVisible = true" style="margin:3px;margin-left:0px;display:none">添加间距</el-button>
                <div style="display:flex;margin-top:2px">
                    <el-table v-bind:data="emailTable"
                              style="width:329px;font-size: 12px;"
                              border max-height="100"
                              v-bind:header-cell-style="{background:'#F0F8FF'}"
                              class="emailTable"
                              key='fourTable'>
                        <el-table-column prop="UserName"
                                         label="接收人"
                                         width="72">
                        </el-table-column>
                        <el-table-column prop="EmailAddress"
                                         label="接收地址"
                                         width="145">
                        </el-table-column>
                        <el-table-column prop="operation1"
                                         label="操作"
                                         width="110">
                            <template slot-scope="scope">
                                <el-button v-show="showbtn" size="small" class="corparationBtn btnone" type="primary" v-on:click="editEmail(scope.$index, scope.row)">编辑</el-button>
                                <el-button v-show="showbtn" size="small" class="corparationBtn btnfour" type="danger" v-on:click="removeEmail(scope.$index, scope.row)">删除</el-button>
                                <el-button size="small" class="corparationBtn" style="display:none;" type="primary" v-on:click="postDataEmail(scope.$index, scope.row)">保存</el-button>
                                <el-button size="small" class="corparationBtn" style="display:none;" type="danger" v-on:click="cancelSaveEmail(scope.$index, scope.row)">取消</el-button>
                            </template>
                        </el-table-column>
                    </el-table>
                    <el-table v-bind:data="emailTable1"
                              style="width:331px;margin-left:2px;font-size: 12px;"
                              border max-height="100"
                              v-bind:header-cell-style="{background:'#F0F8FF'}"
                              class="emailTable1"
                              key='fourTable'>
                        <el-table-column prop="UserName"
                                         label="抄送人"
                                         width="78">
                        </el-table-column>
                        <el-table-column prop="EmailAddress"
                                         label="抄送地址"
                                         width="143">
                        </el-table-column>
                        <el-table-column prop="operation1"
                                         label="操作"
                                         width="108">
                            <template slot-scope="scope">
                                <el-button v-show="showbtn" size="small" class="corparationBtn btnone" type="primary" v-on:click="editCopy(scope.$index, scope.row)">编辑</el-button>
                                <el-button v-show="showbtn" size="small" class="corparationBtn btnfour" type="danger" v-on:click="removeCopy(scope.$index, scope.row)">删除</el-button>
                                <el-button size="small" class="corparationBtn" style="display:none;" type="primary" v-on:click="postDataCopy(scope.$index, scope.row)">保存</el-button>
                                <el-button size="small" class="corparationBtn" style="display:none;" type="danger" v-on:click="cancelSaveCopy(scope.$index, scope.row)">取消</el-button>
                            </template>
                        </el-table-column>
                    </el-table>

                </div>
                <el-button class="tianjia1" type="primary" plain size="mini" v-on:click="toVisible = true" style="margin:3px;display:none">添加发送</el-button>
                <el-button class="tianjia1" type="primary" plain size="mini" v-on:click="copyVisible = true" style="margin:3px;display:none;margin-left:245px">添加抄送</el-button>
            </el-aside>
        </el-container>
    </div>
    <div style="margin-top:20px;margin-bottom:2px;">
        <span style="margin-left:190px;font-size:15px">型号间距对应表</span>
    </div>
    <div class="btt">
        <el-container>
            <el-aside width="620px">
                <el-table v-bind:data="tableData2"
                          border max-height="260"
                          v-bind:row-style="trStyle2"
                          v-bind:header-cell-style="{background:'#F0F8FF'}"
                          class="table-container1" key='threeTable' ref="multipleTable">
                    <el-table-column prop="Mo2"
                                     label="模块型号"
                                     width="148">
                    </el-table-column>
                    <el-table-column prop="Spacing"
                                     label="间距"
                                     width="148">
                    </el-table-column>
                    <el-table-column prop="Creator"
                                     label="添加者"
                                     width="95">
                    </el-table-column>
                    <el-table-column prop="Modifier"
                                     label="修改者"
                                     width="95">
                    </el-table-column>
                    <el-table-column prop="operation2"
                                     label="操作"
                                     width="133">
                        <template slot-scope="scope">
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnone" type="primary" v-on:click="edit2(scope.$index, scope.row)">编辑</el-button>
                            <el-button v-show="showbtn" size="small" class="corparationBtn btnfour" type="danger" v-on:click="remove2(scope.$index, scope.row)">删除</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="primary" v-on:click="postData2(scope.$index, scope.row)">保存</el-button>
                            <el-button size="small" class="corparationBtn" style="display:none;" type="danger" v-on:click="cancelSavemx(scope.$index, scope.row)">取消</el-button>
                        </template>
                    </el-table-column>
                </el-table>
                <el-row>
                    <el-button class="tianjia1" type="primary" plain size="mini" v-on:click="mxVisible = true" style="margin:5px;display:none">添加模型</el-button>
                </el-row>
            </el-aside>
        </el-container>

        <el-container>
            <el-aside width="690px">
                <el-main style="padding:0 15px; margin-top:5px">
                    <div v-for="(item,index) in result" style="border:1px solid #DCDCDC;margin:1px;padding:0 5px;">
                        <div>
                            <div v-bind:id="'id'+(index+1)" class="f1Div" style="margin:2px;">
                                <p>IC引脚电流:</p>
                                <el-input class="aa" v-model="Vvalue[index]" v-on:change="f1()" v-on:keyup.native="f1()" placeholder="V值" size="mini"></el-input>
                                <p style="font-size:16px">÷</p>
                                <el-input class="aa" v-model="Rvalue[index]" v-on:change="f1()" v-on:keyup.native="f1()" placeholder="R值" size="mini"></el-input>
                                <p style="font-size:20px">*</p>
                                <el-input class="aa" v-model="Nvalue[index]" v-on:change="f1()" v-on:keyup.native="f1()" placeholder="N值" size="mini"></el-input>
                                <p style="font-size:20px">*</p>
                                <el-input class="aa" v-model="Gvalue[index]" v-on:change="f1()" v-on:keyup.native="f1()" placeholder="G值" size="mini"></el-input>
                                <p style="font-size:20px">*</p>
                                <el-input style="width:70px;font-size:14px" v-model="Lvalue[index]" v-on:change="f1()" v-on:keyup.native="f1()" placeholder="亮度" size="mini"></el-input>
                                <p style="font-size:15px">*1000</p>
                                <p style="font-size:15px">=</p>
                                <el-input class="wuca" v-model="result[index]" size="mini"></el-input>
                            </div>
                        </div>
                        <div class="f1Div">
                            <p>误差正10%：</p><el-input class="wuca" v-model="Zvalue[index]" size="mini"></el-input>
                            <p>误差负10%：</p><el-input class="wuca" v-model="Fvalue[index]" size="mini"></el-input>
                            <el-button v-on:click="deleteTrDiv(index)" size="mini" type="primary" plain>删除</el-button>
                        </div>
                    </div>
                    <el-button v-on:click="addTrDiv" size="mini" type="primary" plain>添加</el-button>
                </el-main>
            </el-aside>
        </el-container>
    </div>
</div>
@{
    var UserName = Session["User"] == null ? string.Empty : ((JianHeMES.Models.Users)Session["User"]).UserName;
    var UserIds = Session["User"] == null ? 0 : ((JianHeMES.Models.Users)Session["User"]).UserNum;
}
<script>
    var app = new Vue({
        el: "#app",
        data: {
            Zvalue: ["", "", ""],
            Fvalue: ["", "", ""],
            result: ["", "", ""],
            Vvalue: ["", "", ""],
            Rvalue: ["", "", ""],
            Nvalue: ["", "", ""],
            Gvalue: ["", "", ""],
            Lvalue: ["", "", ""],
            tableData: [],     //IC表格数据
            tableData1: [],   //PH间距表格数据
            tableData2: [],  //模块型号间距
            emailTable: [],
            emailTable1: [],
            icNum:'',       //查找IC
            icOption: [],   //查找IC
            mxNum: '',      //查找模块型号
            mxOption: [],   //查找模块型号
            icNumStyle: "",  //用于存放找到的IC型号
            mxNumStyle: "",    //用于存放找到的模块型号
            orderNum: null, //选择订单
            option: [],
            icStyle: "", //第一个表格变红的数据
            Mostyle: "", //第三个表格变红的数据
            Mostyle1: "",//第二个表格变红的数据
            Id: null,
            showbtn: true,
            icVisible: false,
            jjVisible: false,
            mxVisible: false,
            toVisible: false,
            copyVisible: false,
            bind: true,  //添加禁用按钮
            bind1: true, //添加禁用按钮
            bind2:true,  //添加禁用按钮
            labelPosition: 'right',
            IC_Model: null,
            V_REXT: null,
