/*全局样式*/
*, body {
    margin: 0;
    padding: 0;
}

#app {
    padding: 50px 0 0;
}

.el-input {
    max-width: 280px;
}

.el-main {
    padding: 20px 0 0 0 !important;
}

.printBtn {
    width: 400px;
    height: 100px;
    font-size: 50px !important;
}

[v-cloak] {
    display: none;
}

.textCenter {
    text-align: center;
    background-color: #ffffff;
}

.container, .body-content {
    width: 740px;
    padding: 0;
}
.navbar-nav {
     margin:0px;
}
@media screen and (min-width:728px) {
    .container, .body-content {
        width: 1200px;
    }

    .el-main {
        padding: 20px !important;
    }
}

#innerCarton, #masterCarton {
    color: #000;
}
/*内箱条码样式*/
#innerBarcode, .innerBarDiv {
    text-align: center;
    margin: 0 auto;
}

.mozhu {
    font-size: 120px;
    line-height: 105px;
    color: #000000;
    position: relative;
    top: 10px;
    right: 10px;
}

#innerCarton {
    margin: 10px auto;
    background-color: #ffffff;
    width: 480px;
    height: 230px;
}

#innerBarcode > .textCenter {
    background-color: #ffffff;
    position: relative;
}




/*外箱条码样式*/
#masterCarton {
    margin: 0 auto;
    background-color: #ffffff;
    width: 736px;
    padding: 50px 14px 10px 94px;
    height: 948px;
}

#masterBarcode {
    width: 480px;
}

.mcFrame {
    margin: 0 auto;
    width: 616px;
    height: 884px;
    border: 2px solid #000;
}

.mcFrame_top, .mcFrame_bottom {
    height: 440px;
}

    .mcFrame_top > div {
        height: 146px;
    }


.mcFrame_top_1 {
    position: relative;
}

.imgicon {
    position: absolute;
    width: 280px;
    top: 5px;
    left: 5px;
}

.mcFrame_top_1_h {
    margin: 0;
    padding-top: 70px;
    text-align: center;
}

.mcFrame_top_1_h_noimg {
    margin: 0;
    padding-top: 65px;
    text-align: center;
}

.mcFrame_top_3_1_1, .mcFrame_top_3_1_2 {
    display: inline-block;
    line-height: 70px;
    text-align: center;
}

.mcFrame_top_3_1_1 {
    width: 149px;
    font-size: 28px;
    font-weight: 600;
}

    .mcFrame_top_3_1_1 > span {
        font-weight: 600;
    }

.mcFrame_top_3_1_2 {
    width: 454px;
    font-size: 35px;
    font-weight: 600;
}

.mcFrame_top_3_1_1:not(:last-child) {
    border-right: 2px solid #000;
}

.borderTop {
    border-top: 2px solid #000;
}

.mcFrame_bottom_1 {
    /*width: 100%;
    height: 100%;*/
    padding: 5px;
    text-align: left;
}

.mcFrame_top_2 {
    position: relative;
}

.masterBarDiv {
    width: 100%;
    position: absolute;
    top: 8px;
}

.boxspan {
    display: inline-block;
    padding: 2px 8px;
    font-size: 19px;
}

.font20md {
    font-size: 47px;
}

.font20xs {
    font-size: 62px;
    padding: 0px 8px;
}

.h1order {
    font-size: 63px;
}
