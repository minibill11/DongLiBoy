@{
    ViewBag.Title = "打印条码测试";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
@* css放置处 *@
@section cssStyle {

}

<div id="app">
    <el-button @@click="onPrint" type="primary" size="small">打印条形码</el-button>
    @*<canvas id="barcode"></canvas>*@
    <div id="printArea">
        <div v-for="(item,index) in codelist">
            <div>
                <div>{{item.num}}</div>
                <img :id="item.id" />
                @*<canvas :id="item.id"></canvas>*@
                <div>{{item.str}}</div>
            </div>
        </div>
    </div>
</div>
@* 分部页放置处 *@
