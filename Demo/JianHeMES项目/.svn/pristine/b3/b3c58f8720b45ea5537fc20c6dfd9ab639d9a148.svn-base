<script type="text/template" id="Sa_Header">
    <div class="sa-header">
        <div class="sa-title">库存金额表</div>
        <div class="sa-header-btn">
            <el-button v-for="(item,index) in header_list" :key="index" size="small" type="primary" :plain="active==item?false:true" @@click="skip(item)" style="border-bottom: none;font-size:14px;">{{item}}</el-button>
        </div>
        <hr class="style-one" />
    </div>
</script>
<script>
    Vue.component('sa-header', {
        props: ["active"],
        template: document.getElementById("Sa_Header"),
        data: function () {
            return {
                active: '',
                header_list: ['库存金额信息', '月度库存金额', '查询ERP库存月度结存金额']
            }
        },
        methods: {
            skip(val) {
                if (val == '库存金额信息') {
                    window.open("/Warehouse_Material/StockAmount_Index")
                }
                if (val == '月度库存金额') {
                    window.open("/Warehouse_Material/StockAmount_Query")
                }
                if (val == '查询ERP库存月度结存金额') {
                    window.open("/Warehouse_Material/StockAmountCalculate")
                }                
            }
        }
    });
</script>