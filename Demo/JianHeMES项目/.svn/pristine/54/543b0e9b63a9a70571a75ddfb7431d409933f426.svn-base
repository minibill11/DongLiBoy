<style>
    /*全局样式*/
    *, body, .el-main, .el-header, .el-table .cell {
        padding: 0;
    }

    .body-content, .container {
        padding-left: 0px;
        padding-right: 0px;
    }

    #app {
        padding: 50px 0 0;
    }

    .el-header {
        height: auto !important;
    }

    .el-main {
        min-height: 600px;
    }

    [v-cloak] {
        display: none;
    }

    .el-progress-bar {
        margin-right: -60px;
        width: 88%;
    }

    .rightDiv {
        font-weight: 500;
        font-size: 13px;
    }

    @@media screen and (max-width: 768px) {
        .el-table .cell, .el-table--border th:first-child .cell, .el-table--border td:first-child .cell, .el-table th > .cell {
            padding: 0;
        }

        .rightDiv {
            font-size: 11px;
            line-height: 12px;
        }

        .letDiv {
            font-size: 11px;
            line-height: 12px;
            width: 100%;
            text-align: left;
        }

        .leftspan {
            display: inline-block;
            text-align: right;
            width: 45px;
            margin: 0 0 0 10px;
        }

        .lianghangDiv {
            font-size: 11px;
            line-height: 12px;
            height: 33px;
        }

        .el-table--mini th {
            padding: 3px 0;
        }

        .el-table--mini td {
            padding: 8px 0;
        }

        .el-divider--horizontal {
            margin: 4px 0;
        }
    }
</style>
