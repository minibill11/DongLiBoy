<el-table :data="tableList"
          :span-method="arraySpanMethod"
          stripe
          border>
    <el-table-column type="index"
                     width="45"
                     label="序号">
    </el-table-column>
    <el-table-column prop="name"
                     width="120"
                     sortable
                     label="工序">
    </el-table-column>
    <el-table-column prop="StandardTotal"
                     width="78"
                     label="标配人数(人)">
    </el-table-column>
    <el-table-column prop="StandardOutput"
                     label="标准产能(PCS/H)">
    </el-table-column>
    <el-table-column prop="StandardOutputForOne"
                     label="人均产能(PCS/H)">
    </el-table-column>
    <el-table-column prop="ModelNeed"
                     label="模组需求数量">
    </el-table-column>
    <el-table-column prop="ModelNeedForOne"
                     label="转换模组人均产能(PCS/H)">
    </el-table-column>
    <el-table-column prop="ModelNeedTimeForOne"
                     label="单人生产模组时间(H)">
    </el-table-column>
    <el-table-column prop="BalanceRate"
                     label="工段平衡系数(组装为1)">
    </el-table-column>
    <el-table-column label="单人生产每模组需求总工时(PCS/H)">
        <template slot-scope="scope">
            {{ModelNeedTimeForOneCount}}
        </template>
    </el-table-column>
    <el-table-column label="总人数(人)">
        <template slot-scope="scope">
            {{StandardTotalCount}}
        </template>
    </el-table-column>
    <el-table-column label="每小时产能(模组/H)">
        <template slot-scope="scope">
            {{ModelNeedTimeForOneCount==0?0:StandardTotalCount/ModelNeedTimeForOneCount}}
        </template>
    </el-table-column>
</el-table>
