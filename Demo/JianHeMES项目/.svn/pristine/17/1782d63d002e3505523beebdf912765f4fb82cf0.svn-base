                string[] str = ordernum.Split('-');
                string start = str[0].Substring(2);
                string Num = (printCount + 1).ToString().PadLeft(3, '0');
                #endregion

                //外箱条码
                info.Add("boxNum", start + str[1] + str[2] + "-" + Num);
                //SN/TN
                info.Add("SNTN", printCount + 1 + "/" + OuterBoxCapacity);
                //数量QTY
                info.Add("QTY", modelBoxNum.Count());


            }
            else
            {

            }

        }

        //外箱条码生产
        public string GetOuterBoxBarCode(string ordernum)
        {
            string[] str = ordernum.Split('-');
            string start = str[0].Substring(2);
            //得到订单总的箱体数
            var OuterBoxCapacity = db.Packing_BasicInfo.Where(c => c.OrderNum == ordernum).Select(c => c.OuterBoxCapacity).ToList();
            var count = OuterBoxCapacity.Sum();
            //已经打印的箱体数
            var printCount = db.Packing_BarCodePrinting.Count(c => c.OrderNum == ordernum);

            string Num = (count - printCount).ToString().PadLeft(3, '0');
            return start + str[1] + str[2] + "-" + Num;
        }

        //通过订单，显示里面的箱体号
        public List<string> BoxNum(string ordernum)
        {
            var boxNum = db.Packing_BarCodePrinting.Where(c => c.OrderNum == ordernum).Select(c => c.ModuleGroupNum).ToList();
            if (boxNum.Count != 0)
            {
                return boxNum;
            }
            return null;
        }

        //外箱条码OQC确认
        public void CheckOQC(string ordernum)
        {
            var list = db.Packing_BarCodePrinting.Where(c => c.OrderNum == ordernum).ToList();
            if (list.Count != 0)
            {
                foreach (var item in list)
                {
                    item.QC_Operator = ((Users)Session["User"]).UserName;
                    item.QC_ComfirmDate = DateTime.Now;
                    db.SaveChangesAsync();
                }

            }
        }

        //外箱入库录入
        public void CretecWarehouseInfo(string ordernum, string warehouseNum, string boxbarcode)
        {

            if (string.IsNullOrEmpty(ordernum) || string.IsNullOrEmpty(warehouseNum) || string.IsNullOrEmpty(boxbarcode))
            {

            }
            var barcodeList = db.Packing_BarCodePrinting.Where(c => c.OrderNum == ordernum && c.OuterBoxBarcode == boxbarcode).ToList();
            foreach (var item in barcodeList)
            {
                Warehouse_Join join = new Warehouse_Join() { OrderNum = ordernum, BarCodeNum = item.BarCodeNum, ModuleGroupNum = item.ModuleGroupNum, OuterBoxBarcode = boxbarcode, Operator = ((Users)Session["User"]).UserName, Date = DateTime.Now };
                db.Warehouse_Join.Add(join);
                db.SaveChangesAsync();
            }

        }

        //外箱入库OQC确认
        public void CheckWarehouseOQC(string ordernum)
        {
            var list = db.Warehouse_Join.Where(c => c.OrderNum == ordernum).ToList();
            if (list.Count != 0)
            {
                foreach (var item in list)
                {
                    item.QC_Operator = ((Users)Session["User"]).UserName;
                    item.QC_ComfirmDate = DateTime.Now;
                    db.SaveChangesAsync();
                }

            }
        }

