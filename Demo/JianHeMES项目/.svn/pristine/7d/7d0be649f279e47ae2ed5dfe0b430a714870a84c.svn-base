.rightMainContent {
    .topiframe {
        height: 150px;

        .aboBanImg {
            position: fixed;
            z-index: -999;
            width: calc(~"100vw - 320px");
            margin: 0 60px;
            bottom: 80px;
        }

        .textLogo {

            img {
                width: 500px;
                position: fixed;
                left: 26%;
                top: 150px;
            }
        }
    }

    .bottomiframe {

        .indexContent {
            /*height: e("calc(100vh - 230px )");*/
            background-color: #FFF;

            .leftText {
                padding: 15px 30px;

                .text {
                    /*height: e("calc(100vh - 350px )");*/
                    p {
                        font-size: 13px;
                        line-height: 23px;
                        margin-bottom: 20px;
                        color: #5b5b5b;
                    }

                    .el-collapse {
                        border: 0;

                        .el-collapse-item__header {
                            height: 30px;
                            line-height: 30px;
                            border-bottom: 0;

                            .el-collapse-item__arrow {
                                margin: 3px 5px 0;
                            }
                        }
                    }
                }
            }

            .rightImg {
                height: 100%;

                img {
                    width: 15%;
                    position: fixed;
                    bottom: 250px;
                }
            }
        }
    }
}
