@*  <summary>
    1.工序产能下拉选择框组件
    </summary>*@

<el-form :inline="true" :model="queryTable" size="small">
    <el-form-item label="平台">
        <el-select v-model="queryTable.proplatform" placeholder="平台" filterable clearable allow-create default-first-option @*multiple collapse-tags style="width:220px"*@>
            <el-option v-for="item in options.proplatform"
                       :key="item.value"
                       :value="item.value">
            </el-option>
        </el-select>
    </el-form-item>
    <el-form-item label="型号">
        <el-select v-model="queryTable.protype" placeholder="型号" filterable clearable allow-create default-first-option>
            <el-option v-for="item in options.protype"
                       :key="item.value"
                       :value="item.value">
            </el-option>
        </el-select>
    </el-form-item>
    @*<el-form-item label="文件状态">
            <el-select v-model="queryTable.statu" placeholder="审核/核准状态" clearable>
                <el-option v-for="item in options.statu"
                           :key="item.value"
                           :value="item.value">
                </el-option>
            </el-select>
        </el-form-item>*@
    <el-form-item>
        <el-button type="primary" @@click="onQuerySubmit">查询</el-button>
    </el-form-item>
</el-form>