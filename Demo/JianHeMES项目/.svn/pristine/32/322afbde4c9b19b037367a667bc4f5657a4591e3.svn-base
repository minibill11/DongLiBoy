@{
    ViewBag.Title = "MC锡膏看板";
}
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<link href="~/Content/styleFile/solder/solderStyle.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<div id="app" v-cloak>
    <el-container>
        <el-header class="text-center">
            <div v-show="screenSize>=768">
                <h2 class="text-center">@ViewBag.Title</h2>
                <a href="/SMT_Sulderpaster/mcBoard"><el-button size="small">MC看板</el-button></a>
                <a href="/SMT_Sulderpaster/smtBoard"><el-button size="small">SMT看板</el-button></a>
                <a class="luru" href="/SMT_Sulderpaster/AddWarehouse_Material"><el-button size="small">录入物料信息</el-button></a>
                <a class="cangkucaozuo" href="/SMT_Sulderpaster/AddWareHouseBaseInfo"><el-button size="small">锡膏入库</el-button></a>
                <a class="cangkucaozuo" href="/SMT_Sulderpaster/AddWarehouseFreezer"><el-button size="small">锡膏入冰柜</el-button></a>
                <a class="cangkucaozuo" href="/SMT_Sulderpaster/outWarehouseFreezer"><el-button size="small">锡膏出库</el-button></a>
                <a class="cangkucaozuo" href="/SMT_Sulderpaster/printBarcode"><el-button size="small">重新打印条码</el-button></a>
            </div>
            <div v-show="screenSize<768">
                <h3>@ViewBag.Title</h3>
                <el-dropdown placement="bottom">
                    <el-button size="medium ">
                        更多菜单<i class="el-icon-arrow-down el-icon--right"></i>
                    </el-button>
                    <el-dropdown-menu slot="dropdown">
                        <a href="/SMT_Sulderpaster/mcBoard"><el-dropdown-item>MC看板</el-dropdown-item></a>
                        <a href="/SMT_Sulderpaster/smtBoard"><el-dropdown-item>SMT看板</el-dropdown-item></a>
                        <a class="luru" href="/SMT_Sulderpaster/AddWarehouse_Material"><el-dropdown-item>录入物料信息</el-dropdown-item></a>
                        <a class="cangkucaozuo" href="/SMT_Sulderpaster/AddWareHouseBaseInfo"><el-dropdown-item>锡膏入库</el-dropdown-item></a>
                        <a class="cangkucaozuo" href="/SMT_Sulderpaster/AddWarehouseFreezer"><el-dropdown-item>锡膏入冰柜</el-dropdown-item></a>
                        <a class="cangkucaozuo" href="/SMT_Sulderpaster/outWarehouseFreezer"><el-dropdown-item>锡膏出库</el-dropdown-item></a>
                        <a class="cangkucaozuo" href="/SMT_Sulderpaster/printBarcode"><el-dropdown-item>重新打印条码</el-dropdown-item></a>
                    </el-dropdown-menu>
                </el-dropdown>
            </div>
        </el-header>
        <el-main v-loading="loading">
            <el-row class="text-center">
                <span class="inputtext">锡膏条码：</span>
                <el-input placeholder="请输入锡膏条码"
                          v-model.trim="barcode"
                          size="medium"
                          autofocus
                          clearable>
                </el-input>
                <span class="inputtext">料号：</span>
                <el-input placeholder="请输入料号"
                          v-model.trim="materialNumber"
                          size="medium"
                          clearable>
                </el-input>
                <span class="inputtext">批次：</span>
                <el-input placeholder="请输入批次"
                          v-model.trim="batch"
                          size="medium"
                          clearable>
                </el-input>
                <el-switch v-model="total"
                           active-text="全部"
                           inactive-text="已入库">
                </el-switch>
                <el-button v-if="barcode==''&&materialNumber==''&&batch==''" v-on:click="getdata" type="primary" size="medium">查找</el-button>
                <el-button v-else v-on:click="getdata" type="primary" size="medium">查找</el-button>
                @*<div class="inputframe"></div>*@
            </el-row>
            <br />
            <el-row>
                <el-table :data="dataList" ref="filterTable" max-height="500" border stripe>
                    <el-table-column prop="barcode" label="条码号" sortable>
                    </el-table-column>
                    <el-table-column prop="MaterialNumber"
                                     label="物料号"
                                     :filters="filterObj.MaterialNumber"
                                     :filter-method="filterHandler"
                                     sortable>
                    </el-table-column>
                    <el-table-column prop="Batch"
                                     label="批次"
                                     :filters="filterObj.Batch"
                                     :filter-method="filterHandler"
                                     sortable>
                    </el-table-column>
                    <el-table-column prop="Supplier"
                                     label="供应商"
                                     :filters="filterObj.Supplier"
                                     :filter-method="filterHandler"
                                     sortable>
                    </el-table-column>
                    <el-table-column prop="LeaveFactoryTime"
                                     label="生产时间"
                                     :filters="filterObj.LeaveFactoryTime"
                                     :filter-method="filterHandler"
                                     sortable>
                        <template slot-scope="scope">
                            {{scope.row.LeaveFactoryTime.split("T")[0]}}
                        </template>
                    </el-table-column>
                    <el-table-column prop="SolderpasteType"
                                     label="型号"
                                     :filters="filterObj.SolderpasteType"
                                     :filter-method="filterHandler"
                                     sortable>
                    </el-table-column>
                    <el-table-column prop="effTime"
                                     label="剩余有效时间"
                                     sortable>
                        <template slot-scope="scope">
                            <span :class="[scope.row.effTime<=0?'textred':scope.row.effTime<=30?'textorange':'']">{{scope.row.effTime}}天</span>
                        </template>
                    </el-table-column>
                    <el-table-column prop="statue"
                                     label="状态"
                                     :filters="filterObj.statue"
                                     :filter-method="filterHandler"
                                     sortable>
                    </el-table-column>
                </el-table>
                <div v-show="surplusCount>0" style="text-align:right;padding:3px 5px;font-size:16px;font-weight:500;color:#539eee;">仓库剩余总盒数：{{surplusCount}}， 仓库剩余总重量：{{surplusCount*0.5}}KG &nbsp; (以每盒0.5KG计算)</div>
            </el-row>
            <hr />
            <el-row>
                <el-table v-bind:data="infoList" max-height="600" border stripe>
                    <el-table-column label="当前存在的物料信息">
                        <el-table-column prop="ManufactorNum" label="厂商编号" width="120">
                        </el-table-column>
                        <el-table-column prop="ManufactorName" label="厂商简称" width="120">
                        </el-table-column>
                        @*<el-table-column prop="InvoiceNum" label="收货单号" width="100">
                            </el-table-column>
                            <el-table-column prop="InvoiceTerm" label="收货项次" width="60">
                            </el-table-column>
                            <el-table-column prop="InvoiceDate" label="收货日期" width="80">
                                <template slot-scope="scope">
                                    {{scope.row.InvoiceDate.split("T")[0]}}
                                </template>
                            </el-table-column>*@
                        <el-table-column prop="MaterialNumber" label="物料编号" width="120">
                        </el-table-column>
                        <el-table-column prop="ProductName" label="品名" width="120">
                        </el-table-column>
                        <el-table-column prop="Specifications" label="规格">
                        </el-table-column>
                        @*<el-table-column prop="PurchaseNum" label="采购单号" width="100">
                            </el-table-column>
                            <el-table-column prop="PurchaseCount" label="采购数量" width="60">
                            </el-table-column>
                            <el-table-column prop="REQNum" label="请购单号/项次" width="120">
                            </el-table-column>
                            <el-table-column prop="ReceivingCount" label="收货量" width="60">
                            </el-table-column>*@
                        @*<el-table-column prop="WarehouseCompany" label="入库单位" width="60">
                            </el-table-column>*@
                        @*<el-table-column prop="WarehouseCount" label="入库量" width="60">
                            </el-table-column>*@
                        @*<el-table-column prop="ReceivingMoney" label="收货金额" width="80">
                            </el-table-column>*@
                        <el-table-column prop="Type" label="型号" width="120">
                        </el-table-column>
                        @*<el-table-column label="操作" width="50">
                                <template slot-scope="scope">
                                    <el-button v-on:click.native.prevent="editRow(scope.row,scope.$index)"
                                               type="text"
                                               size="mini">
                                        编辑
                                    </el-button>
                                </template>
                            </el-table-column>*@
                    </el-table-column>
                </el-table>
            </el-row>
        </el-main>
    </el-container>
</div>
<script src="~/Content/styleFile/solder/solderJavascript.js"></script>
<script src="~/Scripts/filterMethod/tableFilter.js"></script>
<script>
    var app = new Vue({
        el: "#app",
        mixins: [tableFilterMixin],
        data: {
            loading: false,
            screenSize: document.body.clientWidth,
            barcode: "",
            materialNumber: "",
            batch: "",
            dataList: [],
            infoList: [],
            filterObj: {
                MaterialNumber: [],
                Batch: [],
                Supplier: [],
                LeaveFactoryTime: [],
                SolderpasteType: [],
                statue: []
            },
            total: false,
            surplusCount: 0,
        },
        created: function () {
            window.onresize = function () {
                app.screenSize = document.body.clientWidth;
            };
        },
        mounted: function () {
            //this.getdata();
            this.huoqu();
            setInterval(function () {
                app.huoqu();
            }, 60000);


        },
        methods: {
            //获取已存在信息
            huoqu: function () {
                axios.post("/SMT_Sulderpaster/SelctMaterial").then(res => {
                    //console.log(res.data);
                    this.infoList = res.data[0];
                }).catch(err => {
                    console.warn(err);
                });
            },
            getdata: function () {
                this.loading = true;
                this.surplusCount = 0;
                axios.post("/SMT_Sulderpaster/MCBoard", {
                    barcode: this.barcode,
                    materialNumber: this.materialNumber,
                    batch: this.batch,
                    total: this.total
                }).then(res => {
                    //console.log(res.data);
                    let rt = res.data;
                    rt.sort(function (a, b) {
                        return a.effTime > b.effTime;
                    });
                    this.dataList = rt;
                    this.surplusCompute(rt);
                    this.loading = false;
                }).catch(err => {
                    console.warn(err);
                    this.loading = false;
                });
            },
            //计算剩余数量
            surplusCompute(list) {
                let count = 0;
                if (list.length > 0) {
                    for (let i of list) {
                        if (i.statue == "已入库") {
                            count++;
                        };
                    };
                    this.surplusCount = count;
                };
            }
        },
        watch: {
            dataList(v) {
                try {
                    if (v != '') {
                        let rtArr = this.filterMethod(v);
                        this.$refs.filterTable.clearFilter();
                        for (let i in this.filterObj) {
                            this.$set(this.filterObj, i, rtArr[i]);
                        };
                    };
                } catch (err) {
                    console.log('筛选错误')
                }
            }
        }
    });
</script>