@{
    ViewBag.Title = "灯面生产看板";
}
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<script src="~/Scripts/echarts.js"></script>
<link href="~/Content/styleFile/SMT/smtContenInfo.css" rel="stylesheet" />
<script src="~/Content/styleFile/SMT/smtContenInfo.js"></script>
<div id="app" v-cloak>
    <el-container>
        <el-header class="text-center">
            <el-row class="text-center">
                <h2 style="margin-top:3%">
                    <span @@click="drawerShow=true" style="cursor:pointer;color:#000000;">@ViewBag.Title</span>
                    <el-date-picker v-model="postTime"
                                    :picker-options="pickerOptions"
                                    :editable="false"
                                    style="position:absolute;right:0;bottom:10px"
                                    placeholder="选择日期"
                                    value-format="yyyy-MM-dd"
                                    format="yyyy 年 MM 月 dd 日"
                                    align="right"
                                    size="small"
                                    type="date">
                    </el-date-picker>
                </h2>
            </el-row>
        </el-header>
        <el-main>
            <el-row :gutter="25">
                <el-col :span="8" class="titleDiv-left">
                    <div class="achieveStatus">
                        <div>今班达成状况</div>
                        <div class="achieveStatusDiv">
                            <div>计划达成率</div>
                            <div>{{dataList.achievementRate}}%</div>
                        </div>
                        <div class="achieveStatusDiv">
                            <div>生产良率</div>
                            <div>{{dataList.ProductionYield}}%</div>
                        </div>
                    </div>
                </el-col>
                <el-col :span="8" class="titleDiv-center">
                    <div id="efficiencyBarbody"></div>
                </el-col>
                <el-col :span="8" class="titleDiv-right">
                    <div id="qualityBarbody"></div>
                </el-col>
                <el-col :span="24" style="margin-top:10px;">
                    <el-table :data="dataList.content" border>
                        <el-table-column label="线别" align="center" width="100">
                            <template slot-scope="scope">
                                <div class="scopeDiv">{{scope.row.Line}}线</div>
                            </template>
                        </el-table-column>
                        <el-table-column label="订单号" align="center" min-width="160">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.OrderNum}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column label="总数" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.totalNum}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="生产状态" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.Statu}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="目标投入" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.targetNum}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="实际产出" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.actualNum}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="单板点数" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.VeneerPoints}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="达成率" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.PassRate}}%</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column prop="" label="不良点数" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.AbnormalCount}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                        <el-table-column label="DPPM" align="center">
                            <template slot-scope="scope">
                                <template v-for="(i,index) in scope.row.Content">
                                    <div class="scopeDiv">{{i.DPPM}}</div>
                                    <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                </template>
                            </template>
                        </el-table-column>
                    </el-table>
                </el-col>
            </el-row>
        </el-main>
    </el-container>
    <el-drawer :visible.sync="drawerShow"
               :append-to-body="true"
               :close-on-press-escape="false"
               :show-close="false"
               :wrapperClosable="false"
               :withHeader="false"
               style="overflow:auto"
               direction="ttb"
               size="100%">
        <el-container>
            <el-header class="text-center">
                <el-row class="text-center">
                    <h2>
                        <span @@click="drawerShow=false" style="cursor:pointer;">@ViewBag.Title</span>
                        <el-date-picker v-model="postTime"
                                        :picker-options="pickerOptions"
                                        :editable="false"
                                        style="position:absolute;right:0;bottom:10px"
                                        placeholder="选择日期"
                                        value-format="yyyy-MM-dd"
                                        format="yyyy 年 MM 月 dd 日"
                                        align="right"
                                        size="small"
                                        type="date">
                        </el-date-picker>
                    </h2>
                </el-row>
            </el-header>
            <el-main class="body-color">
                <el-row :gutter="25">
                    <el-col :span="8" class="titleDiv-left">
                        <div class="achieveStatus">
                            <div>今班达成状况</div>
                            <div class="achieveStatusDiv">
                                <div>计划达成率</div>
                                <div>{{dataList.achievementRate}}%</div>
                            </div>
                            <div class="achieveStatusDiv">
                                <div>生产DPPM</div>
                                <div>{{dataList.ProductionYield}}%</div>
                            </div>
                        </div>
                    </el-col>
                    <el-col :span="8" class="titleDiv-center">
                        <div id="efficiencyBar"></div>
                    </el-col>
                    <el-col :span="8" class="titleDiv-right">
                        <div id="qualityBar"></div>
                    </el-col>
                    <el-col :span="24" style="margin-top:10px;">
                        <el-table :data="dataList.content" border>
                            <el-table-column label="线别" align="center" width="100">
                                <template slot-scope="scope">
                                    <div class="scopeDiv">{{scope.row.Line}}线</div>
                                </template>
                            </el-table-column>
                            <el-table-column label="订单号" align="center" min-width="160">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.OrderNum}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column label="总数" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.totalNum}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="生产状态" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.Statu}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="目标投入" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.targetNum}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="实际产出" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.actualNum}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="单板点数" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.VeneerPoints}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="达成率" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.PassRate}}%</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column prop="" label="不良点数" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.AbnormalCount}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                            <el-table-column label="DPPM" align="center">
                                <template slot-scope="scope">
                                    <template v-for="(i,index) in scope.row.Content">
                                        <div class="scopeDiv">{{i.DPPM}}</div>
                                        <el-divider v-if="index<(scope.row.Content.length-1)"></el-divider>
                                    </template>
                                </template>
                            </el-table-column>
                        </el-table>
                    </el-col>
                </el-row>
            </el-main>
        </el-container>
    </el-drawer>
</div>
<script>
    var app = new Vue({
        el: "#app",
        mixins: [chartMixin],
        data: {
            dataList: {},
            postTime: "",
        },
        created: function () {
            this.getdata('DisplayLightContenInfo');
        },
        watch: {
            postTime(v) {
                if (this.isPoll(v)) {
                    this.poll = setInterval(() => {
                        this.getdata('DisplayLightContenInfo');
                    }, 5000);
                } else {
                    this.getdata('DisplayLightContenInfo');
                };
            }
        }
    });
</script>