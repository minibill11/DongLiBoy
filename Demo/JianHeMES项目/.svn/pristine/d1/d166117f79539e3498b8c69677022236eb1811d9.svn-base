
@{
    ViewBag.Title = "SMT生产信息";
}
<link href="~/Scripts/Bootstraps/bootstrap-select.css" rel="stylesheet" />
<script src="~/Scripts/Bootstraps/bootstrap-select.js"></script>
<script src="~/Scripts/imgJS/jquery.artZoom.js"></script>
<link href="~/Content/styleFile/jquery.artZoom.css" rel="stylesheet" />
<link href="~/Content/styleFile/packaging/index.css" rel="stylesheet" />
<script src="~/Content/styleFile/packaging/index.js"></script>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<base target="_blank" />

<style>
    .gotoDetail {
        color: #1449b7;
        cursor: pointer;
    }
    /*.table > tbody > tr:nth-child(n+3):hover {
        background-color: rgba(77, 72, 72, 0.15);
    }*/
    .modal-header, .modal-body {
        padding: 8px 10px 2px;
    }

    .showBtn {
        position: relative;
        color: #14830d;
        cursor: pointer;
    }

    .showDiv {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0px;
        left: 0px;
        visibility: hidden;
        opacity: 0;
        transition: opacity 1s;
    }

    .btnSty {
        position: absolute;
        width: 100%;
        height: 100%;
        padding: 0;
    }

    .showBtn:hover .showDiv {
        visibility: visible;
        opacity: 1;
    }

    .body-content, .container {
        padding: 0;
    }

    .table > thead > tr > th, .table-bordered > tbody > tr > td {
        text-align: center;
        vertical-align: middle;
        font-size: 12px;
        padding: 1px;
        border-color: rgba(35, 35, 35, 0.9);
    }

    .table-bordered > thead > tr > th {
        border-bottom-width: 1px;
    }

    .table > thead:first-child > tr:first-child > th, .table > colgroup + thead > tr:first-child > th {
        border-top: 1px solid rgba(35, 35, 35, 0.9);
    }

    @@media screen and (min-width: 1200px) {
        .container {
            width: 1420px;
        }

        .titleul {
            width: 1200px;
        }
    }

    #app, #vmTitle {
        width: 1420px;
    }

    .table {
        width: 1403px;
    }

    .scroll_table_content {
        width: 100%;
        height: 640px;
        overflow: auto;
    }

    .table > tbody > tr:first-child > td {
        border-top: none;
    }

    .container > .navbar-collapse {
        margin-left: 0;
    }

    .selectpicker {
        border: 1px solid #ccc;
    }

    [v-cloak] {
        display: none;
    }

    .table-bordered > tbody > tr:hover {
        background-color: #e0ecff;
    }
</style>
<div id="vmTitle" class="row text-center" style="position:relative;padding:15px 20px;" v-cloak>
    <span class="text-center" style="font-size:24px;font-family: inherit;font-weight: 500;">SMT生产信息</span>
    <div>
        <select id="ordernumVal" class="selectpicker" title="全部订单" data-live-search="true" data-selected-text-format="count" data-style="form-control" data-actions-box="true" multiple>
            @*<option value="">全部订单</option>*@
            <template v-for="item in ordernumList">
                <option>{{item}}</option>
            </template>
        </select>
        <select id="platformVal" class="selectpicker" title="全部平台" data-live-search="true" data-selected-text-format="count" data-style="form-control" data-actions-box="true" multiple>
            @*<option value="">全部平台</option>*@
            <template v-for="item in platformList">
                <option>{{item}}</option>
            </template>
        </select>
        <select id="jobContentVal" class="selectpicker" title="工作内容" data-live-search="true" data-selected-text-format="count" data-style="form-control" data-actions-box="true" multiple>
            @*<option value="">全部平台</option>*@
            <template v-for="item in jobContentList">
                <option>{{item}}</option>
            </template>
        </select>
    </div>
    <div class="row form-group" style="position:absolute;width:150px;right:20px;top:40px">
        <span class="btn btn-default">
            @Html.ActionLink("历史记录", "SMT_ProductionInfoHistory", new { id = "" }, new { target = "_blank" })
        </span><br />
        <span style="width:140px">{{nowtime}}</span>
    </div>
</div>
<div id="app" v-cloak>
    <table class="table table-bordered" style="margin-bottom:0;">
        <colgroup>
            <col width="30 ">
            <col width="40 ">
            <col width="66 ">
            <col width="56 ">
            <col width="48 ">
            <col width="48 ">
            <col width="60 ">
            <col width="66 ">
            <col width="56 ">
            <col width="56 ">
            <col width="50 ">
            <col width="50 ">

            <col width="54 ">
            <col width="54 ">
            <col width="60 ">
            <col width="60 ">
            <col width="56 ">
            <col width="56 ">
            <col width="56 ">
            <col width="56 ">
            <col width="60 ">
            <col width="60 ">
            <col width="68 ">
            <col width="68 ">
            <col width="68 ">
        </colgroup>
        <thead>
            <tr>
                <th rowspan="2">序号</th>
                <th rowspan="2">线别</th>
                <th colspan="8">PC</th>
                <th rowspan="2" style="width:50px">小样<br />进度</th>
                <th rowspan="2" style="width:50px">小样<br />异常</th>
                <th colspan="4">今天产量</th>
                <th colspan="2">订单在对应线别</th>
                <th colspan="2">订单在对应工作内容</th>
                <th colspan="5">统计</th>
            </tr>
            <tr>
                <th>订单号</th>
                <th>平台</th>
                <th>制程<br />要求</th>
                <th>标准<br />要求</th>
                <th>模块数</th>
                <th>工作内容</th>
                <th>今天<br />计划数</th>
                <th>产能</th>
                <th>良品(PCS)</th>
                <th>不良品(PCS)</th>
                <th>完成率</th>
                <th>合格率</th>
                <th>良品总数</th>
                <th>不良品<br />总数</th>
                <th>良品总数</th>
                <th>不良品<br />总数</th>
                <th>总完成率</th>
                <th>总合格率</th>
                <th>开始时间</th>
                <th>结束时间</th>
                <th>生产用时</th>
            </tr>
        </thead>
    </table>

    <div class="scroll_table_content">
        <table class="table table-bordered">
            <colgroup>
                <col width="30 ">
                <col width="40 ">
                <col width="66 ">
                <col width="56 ">
                <col width="48 ">
                <col width="48 ">
                <col width="60 ">
                <col width="66 ">
                <col width="56 ">
                <col width="56 ">
                <col width="50 ">
                <col width="50 ">

                <col width="54 ">
                <col width="54 ">
                <col width="60 ">
                <col width="60 ">
                <col width="56 ">
                <col width="56 ">
                <col width="56 ">
                <col width="56 ">
                <col width="60 ">
                <col width="60 ">
                <col width="68 ">
                <col width="68 ">
                <col width="68 ">
            </colgroup>
            <tbody>
                <template v-for="(val,index) in sortmarqueeList">
                    <template v-for="(v,i) in val">
                        <tr v-if="i==0">
                            <td v-bind:rowspan="val.length">{{index+1}}</td>
                            <td>{{v[0].LineNum}}</td>
                            <td v-bind:rowspan="val.length" v-on:click="orderDetail(v[0].OrderNum,v[0].Id)" class="gotoDetail">{{v[0].OrderNum}}</td>
                            <td v-bind:rowspan="val.length">{{v[0].PlatformType}}</td>
                            <td v-bind:rowspan="val.length">{{v[0].ProcessingRequire}}</td>
                            <td v-bind:rowspan="val.length">{{v[0].StandardRequire}}</td>
                            <td>{{v[0].Models}}</td>
                            <td>{{v[0].JobContent}}</td>
                            <td>{{v[0].PlanQuantity}}</td>
                            <td>{{v[0].Capacity |zerocut}}</td>
                            <td v-bind:rowspan="val.length"
                                v-bind:class="(v[0].HandSampleScedule=='未开始')?'':'showBtn'"
                                v-on:click.stop.prevent="(v[0].HandSampleScedule=='未开始')?'':clickBtn($event)">
                                <el-popover placement="left"
                                            title="小样报告"
                                            trigger="hover"
                                            :disabled="(v[0].HandSampleSceduleReport=='false')"
                                            :open-delay="0"
                                            :close-delay="0"
                                            transition=""
                                            style="width:100%;height:100%">
                                    <div>
                                        <template v-for="i in v[0].HandSampleSceduleReport">
                                            <a :href="'/Small_Sample/Display?id='+i.id" target="_blank">
                                                <el-button size="small" type="success" style="margin:0 2px;">{{i.Name}}</el-button>
                                            </a>
                                        </template>
                                    </div>
                                    <div slot="reference" style="width:100%;height:100%">
                                        {{v[0].HandSampleScedule}}
                                        <div v-if="(v[0].HandSampleScedule!='未开始')&&(v[0].HandSampleSceduleJpg=='true')&&(v[0].HandSampleScedulePdf=='true')" class="showDiv" v-on:click.stop.prevent>
                                            <button v-on:click.stop.prevent="imgModal(v[0].OrderNum)" class="btn btn-info btnSty" style="width: 100%;right: 50%;">IMG</button>
                                            <button v-on:click.stop.prevent="pdfModal(v[0].OrderNum)" class="btn btn-warning btnSty" style="width: 100%;left: 50%;">PDF</button>
                                        </div>
                                        <div v-else-if="(v[0].HandSampleScedule!='未开始')&&(v[0].HandSampleSceduleJpg=='true')&&(v[0].HandSampleScedulePdf=='false')" class="showDiv">
                                            <button v-on:click.stop.prevent="imgModal(v[0].OrderNum)" class="btn btn-info btnSty" style="width: 100%;right: 0;">IMG</button>
                                        </div>
                                        <div v-else-if="(v[0].HandSampleScedule!='未开始')&&(v[0].HandSampleSceduleJpg=='false')&&(v[0].HandSampleScedulePdf=='true')" class="showDiv">
                                            <button v-on:click.stop.prevent="pdfModal(v[0].OrderNum)" class="btn btn-warning btnSty" style="width: 100%;left: 0;">PDF</button>
                                        </div>
                                        <div v-else class="showDiv">
                                            <button class="btn btn-default btnSty" style="width: 100%;left: 0;">未上传</button>
                                        </div>
                                    </div>
                                </el-popover>
                            </td>
                            <td v-bind:rowspan="val.length"
                                v-bind:class="(v[0].SMTAbnormal_Description)?'showBtn':''"
                                v-on:click="(v[0].SMTAbnormal_Description)?clickBtn($event):''">
                                {{v[0].SMTAbnormal_Description}}
                                <div v-if="(v[0].SMTAbnormal_Description)&&(v[0].SMTAbnormal_DescriptionJpg=='true')&&(v[0].SMTAbnormal_DescriptionPdf=='true')" class="showDiv" v-on:click.stop.prevent>
                                    <button v-on:click.stop.prevent="imgycModal(v[0].OrderNum)" class="btn btn-info btnSty" style="width: 100%;right: 50%;">IMG</button>
                                    <button v-on:click.stop.prevent="pdfycModal(v[0].OrderNum)" class="btn btn-warning btnSty" style="width: 100%;left: 50%;">PDF</button>
                                </div>
                                <div v-else-if="(v[0].SMTAbnormal_Description)&&(v[0].SMTAbnormal_DescriptionJpg=='true')&&(v[0].SMTAbnormal_DescriptionPdf=='false')" class="showDiv">
                                    <button v-on:click.stop.prevent="imgycModal(v[0].OrderNum)" class="btn btn-info btnSty" style="width: 100%;right: 0;">IMG</button>
                                </div>
                                <div v-else-if="(v[0].SMTAbnormal_Description)&&(v[0].SMTAbnormal_DescriptionJpg=='false')&&(v[0].SMTAbnormal_DescriptionPdf=='true')" class="showDiv">
                                    <button v-on:click.stop.prevent="pdfycModal(v[0].OrderNum)" class="btn btn-warning btnSty" style="width: 100%;left: 0;">PDF</button>
                                </div>
                                <div v-else class="showDiv">
                                    <button class="btn btn-default btnSty" style="width: 100%;left: 0;">未上传</button>
                                </div>
                            </td>

                            <td>{{v[0].NormalCount}}</td>
                            <td>{{v[0].AbnormalCount}}</td>
                            <td>{{v[0].TodayFinishRate==""?0:v[0].TodayFinishRate|zerocut}}%</td>
                            <td>{{v[0].TodayPassRate==""?0:v[0].TodayPassRate|zerocut}}%</td>
                            <td>{{v[0].LineAllNomalCount}}</td>
                            <td>{{v[0].LineAllAbnomalCount}}</td>
                            <td>{{v[0].AllNormalCount}}</td>
                            <td>{{v[0].AllAbnormalCount}}</td>
                            <td>{{v[0].OrderFinishRate==""?0:v[0].OrderFinishRate|zerocut}}%</td>
                            <td>{{v[0].OrderPassRate==""?0:v[0].OrderPassRate|zerocut}}%</td>
                            <td>{{v[0].BeginTime}}</td>
                            <td>{{v[0].EndTime}}</td>
                            <td style="word-break:break-all;word-wrap:break-word">{{v[0].TotalTime.split(".")[0]}}<br />{{v[0].TotalTime.split(".")[1]}}</td>
                        </tr>
                        <tr v-else>
                            <td>{{v[0].LineNum}}</td>
                            <td>{{v[0].Models}}</td>
                            <td>{{v[0].JobContent}}</td>
                            <td>{{v[0].PlanQuantity}}</td>
                            <td>{{v[0].Capacity|zerocut}}</td>

                            <td>{{v[0].NormalCount}}</td>
                            <td>{{v[0].AbnormalCount}}</td>
                            <td>{{v[0].TodayFinishRate==""?0:v[0].TodayFinishRate|zerocut}}%</td>
                            <td>{{v[0].TodayPassRate==""?0:v[0].TodayPassRate|zerocut}}%</td>
                            <td>{{v[0].LineAllNomalCount}}</td>
                            <td>{{v[0].LineAllAbnomalCount}}</td>
                            <td>{{v[0].AllNormalCount}}</td>
                            <td>{{v[0].AllAbnormalCount}}</td>
                            <td>{{v[0].OrderFinishRate==""?0:v[0].OrderFinishRate|zerocut}}%</td>
                            <td>{{v[0].OrderPassRate==""?0:v[0].OrderPassRate|zerocut}}%</td>
                            <td>{{v[0].BeginTime}}</td>
                            <td>{{v[0].EndTime}}</td>
                            <td style="word-break:break-all;word-wrap:break-word">{{v[0].TotalTime.split(".")[0]}}<br />{{v[0].TotalTime.split(".")[1]}}</td>
                        </tr>
                    </template>
                </template>
            </tbody>
        </table>
    </div>
    <div class="row">
        <!-- 小样图片 -->
        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog" style="width: 100%;height: auto;max-width: 900px;margin: 10px auto;">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            &times;
                        </button>
                        <h4 class="modal-title text-center" id="myModalLabel">
                            <span id="titlelabel">#</span>&nbsp;
                            <span class="text-center" style="font-size:12px;color:#777;position:absolute;top:15px">点击图片可进行旋转</span>
                        </h4>
                    </div>
                    <div id="contentModal" class="modal-body text-center">
                        <iframe id="pdfIframe" name="pdfIframe" src="" style="width:100%;"></iframe>
                    </div>
                </div>
            </div>
        </div>
        <button id="showModal" class="form-control" style="display:inline;width:60px;display:none" data-toggle="modal" data-target="#myModal">选择</button>
    </div>
</div>

<div id="setPdf"></div>
<div id="setForm"></div>
<script>
    var isHoverNum = "true";
    var firstclick = true;
    var app = new Vue({
        el: "#app",
        data: {
            smtLineData_new: [],
        },
        computed: {
            sortmarqueeList() {
                var initialList = this.smtLineData_new;
                var lastVal = filterList(initialList);
                return lastVal;
            },
        },
        mounted: function () {
            if (isMobile()) {
                $(document).mousedown(function (e) {
                    if ($(e.target).parent(".showDiv").length == 0) {
                        cancelHover();
                    };
                });
            };
        },
        methods: {
            orderDetail: function (val, station) {
                this.$options.methods.postForm("/OrderMgms/Details/" + station, val);
            },
            postForm: function (url, params) {
                let newForm = $("<form action=" + url + " target='_blank' method='post' style='display:none' ><input name='OrderNum' value=" + params + " ></form>");
                $("#setForm").append(newForm);
                newForm.submit();
                newForm.remove();
            },
            clickBtn: function (event) {
                event.preventDefault();
                event.stopPropagation();
                $(event.target).children(".showDiv").css({ "visibility": "visible", "opacity": "1" })
            },
            //小样进度图片
            imgModal: function (OrderNumVal) {
                if (firstclick == true && isMobile()) {
                    firstclick = false;
                    return;
                };
                $(".imgRotateDiv").remove();
                $("#pdfIframe").hide();
                $.ajax({
                    url: "/OrderMgms/GetSmallSampleImg",
                    type: "post",
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (returnVal) {
                        if (returnVal == "图片文档未上传或不存在！") {
                            alert(returnVal)
                        } else {
                            var json = JSON.parse(returnVal);
                            for (var img in json) {
                                $("#contentModal").append('<div class="imgRotateDiv" >' +
                                    '<img class="imgModalc artZoom" src="/MES_Data/SmallSample_Files/' + OrderNumVal + '/' + json[img] + '" style="width:100%;" alt="该小样订单照片尚未上传！" />' +
                                    '</div>');
                            }
                            $('.artZoom').artZoom({
                                path: '/SmallSample_Files/',	// 设置artZoom图片文件夹路径
                                preload: true,		// 设置是否提前缓存视野内的大图片
                                blur: true,			// 设置加载大图是否有模糊变清晰的效果

                                // 语言设置
                                left: '左旋转',		// 左旋转按钮文字
                                right: '右旋转',		// 右旋转按钮文字
                                source: '看原图'		// 查看原图按钮文字
                            });
                            $("#titlelabel").text("小样订单");
                            $("#showModal").click();
                        }
                    },
                    error: function (err) {
                        alert("小样订单图片尝未上传！", err);
                    }
                });
                //this.imgSrc = "/OrderMgms/GetImg?ordernum=" + ordernum;
            },
            //小样进度PDF
            pdfModal: function (OrderNumVal) {
                if (firstclick == true && isMobile()) {
                    firstclick = false;
                    return;
                };
                $(".imgRotateDiv").remove();
                $("#pdfIframe").show();
                $.ajax({
                    url: "/OrderMgms/preview_SmallSample_pdf",
                    type: "post",
                    cache: false,
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (address) {
                        if (address == "pdf文档未上传或不存在！") {
                            alert(address)
                        } else {
                            //let newForm = $("<form action='/Commonality/PDFRedirectToAction/'  method='get' style='display:none' >" +
                            //    "<input name='address' value=" + address + " >" +
                            //    "</form>");
                            //$("#setForm").append(newForm);
                            //newForm.submit();
                            //newForm.remove();

                            if (isMobile()) {
                                window.location.href = '..' + address + '?' + Math.random();
                            } else {
                                $("#setPdf").append('<a id="temporary" href="../../Scripts/pdf.js/web/viewer.html?file=' + address + '?' + Math.random() + '" target="pdfIframe" style="display:none"></a>');
                                document.getElementById("temporary").click();
                                $("#temporary").remove();
                                $("#titlelabel").text("小样订单");
                                if (isMobile()) {
                                    $("#pdfIframe").css({ "height": $(window).height() })//.attr("src", "");
                                } else {
                                    $("#pdfIframe").css({ "height": $(window).height() - 93 })//.attr("src", "");
                                }
                                $("#showModal").click();
                            };
                        };
                    },
                    error: function (err) {
                        alert("查看PDF失败", err);
                    }
                });
            },
            //小样异常图片
            imgycModal: function (OrderNumVal) {
                if (firstclick == true && isMobile()) {
                    firstclick = false;
                    return;
                };
                $(".imgRotateDiv").remove();
                $("#pdfIframe").hide();
                $.ajax({
                    url: "/OrderMgms/GetSMTAbnormalOrderImg",
                    type: "post",
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (returnVal) {
                        if (returnVal == "此SMT异常单订单图片尚未上传！") {
                            alert(returnVal)
                        } else {
                            var json = JSON.parse(returnVal);
                            for (var img in json) {
                                $("#contentModal").append('<div class="imgRotateDiv" >' +
                                    '<img class="imgModalc artZoom" src="/MES_Data/SMTAbnormalOrder_Files/' + OrderNumVal + '/' + json[img] + '" style="width:100%;" alt="该小样异常照片尚未上传！" />' +
                                    '</div>');
                            }
                            $('.artZoom').artZoom({
                                path: '/SMTAbnormalOrder_Files/',	// 设置artZoom图片文件夹路径
                                preload: true,		// 设置是否提前缓存视野内的大图片
                                blur: true,			// 设置加载大图是否有模糊变清晰的效果

                                // 语言设置
                                left: '左旋转',		// 左旋转按钮文字
                                right: '右旋转',		// 右旋转按钮文字
                                source: '看原图'		// 查看原图按钮文字
                            });
                            $("#titlelabel").text("SMT异常订单");
                            $("#showModal").click();
                        }
                    },
                    error: function (err) {
                        alert("SMT异常订单图片尝未上传！", err);
                    }
                });
                //this.imgSrc = "/OrderMgms/GetImg?ordernum=" + ordernum;
            },
            //小样异常PDF
            pdfycModal: function (OrderNumVal) {
                if (firstclick == true && isMobile()) {
                    firstclick = false;
                    return;
                };
                $(".imgRotateDiv").remove();
                $("#pdfIframe").show();
                $.ajax({
                    url: "/OrderMgms/GetSMTAbnormalOrderPDF",
                    type: "post",
                    cache: false,
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (address) {
                        if (address == "此SMT异常单pdf版文件尚未上传，无pdf文件可下载！") {
                            alert(address)
                        } else {
                            //let newForm = $("<form action='/Commonality/PDFRedirectToAction/'  method='get' style='display:none' >" +
                            //    "<input name='address' value=" + address + " >" +
                            //    "</form>");
                            //$("#setForm").append(newForm);
                            //newForm.submit();
                            //newForm.remove();

                            if (isMobile()) {
                                window.location.href = '..' + address + '?' + Math.random();
                            } else {
                                $("#setPdf").append('<a id="temporary" href="../../Scripts/pdf.js/web/viewer.html?file=' + address + '?' + Math.random() + '" target="pdfIframe" style="display:none"></a>');
