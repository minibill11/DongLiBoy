    //过滤订单和平台
    function filterList(array) {
        let thisArr, finalFilterArray = [];
        //去除空数组
        thisArr = array.filter(function (val) {
            if (val.length != 0) {
                return val;
            };
        });
        //根据选择框条件过滤
        thisArr.filter(function (val) {
            let o = false, p = false, j = false;
            if (vmSearch.ordernumVal != null && vmSearch.ordernumVal != "") {
                for (let i in vmSearch.ordernumVal) {
                    if (val[0][0].OrderNum == vmSearch.ordernumVal[i]) {
                        o = true;
                    };
                };
            } else {
                o = true;
            };
            if (vmSearch.platformVal != null && vmSearch.platformVal != "") {
                for (let i in vmSearch.platformVal) {
                    if (val[0][0].PlatformType == vmSearch.platformVal[i]) {
                        p = true;
                    };
                };
            } else {
                p = true;
            };
            if (vmSearch.jobContentVal != null && vmSearch.jobContentVal != "") {
                val = val.filter(function (v) {
                    for (let i in vmSearch.jobContentVal) {
                        if (v[0].JobContent == vmSearch.jobContentVal[i]) {
                            return v;
                        };
                    };
                });
                if (val.length != 0) {
                    j = true;
                };
                //if (val[0][0].JobContent  == vmSearch.jobContentVal[i]) {
                //    j = true;
                //};
            } else {
                j = true;
            };
            if (o == true && p == true && j == true) {
                finalFilterArray.push(val);
            };
        });
        return finalFilterArray;
    };

    Vue.filter('zerocut', function (value) {
        let _thisnum = value.toString().split(".");
        if (_thisnum.length == 1) {
            return value;
        } else if (_thisnum[1] == "00") {
            return _thisnum[0];
        } else if (_thisnum[1].split("")[1] == "0") {
            return _thisnum[0] + "." + _thisnum[1].split("")[0];
        } else {
            return value;
        }
    })

    Date.prototype.Format = function (fmt) {
        var o = {
            "M+": this.getMonth() + 1,
            "d+": this.getDate(),
            "h+": this.getHours(),
            "m+": this.getMinutes(),
            "s+": this.getSeconds(),
            "q+": Math.floor((this.getMonth() + 3) / 3),
            "S": this.getMilliseconds()
        };
        if (/(y+)/.test(fmt))
            fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
            if (new RegExp("(" + k + ")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    };

    function isMobile() {
        var judge = (navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|iPad|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i));
        return judge;
    };
    function cancelHover() {
        $(".showDiv").each(function () {
            $(this).css({ "visibility": "hidden", "opacity": "0" })
        });
    };
