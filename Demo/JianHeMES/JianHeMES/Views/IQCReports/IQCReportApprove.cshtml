                    </td>
                    <td style="border:none">
                        @Html.EditorFor(model => model.Assessor, new { @class = "form-control", @readonly = "readonly" })
                        @Html.DisplayFor(model => model.Assessor, new { @class = "form-control", @readonly = "readonly" })
                    </td>
                    <td style="text-align:center;border-left: none;border-right: none">检验：</td>
                    <td style="border-left: none" class="form-group">
                        @Html.EditorFor(model => model.Inspector, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        @Html.ValidationMessageFor(model => model.Inspector, "", new { @class = "text-danger" })
                    </td>
                </tr>

                @Html.EditorFor(model => model.Creator, new { @class = "form-control" })
                @Html.EditorFor(model => model.CreatedDate, new { @class = "form-control" })
                @Html.EditorFor(model => model.AssessedDate, new { @class = "form-control", @readonly = "readonly" })

                <tr>
                    <td>审核评语</td>
                    <td colspan="7" style="border-left:" class="form-group">
                        @Html.TextAreaFor(model => model.AssessorRemark, new { @class = "form-control", @readonly = "readonly" })
                        @Html.ValidationMessageFor(model => model.AssessorRemark, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr class="saveShow">
                    <td>批准评语</td>
                    <td colspan="7" style="border-left: none;" class="form-group">
                        @Html.TextAreaFor(model => model.ApproveRemark, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.ApproveRemark, "", new { @class = "text-danger" })
                    </td>
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <td style="border:none">
                        @*<div class="form-group">
                        <input type="submit" value="新建" class="btn btn-default" />
                    </div>*@
                    </td>
                    <td colspan="7" style="text-align: right;border: none">表单编号：QD-007-B</td>
                </tr>

                <tr>
                    <td colspan="8" style="border: none;text-align:left">
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
