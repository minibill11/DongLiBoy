    }

    .Dfive input {
        width: 20%;
        float: left;
    }

    .Dthree input {
        width: 33.3%;
        float: left;
    }

    label {
        font-weight: normal;
    }

    #P0, #R0, #AM0 {
        padding: 0;
    }

    #RoHS_REACH, #SamplingPlan, #NGD, #NGHandle, #Creator, #CreatedDate, #AssessedDate ,#ApprovePass,#AssessorPass ,#Assessor,#SRJson,#SGJson,#SBJson,#EquipmentNum{
        display: none;
    }

    textarea {
        border: none;
        width: 100%;
        max-width: initial;
        resize: none;
    }

        textarea.form-control {
            line-height: 14px;
            font-size: 14px;
            padding: 3px 6px;
        }
     .sr input,.sg input,.sb input {
        width: 10%;
        float: left;
    }
    #EquipmentNumShow{
        text-align:left;
        padding:0;
        line-height:13px;
        font-size:13px;
        color:#555;
    }
    .saveShow{
