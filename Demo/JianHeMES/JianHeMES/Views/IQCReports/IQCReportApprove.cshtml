                        @Html.EditorFor(model => model.AssessorPass)
                        <select class="list-box tri-state" id="AssessorPassSelect" disabled="disabled">
                            <option value="">未审核</option>
                            <option value="true">通过</option>
                            <option value="false">未通过</option>
                        </select>
