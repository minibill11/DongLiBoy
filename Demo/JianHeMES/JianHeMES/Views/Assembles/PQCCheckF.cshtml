@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
   <script>
        var app = new Vue({
            el: "#app",
            data: {
                abnormalarea: [],
                selectrepair: "",
                abnormalshow: false,
                lineid:localStorage.getItem('LineId')
            },
            watch: {
                selectrepair: function (val) {
                    if (val == "") {
                        $("#AbnormalShow").hide();
                        $("#AbnormalTable").hide();
                        $("#OrderDetails").show();
                        this.abnormalarea = [];
                    } else if (val == "正常") {
                        $("#AbnormalShow").hide();
                        $("#AbnormalTable").hide();
                        $("#OrderDetails").show();
                        this.abnormalarea = ["正常"];
                    } else {
                        $("#OrderDetails").hide();
                        $("#AbnormalShow").show();
                        $("#AbnormalTable").show();
                        this.abnormalarea = [];
                    }
                },
                abnormalarea: function (val) {
                    $('#PQCCheckAbnormal').attr('value', val);
                    console.log(val);
                    console.log(JSON.stringify(val));
                },
                lineid: function (val) {
                    localStorage.setItem('LineId', val)
                }
            }
        });
        $("select").change(function () {
            app.selectrepair = $("select option:checked").val();
        });
        $("#AssembleLineId").focus();
</script>
    
}
