
@{
    ViewBag.Title = "b_a_list";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>b_a_list</h2>

@using (Html.BeginForm("b_a_list", "Test", FormMethod.Post, new { id = "searchForm", @class = "text-center" }))
{
    @Html.AntiForgeryToken()
    <span>订单号：<input type="text" name="ordernum" v-model="SearchDate" /></span>
    <input type="submit" id="searchButton" class="hidden" value="查找" />
}


@foreach (var item in @ViewBag.resultlist)
{
    <li>
        @item
    </li>
}

