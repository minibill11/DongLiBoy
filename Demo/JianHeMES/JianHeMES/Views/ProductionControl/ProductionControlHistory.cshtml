                </td>

                @*FQC*@
                <td v-on:click="Detail(item.OrderNum,'FinalQC')"
                    v-bind:class="(item.FinalQC_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                    {{item.FinalQC_Finish_Rate}}
                    <span v-show="item.FinalQC_Record_Count"><br />({{item.FinalQC_Finish}}/{{item.Quantity}})</span>
                </td>
                <td v-on:click="Detail(item.OrderNum,'FinalQC')"
                    v-bind:class="(item.FinalQC_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.FinalQC_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.FinalQC_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                    {{item.FinalQC_Pass_Rate}}
                    <span v-show="item.FinalQC_Record_Count"><br />({{item.FinalQC_Finish}}/{{item.FinalQC_Record_Count}})</span>
                </td>

                @*老化*@
                @*<td v-on:click="Detail(item.OrderNum,'Burn_in')" >{{item.Burn_in_Doing_Count}}</td>*@
                @*组装完成且24h未进入老化*@
                <td v-on:click="Detail(item.OrderNum,'Burn_in')"
                    v-bind:class="(item.Burn_in_Finish_Rate.split('%')[0])>=100?'bgcolor100':''" v-bind:style="(item.Overtime_never_join_burn_in=='')?'':'border:3px solid red;'">
                    {{item.Burn_in_Finish_Rate}}
                    <span v-show="item.Burn_in_Count"><br />({{item.Burn_in_Record_Count}}/{{item.Quantity}})</span>
                </td>
                <td v-on:click="Detail(item.OrderNum,'Burn_in')"
                    v-bind:class="(item.Burn_in_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Burn_in_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Burn_in_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                    {{item.Burn_in_Pass_Rate}}
                    <span v-show="item.Burn_in_Count"><br />({{item.Burn_in_Finish}}/{{item.Burn_in_Record_Count}})</span>
                </td>
                @*校正*@
                <td v-on:click="Detail(item.OrderNum,'Calibration')"
                    v-bind:class="(item.Calibration_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                    {{item.Calibration_Finish_Rate}}
                    <span v-show="item.Calibration_Count"><br />({{item.Calibration_Finish}}/{{item.Quantity}})</span>
                </td>
                <td v-on:click="Detail(item.OrderNum,'Calibration')"
                    v-bind:class="(item.Calibration_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Calibration_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Calibration_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
