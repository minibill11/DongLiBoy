                    v-bind:class="(item.Appearances_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Appearances_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Appearances_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                    {{item.Appearances_Pass_Rate}}
                    <span v-show="item.Appearances_Count"><br />({{item.Appearances_Finish}}/{{item.Appearances_Record_Count}})</span>
                </td>
                <td>{{item.ActualFinishTime}}</td>
                <td style="white-space:nowrap;" v-if="item.TotalTime.length==4">{{item.TotalTime[0]+"天"+item.TotalTime[1]+"时"}}<br />{{item.TotalTime[2]+"分"+item.TotalTime[3]+"秒"}}</td>
                <td style="white-space:nowrap;" v-else-if="item.TotalTime.length==3">{{item.TotalTime[0]+"时"}}<br />{{item.TotalTime[1]+"分"+item.TotalTime[2]+"秒"}}</td>
