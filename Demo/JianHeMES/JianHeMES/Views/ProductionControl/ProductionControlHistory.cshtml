<div id="setForm"></div>

<script>
    var app = new Vue({
        el: "#app",
        data: {
            viewbagval: $("#viewbagValue").val(),
            marqueeList: [],
            //imgSrc: ""
        },
        computed: {
            sortmarqueeList() {
                return sortByActualFinishTime(sortByActualProductionTime(this.marqueeList, "ActualProductionTime"), "ActualFinishTime")
            }
        },
        created: function () {
            viewbagJson = JSON.parse(this.viewbagval);
            console.log(viewbagJson)
            for (index in viewbagJson) {
                var totalTimeList = [];
                if (viewbagJson[index].TotalTime == "") {
                    var totalTime = "";
                } else {
                    var totalTime = viewbagJson[index].TotalTime.split(".");
                };
                if (totalTime.length == 3) {
                    var totalTimenday = totalTime[1].split(":");
                    totalTimeList.push(totalTime[0]);
                    for (item in totalTimenday) {
                        totalTimeList.push(totalTimenday[item]);
                    };
                } else if (totalTime.length == 2) {
                    var totalTimenday = totalTime[0].split(":");
                    var totalTimenday1 = totalTime[1].split(":");
                    if (totalTimenday.length == 3) {
                        for (item in totalTimenday) {
                            totalTimeList.push(totalTimenday[item]);
                        };
                    } else if (totalTimenday1.length == 3) {
                        for (item in totalTimenday1) {
                            totalTimeList.push(totalTimenday1[item]);
                        };
                    }
                } else {
                    totalTimeList.push(totalTime);
                };
                this.marqueeList.push({
                    Id: viewbagJson[index].Id,
                    OrderNum: viewbagJson[index].OrderNum,
                    Quantity: viewbagJson[index].Quantity,
                    PlatformType: viewbagJson[index].PlatformType,
                    PlanInputTime: viewbagJson[index].PlanInputTime,
                    PlanCompleteTime: viewbagJson[index].PlanCompleteTime,
                    ActualFinishTime: viewbagJson[index].ActualFinishTime,
                    TotalTime: totalTimeList,
                    ActualProductionTime: viewbagJson[index].ActualProductionTime,
                    Assemble_Finish_Rate: viewbagJson[index].Assemble_Finish_Rate,
                    Assemble_Pass_Rate: viewbagJson[index].Assemble_Pass_Rate,
                    Burn_in_Finish_Rate: viewbagJson[index].Burn_in_Finish_Rate,
                    Burn_in_Pass_Rate: viewbagJson[index].Burn_in_Pass_Rate,
                    Calibration_Finish_Rate: viewbagJson[index].Calibration_Finish_Rate,
                    Calibration_Pass_Rate: viewbagJson[index].Calibration_Pass_Rate,
                    Appearances_Finish_Rate: viewbagJson[index].Appearances_Finish_Rate,
                    Appearances_Pass_Rate: viewbagJson[index].Appearances_Pass_Rate,
                    Assemble_Finish: viewbagJson[index].Assemble_Finish,
                    AssembleRecord_Count: viewbagJson[index].AssembleRecord_Count,
                    //FQC
                    FinalQC_Finish_Rate: viewbagJson[index].FinalQC_Finish_Rate,
                    FinalQC_Pass_Rate: viewbagJson[index].FinalQC_Pass_Rate,
                    FinalQC_Finish: viewbagJson[index].FinalQC_Finish,
                    FinalQC_Record_Count: viewbagJson[index].FinalQC_Record_Count,
                    FinalQC_Spot_Count: viewbagJson[index].FinalQC_Spot_Count,

                    Burn_in_Finish: viewbagJson[index].Burn_in_Finish,
                    Burn_in_Count: viewbagJson[index].Burn_in_Count,
                    Burn_in_Record_Count: viewbagJson[index].Burn_in_Record_Count,
                    Calibration_Finish: viewbagJson[index].Calibration_Finish,
                    Calibration_Count: viewbagJson[index].Calibration_Count,
                    Calibration_Record_Count: viewbagJson[index].Calibration_Record_Count,
                    Appearances_Finish: viewbagJson[index].Appearances_Finish,
                    Appearances_Count: viewbagJson[index].Appearances_Count,
                    Appearances_Record_Count: viewbagJson[index].Appearances_Record_Count,
                    AOD_Description: viewbagJson[index].AOD_Description,

                    Overtime_never_join_burn_in: viewbagJson[index].Overtime_never_join_burn_in,
                    Overtime_never_join_appearances: viewbagJson[index].Overtime_never_join_appearances,
                });
            }
        },
        methods: {
            Detail: function (val, station) {
                this.$options.methods.postForm("/ProductionControl/" + station, val);
            },
            orderDetail: function (val, station) {
                this.$options.methods.postForm("/OrderMgms/Details/" + station, val);
            },
            postForm: function (url, params) {
                var newForm = $("<form action=" + url + " target='_blank' method='post' style='display:none' ><input name='OrderNum' value=" + params + " ></form>");
                $("#setForm").append(newForm);
                newForm.submit();
                newForm.remove();
            },
            imgModal: function (OrderNumVal) {
                $("#imgModal").children().remove();
                $.ajax({
                    url: "/OrderMgms/GetImg",
                    type: "post",
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (returnVal) {
                        var json = JSON.parse(returnVal);
                        for (var img in json) {
                            $("#imgModal").append('<img src="/AOD_Files/' + OrderNumVal + '/' + json[img] + '" style="width:100%;" /><br /><br />')
                        }
                        $("#showModal").click();
                    },
                    error: function (err) {
                        alert("查看特采订单失败", err);
                    }
                })
                //this.imgSrc = "/OrderMgms/GetImg?ordernum=" + ordernum;
            }
        },
        mounted: function () {
            this.$nextTick(function () {
                $("#table_head th").each(function (index, value) {
                    var tempWidth = $(value)[0].getBoundingClientRect().width;
                    var tempHeight = $(value)[0].getBoundingClientRect().height;
                    $("#fixed_table th").eq(index).css({ 'width': tempWidth + "px", 'height': tempHeight + "px" });
                });
            });
        }
    });

    function sortByActualProductionTime(array, key) {
        return array.sort(function (a, b) {
            var x = a[key];
            var y = b[key];
            return ((x < y) ? -1 : ((x > y) ? 1 : 0))
        })
    }
    function sortByActualFinishTime(array, key) {
        return array.sort(function (a, b) {
            var x = a[key];
            var y = b[key];
            return ((x > y) ? -1 : ((x < y) ? 1 : 0))
        })
    };

    $(function () {
        isQuery();
        $("#orderNum").change(function () {
            isQuery();
        });
        $("#PlatformType").change(function () {
            isQuery();
        });
    });

    function isQuery() {
        if ($("#orderNum").val() == "" && $("#PlatformType").val() == "") {
            $("#queryBtn").attr("disabled", "disabled")
        } else {
            $("#queryBtn").attr("disabled", false)
        }
    };

</script>
<script>
    $(function () {
        var sourceTable = $("#top_fix_table");//table id
        var sourceTableHeight = sourceTable.height();
        var sourceTableHead = $("#table_head");//table thead tr id
        var headHeight = sourceTableHead.height();//table thead tr height
        var sourceTableWidth = sourceTable.outerWidth(); //get source table width
        var testDiv = $("#test_width"); //測試獲取內容寬度
        var wrapDiv = $("#app"); //滾動條外層div，用於獲取div距離頭部的距離
        var tempTop = wrapDiv.offset().top - $(window).scrollTop(); //滾動頭部距離網頁頂部的距離
        var tempLeft = wrapDiv.offset().left;
        //copy table and thead html tag from source table,
        //$('body').append('<div id="shelter"><div id="fixed_table_wrap"><table id="fixed_table"  border="0" cellpadding="4" cellspacing="0" class="table table-hover"><thead></thead></table></div></div>');
        var fixTopDiv = $("#shelter");
        fixTopDiv.hide();
        //only set top and left,beacuse i need the top bar can scroll left
        fixTopDiv.css({
            'height': headHeight,
            'position': 'fixed',
            'top': tempTop + "px",
            'left': tempLeft + 'px',
            'width': testDiv.width() + "px",
            'overflow': 'hidden'
        });
        $("#fixed_table_wrap,#fixed_table").css({ 'width': sourceTableWidth + "px" });
        //only clone tr html and change thead tr id attr
        //var targetHead = sourceTableHead.clone().attr('id', 'fix_head');
        //targetHead.appendTo('#fixed_table thead');
        //mark target table thead td width,height equal source table thead td width,height
        //$("#table_head th").each(function (index, value) {
        //    //var tempWidth = $(value).outerWidth();//不能小数
        //    //var tempHeight = $(value).outerHeight();
        //    var tempWidth = $(value)[0].getBoundingClientRect().width;
        //    var tempHeight = $(value)[0].getBoundingClientRect().height;
        //    $("#fixed_table th").eq(index).css({ 'width': tempWidth + "px", 'height': tempHeight + "px" });
        //});
        //Div中內容滾動
        wrapDiv.scroll(function () {
            //scroll left method
            var sl = -Math.max(wrapDiv.scrollLeft(), $('document').scrollLeft());
            if (isNaN(sl)) {
                sl = -wrapDiv.scrollLeft();
            }
            fixTopDiv.css("left", sl + wrapDiv.offset().left + 'px');
            //var scroll_top = wrapDiv.scrollTop() - headHeight + $(window).scrollTop();
            var scroll_top = wrapDiv.scrollTop() + $(window).scrollTop();
            tempTop = wrapDiv.offset().top - $(window).scrollTop();
            if (tempTop <= 0) {
                tempTop = 0;
            }
            var baseWidth = testDiv.width() + wrapDiv.scrollLeft();
            //control  show or hide
            if (scroll_top > 0) {
                fixTopDiv.css({ 'top': tempTop + "px", 'width': baseWidth + "px" });
                if (!fixTopDiv.is(':visible')) {
                    fixTopDiv.show();
                }
            } else {
                if (fixTopDiv.is(':visible')) {
                    fixTopDiv.hide();
                }
            }
        });
        //瀏覽器srcoll
        $(window).on('scroll', function () {
            tempTop = wrapDiv.offset().top - $(window).scrollTop();
            //var scroll_top = wrapDiv.scrollTop() - headHeight + $(window).scrollTop();
            var scroll_top = wrapDiv.scrollTop() + $(window).scrollTop();
            var baseWidth = testDiv.width() + wrapDiv.scrollLeft();
            if (tempTop <= 0) {
                tempTop = 0;
            }
            fixTopDiv.css({ 'top': tempTop + "px", 'width': baseWidth + "px" });
            //control show or hide
            if (scroll_top > 0) {
                if (!fixTopDiv.is(':visible')) {
                    fixTopDiv.show();
                }
            } else {
                if (fixTopDiv.is(':visible')) {
                    fixTopDiv.hide();
                }
            }
        });


        $(function () {
            $('.selectpicker').selectpicker();
            $('#searchButton').click(function () {
                submitForm($('#pagebar .currentpagenumber').text());
            })
        });
    });
</script>