        <tbody>
            <tr v-for="(item, index) in sortmarqueeList">
                <td>{{index+1}}</td>
                <td v-on:click="orderDetail(item.OrderNum,item.Id)">{{item.OrderNum}}</td>
                <td>{{item.Quantity}}个</td>
                <td>{{item.PlatformType}}</td>
                <td>{{item.PlanInputTime.split(" ")[0]}}</td>
                <td>{{item.PlanCompleteTime.split(" ")[0]}}</td>
                <td>{{item.ActualProductionTime}}</td>
                @*组装*@
                <td v-on:click="Detail(item.OrderNum,'Assemble')"
                    v-bind:class="(item.Assemble_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                    {{item.Assemble_Finish_Rate}}
                    <span v-show="item.AssembleRecord_Count"><br />({{item.Assemble_Finish}}/{{item.Quantity}})</span>
                </td>
                <td v-on:click="Detail(item.OrderNum,'Assemble')"
                    v-bind:class="(item.Assemble_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Assemble_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Assemble_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
