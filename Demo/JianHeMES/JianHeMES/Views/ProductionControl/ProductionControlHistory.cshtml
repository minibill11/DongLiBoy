                <td v-on:click="Detail(item.OrderNum,'Appearance')"
                    v-bind:class="(item.Appearances_Finish_Rate.split('%')[0])>=100?'bgcolor100':''" v-bind:style="(item.Overtime_never_join_appearances=='')?'':'border:3px solid red;'">
                    {{item.Appearances_Finish_Rate}}
                    <span v-show="item.Appearances_Count"><br />({{item.Appearances_Finish}}/{{item.Quantity}})</span>
                </td>
