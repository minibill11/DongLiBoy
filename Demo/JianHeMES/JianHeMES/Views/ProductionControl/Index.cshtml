@{
    ViewBag.Title = "生产管控信息";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/jquery-1.6.4.js"></script>
<script src="~/Scripts/imgJS/jquery.artZoom.js"></script>
<link href="~/Content/styleFile/jquery.artZoom.css" rel="stylesheet" />
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<style>
    .table > thead > tr > th, .table > tbody > tr > td {
        text-align: center;
        vertical-align: middle;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(-n+5), .table > thead:first-child > tr:first-child > th:nth-child(-n+5) {
        border-top: 1px solid red;
    }

    .table > colgroup + thead > tr:first-child > th:nth-child(n+6), .table > thead:first-child > tr:first-child > th:nth-child(n+6) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(-n+5), .rowTitle2 > th:nth-child(-n+3), .table > tbody > tr > td:nth-child(-n+6) {
        border: 1px solid red;
    }

    .rowTitle1 > th:nth-child(n+6), .rowTitle2 > th:nth-child(n+3), .table > tbody > tr > td:nth-child(n+7) {
        border: 1px solid blue;
    }

    .rowTitle1 > th:nth-child(5), .rowTitle2 > th:nth-child(2), .table > tbody > tr > td:nth-child(6) {
        border-right: 1px solid blue;
    }

    .table > tbody > tr > td:nth-child(n+8), .table > tbody > tr > td:nth-child(2) {
        cursor: pointer;
    }

    .table > tbody > tr > td:nth-child(n+18) {
        cursor: auto;
    }

    .table > tbody > tr:nth-child(n+1):hover {
        background-color: rgba(77, 72, 72, 0.15);
    }

    .table > tbody > tr > td, .table > thead > tr > th {
        padding: 2px 1px;
    }

    @@media screen and (min-width: 1200px) {
        .container {
            width: 1400px;
        }

        .titleul {
            width: 1200px;
        }
    }

    #shelter {
        box-shadow: 0px 3px 5px #888888;
        display: none;
        z-index: 999;
    }

    #fixed_table #fix_head {
        background: #FFFFFF;
    }

    #fixed_table {
        position: absolute;
        top: 0px;
        left: 0px;
        /*table-layout: fixed;*/
    }

    .scroll_table_content {
        width: 100%;
        height: 720px;
        overflow: auto;
    }

    #app {
        font-size: 13px;
    }

    .bgcolor100 {
        background-color: rgba(62, 215, 0, 0.4);
    }

    .bgcolor90 {
        background-color: rgba(233, 230, 0, 0.6);
    }

    .bgcolor80 {
        background-color: rgba(233, 32, 32, 0.8);
        color: white;
    }

    .modal-body,.modal-header {
        padding: 8px 10px 2px;
    }
    
    .showBtn {
        position: relative;
        cursor: pointer;
    }

    .showDiv {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0px;
        right: 0px;
        visibility: hidden;
        opacity: 0;
        transition: opacity 1s;
    }

    .btnSty {
        position: absolute;
        width: 50%;
        height: 100%;
        padding: 0;
    }

    .showBtn:hover .showDiv {
        visibility: visible;
        opacity: 1;
    }
    .imgModalc{
        margin-bottom:5px;
    }
</style>
<div id="ieShow" class="row" style="display:none;padding:200px 0">
    <h2 class="text-center">当前浏览器版本过低，建议升级浏览器</h2>
    <h4 class="text-center">建议使用Firefox浏览器，下载地址：<a href="http://www.firefox.com.cn/" target="_blank">Firefox官网</a></h4>
</div>
<div id="content" style="display:none">
    <div class="row text-center" style="padding:15px 20px 0;">
        <span class="text-center" style="font-size:24px;font-family: inherit;font-weight: 500;margin-left:5%">生产管控信息</span>
        <div class="row form-group" style="float:right;margin:0;width:150px">
            <span class="btn btn-default">
                @Html.ActionLink("历史记录", "ProductionControlHistory", new { id = "" }, new { target = "_blank" })
            </span><br />
            <span id="nowtime" style="width:140px"></span>
        </div>
    </div>

    <div id="app" class="form-group scroll_table_content">
        <div id="shelter">
            <div id="fixed_table_wrap">
                <table id="fixed_table" class="table table-bordered">
                    <thead id="fix_head">
                        <tr class="rowTitle1">
                            <th rowspan="2">序号</th>
                            <th rowspan="2">订单号</th>
                            <th rowspan="2">订单<br />数量</th>
                            <th rowspan="2">平台型号</th>
                            <th colspan="2">生产排期</th>
                            <th rowspan="2">实际开始生产时间</th>
                            <th colspan="2">组装工序</th>
                            <th colspan="2">FQC</th>
                            <th colspan="2">调试老化</th>
                            <th colspan="2">校正</th>
                            <th colspan="2">包装</th>
                            <th rowspan="2">完成时间</th>
                            <th rowspan="2">生产时长</th>
                            <th rowspan="2">特采描述</th>
                            <th rowspan="2">异常描述</th>
                        </tr>
                        <tr class="rowTitle2">
                            <th>起始日期</th>
                            <th>出货时间</th>
                            <th>完成率</th>
                            <th>合格率</th>
                            <th style="font-size:12px">抽检率</th>
                            <th style="font-size:12px">抽检<br />合格率</th>
                            @*<th>老化数</th>*@
                            <th>老化数</th>
                            <th>合格率</th>
                            <th>完成率</th>
                            <th>合格率</th>
                            <th>完成率</th>
                            <th>合格率</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
        <table id="top_fix_table" class="table table-bordered">
            <colgroup>
                <col width="35 ">
                <col width="70 ">
                <col width="45 ">
                <col width="50 ">
                <col width="60 ">
                <col width="60 ">
                <col width="60 ">
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">

                @*<col width="65 ">*@
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">
                <col width="65 ">
                <col width="70 ">
                <col width="70 ">
                <col width="70 ">
                <col width="70 ">
            </colgroup>
            <thead id="table_head">
                <tr class="rowTitle1">
                    <th rowspan="2">序号</th>
                    <th rowspan="2" style="min-width:60px">订单号</th>
                    <th rowspan="2">订单<br />数量</th>
                    <th rowspan="2">平台型号</th>
                    <th colspan="2">生产排期</th>
                    <th rowspan="2">实际开始生产时间</th>
                    <th colspan="2">组装工序</th>
                    <th colspan="2">FQC</th>
                    <th colspan="2">调试老化</th>
                    <th colspan="2">校正</th>
                    <th colspan="2">包装</th>
                    <th rowspan="2">完成时间</th>
                    <th rowspan="2">生产时长</th>
                    <th rowspan="2">特采描述</th>
                    <th rowspan="2">异常描述</th>
                </tr>
                <tr class="rowTitle2">
                    <th>起始日期</th>
                    <th>出货时间</th>
                    <th>完成率</th>
                    <th>合格率</th>
                    <th style="font-size:12px">抽检率</th>
                    <th style="font-size:12px">抽检<br />合格率</th>
                    @*<th>老化数</th>*@
                    <th>老化数</th>
                    <th>合格率</th>
                    <th>完成率</th>
                    <th>合格率</th>
                    <th>完成率</th>
                    <th>合格率</th>
                </tr>
            </thead>
            <tbody>
                <tr v-for="(item, index) in sortmarqueeList">
                    <td>{{index+1}}</td>
                    <td v-on:click="orderDetail(item.OrderNum,item.Id)">{{item.OrderNum}}</td>
                    <td>{{item.Quantity}}个</td>
                    <td>{{item.PlatformType}}</td>
                    <td>{{item.PlanInputTime.split(" ")[0]}}</td>
                    <td>{{item.PlanCompleteTime.split(" ")[0]}}</td>
                    <td>{{item.ActualProductionTime}}</td>
                    @*组装*@
                    <td v-on:click="Detail(item.OrderNum,'Assemble')"
                        v-bind:class="(item.Assemble_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                        {{item.Assemble_Finish_Rate}}
                        <span v-show="item.AssembleRecord_Count"><br />({{item.Assemble_Finish}}/{{item.Quantity}})</span>
                    </td>
                    <td v-on:click="Detail(item.OrderNum,'Assemble')"
                        v-bind:class="(item.Assemble_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Assemble_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Assemble_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                        {{item.Assemble_Pass_Rate}}
                        <span v-show="item.AssembleRecord_Count"><br />({{item.Assemble_Finish}}/{{item.AssembleRecord_Count}})</span>
                    </td>

                    @*FQC*@
                    <td v-on:click="Detail(item.OrderNum,'FinalQC')"
                        v-bind:class="(item.FinalQC_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                        {{item.FinalQC_Finish_Rate}}
                        <span v-show="item.FinalQC_Record_Count"><br />({{item.FinalQC_Finish}}/{{item.Quantity}})</span>
                    </td>
                    <td v-on:click="Detail(item.OrderNum,'FinalQC')"
                        v-bind:class="(item.FinalQC_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.FinalQC_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.FinalQC_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                        {{item.FinalQC_Pass_Rate}}
                        <span v-show="item.FinalQC_Record_Count"><br />({{item.FinalQC_Finish}}/{{item.FinalQC_Record_Count}})</span>
                    </td>

                    @*老化*@
                    @*<td v-on:click="Detail(item.OrderNum,'Burn_in')" >{{item.Burn_in_Doing_Count}}</td>*@
                    @*组装完成且24h未进入老化*@
                    <td v-on:click="Detail(item.OrderNum,'Burn_in')"
                        v-bind:class="(item.Burn_in_Finish_Rate.split('%')[0])>=100?'bgcolor100':''" v-bind:style="(item.Overtime_never_join_burn_in=='')?'':'border:3px solid red;'">
                        {{item.Burn_in_Finish_Rate}}
                        <span v-show="item.Burn_in_Count"><br />({{item.Burn_in_Record_Count}}/{{item.Quantity}})</span>
                    </td>
                    <td v-on:click="Detail(item.OrderNum,'Burn_in')"
                        v-bind:class="(item.Burn_in_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Burn_in_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Burn_in_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                        {{item.Burn_in_Pass_Rate}}
                        <span v-show="item.Burn_in_Count"><br />({{item.Burn_in_Finish}}/{{item.Burn_in_Record_Count}})</span>
                    </td>
                    @*校正*@
                    <td v-on:click="Detail(item.OrderNum,'Calibration')"
                        v-bind:class="(item.Calibration_Finish_Rate.split('%')[0])>=100?'bgcolor100':''">
                        {{item.Calibration_Finish_Rate}}
                        <span v-show="item.Calibration_Count"><br />({{item.Calibration_Finish}}/{{item.Quantity}})</span>
                    </td>
                    <td v-on:click="Detail(item.OrderNum,'Calibration')"
                        v-bind:class="(item.Calibration_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Calibration_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Calibration_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                        {{item.Calibration_Pass_Rate}}
                        <span v-show="item.Calibration_Count"><br />({{item.Calibration_Finish}}/{{item.Calibration_Record_Count}})</span>
                    </td>
                    @*包装*@
                    @*老化完成且72h未结束包装*@
                    <td v-on:click="Detail(item.OrderNum,'Appearance')"
                        v-bind:class="(item.Appearances_Finish_Rate.split('%')[0])>=100?'bgcolor100':''" v-bind:style="(item.Overtime_never_join_appearances=='')?'':'border:3px solid red;'">
                        {{item.Appearances_Finish_Rate}}
                        <span v-show="item.Appearances_Count"><br />({{item.Appearances_Finish}}/{{item.Quantity}})</span>
                    </td>
                    <td v-on:click="Detail(item.OrderNum,'Appearance')"
                        v-bind:class="(item.Appearances_Pass_Rate.split('%')[0])>=100?'bgcolor100':(item.Appearances_Pass_Rate.split('%')[0])<80?'bgcolor80':(item.Appearances_Pass_Rate.split('%')[0])<90?'bgcolor90':''">
                        {{item.Appearances_Pass_Rate}}
                        <span v-show="item.Appearances_Count"><br />({{item.Appearances_Finish}}/{{item.Appearances_Record_Count}})</span>
                    </td>
                    <td>{{item.ActualFinishTime}}</td>
                    <td style="white-space:nowrap;" v-if="item.TotalTime.length==4">{{item.TotalTime[0]+"天"+item.TotalTime[1]+"时"}}<br />{{item.TotalTime[2]+"分"+item.TotalTime[3]+"秒"}}</td>
                    <td style="white-space:nowrap;" v-else-if="item.TotalTime.length==3">{{item.TotalTime[0]+"时"}}<br />{{item.TotalTime[1]+"分"+item.TotalTime[2]+"秒"}}</td>
                    <td style="white-space:nowrap;" v-else></td>
                    @*<td v-bind:style="(item.AOD_Description)?'cursor:pointer':''" v-on:click="(item.AOD_Description)?imgModal(item.OrderNum):''"><span>{{item.AOD_Description}}</span></td>*@
                    <td v-bind:class="(item.AOD_Description)?'showBtn':''"
                        v-on:click="(item.AOD_Description)?clickBtn($event):''">
                        {{item.AOD_Description}}
                        <div v-if="(item.AOD_Description)" class="showDiv">
                            <button v-on:click.stop="imgModal_AOD(item.OrderNum)" class="btn btn-info btnSty" style="right: 50%;">IMG</button>
                            <button v-on:click.stop="pdfModal_AOD(item.OrderNum)" class="btn btn-warning btnSty" style="left: 50%;">PDF</button>
                        </div>
                    </td>
                    <td v-bind:class="(item.AbnormalOrder_Description)?'showBtn':''"
                        v-on:click="(item.AbnormalOrder_Description)?clickBtn($event):''">
                        {{item.AbnormalOrder_Description}}
                        <div v-if="(item.AbnormalOrder_Description)" class="showDiv">
                            <button v-on:click.stop="imgModal_Abnormal(item.OrderNum)" class="btn btn-info btnSty" style="right: 50%;">IMG</button>
                            <button v-on:click.stop="pdfModal_Abnormal(item.OrderNum)" class="btn btn-warning btnSty" style="left: 50%;">PDF</button>
                        </div>
                    </td>
                    @*<td v-bind:style="(item.AbnormalOrder_Description)?'cursor:pointer':''" v-on:click="(item.AbnormalOrder_Description)?imgModal(item.OrderNum):''"><span>{{item.AbnormalOrder_Description}}</span></td>*@
                </tr>
            </tbody>
        </table>

        <div id="test_width"></div>
        <div class="row">
            <!-- 模态框 -->
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog" style="width: 100%;height: auto;max-width: 900px;margin: 10px auto;">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                &times;
                            </button>
                            <h4 class="modal-title text-center" id="myModalLabel">
                                <span id="titlelabel">#</span>&nbsp;
                                <span class="text-center" style="font-size:12px;color:#777;position:absolute;top:15px">点击图片可进行旋转</span>
                            </h4>
                        </div>
                        <div id="contentModal" class="modal-body text-center">
                            <iframe id="pdfIframe" name="pdfIframe" src="" style="width:100%;"></iframe>
                        </div>
                    </div>
                </div>
            </div>
            <button id="showModal" class="form-control" style="display:inline;width:60px;display:none" data-toggle="modal" data-target="#myModal">选择</button>
        </div>
    </div>
    <div>
        <span style="display: inline-block; background: rgba(62, 215, 0, 0.4);width: 10px;height: 10px;"></span> 达到100% &nbsp;&nbsp;
        <span style="display: inline-block; background: rgba(233, 32, 32, 0.8);width: 10px;height: 10px;"></span> 合格率范围为<80% &nbsp;&nbsp;
    	<span style="display: inline-block; background: rgba(233, 230, 0, 0.6);width: 10px;height: 10px;"></span> 合格率范围为80%~90% &nbsp;&nbsp;
    	<span style="display: inline-block; border: 2px solid red;width: 10px;height: 10px;"></span> 老化数栏：组装完成后，超过24H未进入老化工序； 包装完成率栏：老化完成后，超过72H进入包装工序。
    </div>
    <div id="setPdf"></div>
    <div id="setForm"></div>
</div>
<script>
    $(function () {
        if (isIE()) {
            $("#ieShow").show();
        } else {
            $("#content").show()
        };
        function isIE() {
            if (!!window.ActiveXObject || "ActiveXObject" in window) {
                return true;
            }
            else {
                return false;
            }
        };
    });
</script>
<script>
    var isHoverNum = "true";
    var app = new Vue({
        el: "#app",
        data: {
            marqueeList: [],
            //imgSrc: "",
        },
        computed: {
            sortmarqueeList() {
                var initialList = this.marqueeList;
                var firstSort = sortByActualProductionTime(initialList, "ActualProductionTime");
                var secondSort = sortByActualFinishTime(firstSort, "ActualFinishTime");
                var thirdSort = sortByPlanInputTime(secondSort, "PlanInputTime");
                return secondSort;
            }
        },
        created: function () {
            setInterval(function () {
                $("#nowtime").text(new Date().Format("yyyy-MM-dd hh:mm:ss"));
            }, 1000);
        },
        methods: {
            Detail: function (val, station) {
                this.$options.methods.postForm("/ProductionControl/" + station, val);
            },
            orderDetail: function (val, station) {
                this.$options.methods.postForm("/OrderMgms/Details/" + station, val);
            },
            postForm: function (url, params) {
                var newForm = $("<form action=" + url + " target='_blank' method='post' style='display:none' ><input name='OrderNum' value=" + params + " ></form>");
                $("#setForm").append(newForm);
                newForm.submit();
                newForm.remove();
            },
            clickBtn: function (event) {
                $(event.target).children(".showDiv").css({ "visibility": "visible", "opacity": "1" })
            },
            imgModal_AOD: function (OrderNumVal) {
                $(".imgRotateDiv").remove();
                $("#pdfIframe").hide();
                $.ajax({
                    url: "/OrderMgms/GetImg",
                    type: "post",
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (returnVal) {
                        if (returnVal == "此特采单订单图片尚未上传！") {
                            alert(returnVal)
                        } else {
                            var json = JSON.parse(returnVal);
                            for (var img in json) {
                                $("#contentModal").append('<div class="imgRotateDiv" >' +
                                    '<img class="imgModalc artZoom" src="/AOD_Files/' + OrderNumVal + '/' + json[img] + '?' + Math.random() + '" style="width:100%;" alt="该特采订单照片尚未上传！" />' +
                                     '</div>');
                            };
                            $('.artZoom').artZoom({
                                path: '/AOD_Files/',	// 设置artZoom图片文件夹路径
                                preload: true,		// 设置是否提前缓存视野内的大图片
                                blur: true,			// 设置加载大图是否有模糊变清晰的效果

                                // 语言设置
                                left: '左旋转',		// 左旋转按钮文字
                                right: '右旋转',		// 右旋转按钮文字
                                source: '看原图'		// 查看原图按钮文字
                            });
                            $("#titlelabel").text("特采订单");
                            $("#showModal").click();
                        };
                    },
                    error: function (err) {
                        alert("特采订单图片尝未上传！", err);
                    }
                });
                //this.imgSrc = "/OrderMgms/GetImg?ordernum=" + ordernum;
            },
            imgModal_Abnormal: function (OrderNumVal) {
                $(".imgRotateDiv").remove();
                $("#pdfIframe").hide();
                $.ajax({
                    url: "/OrderMgms/GetAbnormalOrderImg",
                    type: "post",
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (returnVal) {
                        if (returnVal == "此异常单订单图片尚未上传！") {
                            alert(returnVal)
                        } else {
                            var json = JSON.parse(returnVal);
                            for (var img in json) {
                                $("#contentModal").append('<div class="imgRotateDiv" >' +
                                    '<img class="imgModalc artZoom" src="/AbnormalOrder_Files/' + OrderNumVal + '/' + json[img] + '?' + Math.random() + '" style="width:100%;" alt="该异常订单照片尚未上传！" />' +
                                    '</div>');
                            };
                            $('.artZoom').artZoom({
                                path: '/AbnormalOrder_Files/',	// 设置artZoom图片文件夹路径
                                preload: true,		// 设置是否提前缓存视野内的大图片
                                blur: true,			// 设置加载大图是否有模糊变清晰的效果

                                // 语言设置
                                left: '左旋转',		// 左旋转按钮文字
                                right: '右旋转',		// 右旋转按钮文字
                                source: '看原图'		// 查看原图按钮文字
                            });
                            $("#titlelabel").text("异常订单");
                            $("#showModal").click();
                        };
                    },
                    error: function (err) {
                        alert("异常订单图片尝未上传！", err);
                    }
                });
                //this.imgSrc = "/OrderMgms/GetImg?ordernum=" + ordernum;
            },
            pdfModal_AOD: function (OrderNumVal) {
                $(".imgRotateDiv").remove();
                $("#pdfIframe").show();
                $.ajax({
                    url: "/OrderMgms/GetPDF",
                    type: "post",
                    cache: false,
                    data: {
                        ordernum: OrderNumVal,
                    },
                    success: function (address) {
                        if (address == "此特采单pdf版文件尚未上传，无pdf文件可下载！") {
                            alert(address)
                        } else {
                            $("#setPdf").append('<a id="temporary" href="../../Scripts/pdf.js/web/viewer.html?file=\\AOD_Files\\' + OrderNumVal + '\\' + OrderNumVal + '_AOD.pdf?' + Math.random() + '" target="pdfIframe" style="display:none"></a>');
                            document.getElementById("temporary").click();
