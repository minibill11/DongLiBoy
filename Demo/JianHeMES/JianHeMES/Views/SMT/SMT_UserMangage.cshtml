@model IEnumerable<JianHeMES.Models.Users>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/vue.js"></script>
    <script src="~/Scripts/jquery-1.11.3.js"></script>
    <title>SMT用户管理</title>
</head>
<body>
    <div id="app" class="container body-content">
        <h4>SMT用户管理</h4>
        <p class="text-center btn btn-default">
            @Html.ActionLink("新建用户", "SMT_UserCreate")
        </p>
        <p class="text-center btn btn-default">
            @Html.ActionLink("返回", "SMT_Mangage")
        </p>

        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.UserNum)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.PassWord)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Creator)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserAuthorize)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Role)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Department)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LineNum)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.Deleter)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DeleteDate)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.Description)
                </th>
                <th></th>
            </tr>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.PassWord)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.CreateDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Creator)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserAuthorize)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Department)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LineNum)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.Deleter)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DeleteDate)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.ActionLink("修改", "SMT_UserEdit", new { id = item.ID }) |
                    @Html.ActionLink("详细", "SMT_UserDetails", new { id = item.ID })|
                    @Html.ActionLink("删除", "SMT_UserDelete", new { id = item.ID })
                </td>
            </tr>
                }

            }
        </table>


        <hr />
        <footer>
            <p align="center">© 2018 - 健和光电MES</p>
        </footer>
    </div>
    <script>
        var app = new Vue({
            el: "#app",
            data: {
            },
            created: function () {
            },
            methods: {
            }
        })
    </script>
</body>
</html>
