    }
    /*RIGHT*/
    .fqc-medle-content-right {
        height: 220px;
        width: 320px;
        float: right;
    }

    .highcharts-title > tspan {
        font-size: 16px;
    }

    tspan {
        font-size: 11px;
    }
    /*输出的表头部分*/
    .fqc-for-tableheader {
        height: 35px;
        margin-top: 5px;
        float: left;
        width: 500px;
    }

    .fqc-for-tableheader-inner {
        font-size: 18px;
    }

    strong {
        color: black;
    }

    .fqc-hidden-content {
        border-bottom: 1px solid #808080;
    }
    /*输出表格样式--无效样式？*/
    .table > tbody > tr > th {
        font-size: 12px;
    }

    .table > tbody > tr {
        height: 30px;
    }

    tablescroll {
        height: 300px;
