function addTab(n,t,i){$("#mainTab").tabs("exists",n)?($("#mainTab").tabs("select",n),$("#tab_menu-tabrefresh").trigger("click")):$("#mainTab").tabs("add",{title:n,content:createFrame(t),closable:!0,icon:i});$(".layout-button-left").trigger("click")}function createFrame(n){return'<iframe frameborder="0" src="'+n+'" scrolling="auto" style="width:100%; height:99%"><\/iframe>'}$(function(){$("#tab_menu-tabrefresh").click(function(){var n=$(".tabs-panels .panel").eq($(".tabs-selected").index()).find("iframe").attr("src");$(".tabs-panels .panel").eq($(".tabs-selected").index()).find("iframe").attr("src",n)});$("#tab_menu-openFrame").click(function(){var n=$(".tabs-panels .panel").eq($(".tabs-selected").index()).find("iframe").attr("src");window.open(n)});$("#tab_menu-tabclose").click(function(){var n=$(".tabs-selected .tabs-inner span").text();$("#mainTab").tabs("close",n);$(".tabs li").length==0&&$(".layout-button-right").trigger("click")});$("#tab_menu-tabcloseall").click(function(){$(".tabs-inner span").each(function(n,t){if($(this).parent().next().is(".tabs-close")){var i=$(t).text();$("#mainTab").tabs("close",i)}});$(".layout-button-right").trigger("click")});$("#tab_menu-tabcloseother").click(function(){var n=$(".tabs-selected .tabs-inner span").text();$(".tabs-inner span").each(function(t,i){if($(this).parent().next().is(".tabs-close")){var r=$(i).text();r!=n&&$("#mainTab").tabs("close",r)}})});$("#tab_menu-tabcloseright").click(function(){var n=$(".tabs-selected").nextAll();return n.length==0?($.messager.alert("提示","前面没有了!","warning"),!1):(n.each(function(n,t){if($("a.tabs-close",$(t)).length>0){var i=$("a:eq(0) span",$(t)).text();$("#mainTab").tabs("close",i)}}),!1)});$("#tab_menu-tabcloseleft").click(function(){var n=$(".tabs-selected").prevAll();return n.length==0?($.messager.alert("提示","后面没有了!","warning"),!1):(n.each(function(n,t){if($("a.tabs-close",$(t)).length>0){var i=$("a:eq(0) span",$(t)).text();$("#mainTab").tabs("close",i)}}),!1)})});$(function(){$(".tabs li").live("contextmenu",function(n){var t=$(this).text();return $("#mainTab").tabs("select",t),$("#tab_menu").menu("show",{left:n.pageX,top:n.pageY}),!1})});$(function(){$(".ui-skin-nav .li-skinitem span").click(function(){var n=$(this).attr("rel");$.messager.confirm("提示","切换皮肤将重新加载系统！",function(t){t&&$.post("../../Home/SetThemes",{value:n},function(){window.location.reload()},"json")})})})