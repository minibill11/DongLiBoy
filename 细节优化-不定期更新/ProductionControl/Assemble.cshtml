@model IEnumerable<JianHeMES.Models.Assemble>

@{
    ViewBag.Title = "Assemble";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .barcodeFrame {
        width: 250px;
        height: 100px;
        border: 1px solid #000000;
        overflow: auto;
    }
    .titlerow div{
        font-size:16px;
        font-weight:bold;
    }
</style>
<h2>@ViewBag.OrderNum 组装工序详细情况</h2>
@{
    <div>
        @ViewBag.abnormalList;<br />
        @ViewBag.finishedAnd2record;<br />
        @ViewBag.unfinishAndRecord;<br />
        @ViewBag.unbeginRecord;<br />
        @ViewBag.going;<br />
        @ViewBag.passed;<br />
        @ViewBag.JsonObj;<br />
    </div>
    var JsonObj = @ViewBag.JsonObj;
}
<hr />
<div class="row titlerow">
    <div class="col-md-3">
        订单号：@JsonObj.OrderNum
    </div>
    <div class="col-md-2">
        模组数：@JsonObj.modelGroupQuantity
    </div>
    <div class="col-md-2">
        已完成PQC数：@JsonObj.finished
    </div>
    <div class="col-md-2">
        总PQC次数：@JsonObj.assemblePQC_Count
    </div>
    <div class="col-md-2">
        异常次数：@JsonObj.abnormal_Count
    </div>
    <div class="col-md-1">
    </div>
</div>
<br />
<div class="row titlerow">
    <div class="col-md-3">
        异常工时：@JsonObj.abnormal_time
    </div>
    <div class="col-md-2">
        完成率：@JsonObj.finisthRate %
    </div>
    <div class="col-md-2">
        合格率：@JsonObj.passRate %
    </div>
    <div class="col-md-2">
        直通率：@JsonObj.firstPassYield_Rate
    </div>
    <div class="col-md-2">
        直通个数：@JsonObj.firstPassYieldCount
    </div>
    <div class="col-md-1">
    </div>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <div>
            正在进行PQC：（- 个）
            <div class="barcodeFrame"></div>
        </div>
        <div>
            未进行PQC：（- 个）
            <div class="barcodeFrame"></div>
        </div>
        <div>
            已完成PQC：（- 个）
            <div class="barcodeFrame"></div>
        </div>
    </div>
    <div class="col-md-4">
        <div>
            已完成的条码-经过2次及以上PQC：（- 个）
            <div class="barcodeFrame"></div>
        </div>
        <div>
            未通过的条码-经过1次及以上PQC：（- 个）
            <div class="barcodeFrame"></div>
        </div>
        <div>
            异常记录清单：（- 个）
            <div class="barcodeFrame"></div>
        </div>
    </div>
    <div class="col-md-4">
        <p>异常统计：</p>
        <ol>
            <li>箱体表面：掉漆<span>（ - 次 ）</span></li>
            <li>螺丝：漏打/刮花/不到位<span>（ - 次 ）</span></li>
            <li>颜色：暗灯/瞎灯<span>（ - 次 ）</span></li>
        </ol>
    </div>
</div>

